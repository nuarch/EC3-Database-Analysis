    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_InvoiceUpdate]\n\t@invoiceId INT,@statusId INT,@comments VARCHAR(500) = NULL,@correlationId AS VARCHAR(128) = NULL,@machineName AS VARCHAR(128) = NULL,@processName AS VARCHAR(128) = NULL,@domain AS VARCHAR(25) = NULL,@username AS VARCHAR(25) = NULL,@userId AS INT = NULL,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @invoiceId = NULLIF(@invoiceId, 0);\r\n\tSET @statusId = NULLIF(@statusId, 0);\r\n\tSET @comments = NULLIF(@comments, '');\r\n\r\n\tDECLARE @action VARCHAR(25), @severity VARCHAR(25), @message VARCHAR(255), @messageDetails VARCHAR(MAX), @startTime DATETIME, @elapsedTime INT, @sendEmail BIT, @oldStatusId INT;\r\n\tSET @startTime = GETDATE();\t\r\n\tSET @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()));\r\n\tSET @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA');\r\n\tSET @processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\tSET @action = 'UPDATE';\r\n\tSET @severity = 'INFO';\r\n\tSET @message = 'Updated invoice';\r\n\tSET @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @invoiceId AS PRVID, @statusId AS PRD, @comments AS CMT FOR XML PATH('INPUTS')));\r\n\tSET @sendEmail = 0;\r\n\tSET @userId = ISNULL(@userId, 0);\r\n\r\n\tDECLARE @tranName VARCHAR(128);\r\n\tSET @tranName = CONVERT(VARCHAR(128), NEWID());\r\n\r\n\tDECLARE @actionType VARCHAR(25);\r\n\tSET @actionType = CASE\r\n WHEN @statusId = 2 THEN 'SUBMITTED'\r\n WHEN @statusId = 3 THEN 'RETURNED'\r\n WHEN @statusId = 4 THEN 'APPROVED'\r\n WHEN @statusId = 5 THEN 'SENT to A&A'\r\n ELSE 'UPDATED'\r\n END;\r\n\r\n\tIF ISNULL(@comments, '') = ''\r\n SET @comments = CASE\r\n WHEN @actionType = 'SUBMITTED' THEN 'Invoice submitted for approval'\r\n WHEN @actionType = 'RETURNED' THEN 'Invoice returned for corrections'\r\n WHEN @actionType = 'APPROVED' THEN 'Invoice approved, sent for A&A review'\r\n ELSE 'Invoice updated'\r\n END;\r\n\t\r\n\tSELECT @oldStatusId = StatusID\r\n\tFROM invoice.Invoice\r\n\tWHERE InvoiceID=@invoiceId;\r\n\r\n\tBEGIN TRY \r\n BEGIN TRAN @tranName;\r\n\r\n -- Update invoice header\r\n UPDATE [Invoice].[Invoice]\r\n SET StatusID = @statusId, InvoiceDate\t= CASE WHEN @actionType = 'APPROVED' THEN GETDATE() ELSE InvoiceDate END, ModifiedDate\t= GETDATE(), ModifiedBy\t= @userId\r\n WHERE InvoiceID = @invoiceId;\r\n\r\n -- Create audit note\r\n INSERT INTO [Common].[Note] (ParentType, ParentID, [Action], Note, CreatedBy)\r\n VALUES ('INVOICE', @invoiceId, @actionType, @comments, @userId);\r\n\r\n COMMIT TRAN @tranName;\r\n\r\n -- Update close amount of contract and contract lines, added on 4/11/18\r\n IF (@statusId=3 AND @oldStatusId=4) OR @statusId=4\r\n BEGIN\r\n UPDATE [Billing].ContractLine\r\n SET\tCloseAmount = [Billing].[UDF_GetContractLineAmountPaid](a.ContractLineID, GETDATE()), OpenAmount = a.ActualAmount - [Billing].[UDF_GetContractLineAmountPaid](a.ContractLineID, GETDATE()), ModifiedDate = GETDATE(), ModifiedBy = @userId\r\n FROM Billing.ContractLine a\r\n WHERE a.ContractLineID IN \r\n (\r\n SELECT DISTINCT ContractLineID\r\n FROM invoice.InvoiceContractLine\r\n WHERE invoiceid = @invoiceId\r\n );\r\n\r\n UPDATE [Billing].Contract\r\n SET\tCloseAmount = ISNULL(b.CloseAmount, 0),OpenAmount = ISNULL(b.OpenAmount, 0),ModifiedDate = GETDATE(),ModifiedBy = @userId\r\n FROM [Billing].Contract a JOIN\r\n (\r\n SELECT ContractID, SUM(ISNULL(CloseAmount, 0)) AS CloseAmount, SUM(ISNULL(OpenAmount, 0)) AS OpenAmount\r\n FROM [Billing].ContractLine \r\n WHERE ContractID IN \r\n (\r\n SELECT DISTINCT contractid\r\n FROM invoice.InvoiceContractLine\r\n WHERE invoiceid = @invoiceId\r\n )\r\n GROUP BY ContractID\r\n ) b ON a.ContractID=b.contractid;\r\n \r\n END;\r\n\r\n -- insert payments into history table\r\n DECLARE @dte DATETIME\r\n \r\n IF @actionType = 'APPROVED'\r\n BEGIN\r\n SET @dte = getdate();\r\n INSERT INTO Billing.HistoricalContractLine\r\n (\r\n TransactionType,\r\n ContractLineID,\r\n ActualAmount,\r\n StartDate,\r\n EndDate,\r\n ModifiedBy,\r\n ChangeAmount,\r\n InvoiceId\r\n )\r\n --values(2,0,0,@dte,null,@username,0,@invoiceId)\r\n SELECT 2,\r\n c.ContractLineID,\r\n 0,\r\n @dte,\r\n NULL,\r\n @username,\r\n -SUM(c.AmountPaid),\r\n @invoiceId\r\n FROM Invoice.InvoiceGroupContract a\r\n JOIN Billing.BillingGroupContract b ON a.BillingGroupContractID = b.BillingGroupContractID\r\n JOIN Invoice.InvoiceContractLine c ON a.InvoiceID=c.InvoiceID AND b.ContractID = c.ContractID\r\n WHERE a.InvoiceID = @invoiceId\r\n GROUP BY c.ContractLineID;\r\n\r\n\r\n UPDATE Billing.HistoricalContractLine\r\n SET EndDate = GETDATE()\r\n WHERE StartDate < @dte AND invoiceId = @invoiceId AND EndDate IS NULL AND TransactionType = 2\r\n END\r\n\r\n IF (@statusId=3 AND @oldStatusId=4) --@actionType = 'RETURNED' [changed by Xin on 08/16/2019]\r\n BEGIN\r\n INSERT INTO Billing.HistoricalContractLine\r\n (\r\n TransactionType,\r\n ContractLineID,\r\n ActualAmount,\r\n StartDate,\r\n EndDate,\r\n ModifiedBy,\r\n ChangeAmount,\r\n InvoiceId\r\n )\r\n SELECT 3,\r\n c.ContractLineID,\r\n 0,\r\n GETDATE(),\r\n NULL,\r\n @username,\r\n SUM(c.AmountPaid),\r\n @invoiceId\r\n FROM Invoice.InvoiceGroupContract a\r\n JOIN Billing.BillingGroupContract b ON a.BillingGroupContractID = b.BillingGroupContractID\r\n JOIN Invoice.InvoiceContractLine c ON a.InvoiceID=c.InvoiceID AND b.ContractID = c.ContractID AND b.FiscalYear=c.FiscalYear\r\n WHERE a.InvoiceID = @invoiceId\r\n GROUP BY c.ContractLineID;\r\n END;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n IF XACT_STATE() <> 0 ROLLBACK TRAN @tranName;\r\n\r\n SELECT @exceptionDetails = ERROR_MESSAGE();\r\n SET @severity\t= 'ERROR';\r\n SET @message\t= 'Failed to update invoice';\r\n SET @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId,@machineName = @machineName,@processName = @processName,@action = @action,@severity = @severity,@message = @message,@messageDetails = @messageDetails,@exceptionDetails = @exceptionDetails,@domain = @domain,@username = @username,@userId = @userId,@sendEmail = @sendEmail,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}