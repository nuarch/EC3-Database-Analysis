        {
          "type": "text",
          "text": "CREATE PROCEDURE [Billing].[USP_ContractRetrieve]\n(\r\n\t@providerId AS INT = NULL\r\n\t,@contractId AS INT = NULL\r\n\t,@contractNumber AS VARCHAR(25) = NULL\r\n\t,@groupId AS INT = NULL\r\n\t,@fiscalYear AS VARCHAR(4) = NULL\r\n\t,@activeOnly AS BIT = NULL\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSELECT @providerId = NULLIF(@providerId, 0), @contractId = NULLIF(@contractId, 0), @contractNumber = NULLIF(@contractNumber, '')\r\n\t\t,@fiscalYear = NULLIF(@fiscalYear, ''), @activeOnly = ISNULL(@activeOnly, 0), @userId = ISNULL(@userId, 0)\r\n\t\t,@correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())), @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA')\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Retrieved utility provider contracts', @messageDetails AS VARCHAR(MAX)\r\n\t\t,@startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0;\r\n\t\r\n\tSELECT @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @providerId AS PRVID, @fiscalYear AS FY, @groupId AS GRPID, @contractId AS ID, @contractNumber AS NUM\r\n\t\t,@activeOnly AS ACT FOR XML PATH('INPUTS')));\r\n\r\n\tBEGIN TRY\r\n\t\tDECLARE @fiscalYearStart AS VARCHAR(8);\r\n\r\n\t\tIF (ISNULL(@fiscalYear, '') <> '')\r\n\t\tBEGIN\r\n\t\t\tSET @fiscalYearStart = CONVERT(VARCHAR(4), (CONVERT(INT, @fiscalYear) - 1)) + '0701';\r\n\t\tEND;\r\n\r\n\t\tDECLARE @tmpContracts AS TABLE (ContractID INT NOT NULL);\r\n\r\n\t\tIF (ISNULL(@groupId, 0) > 0)\r\n\t\tBEGIN\r\n\t\t\tINSERT INTO @tmpContracts (ContractID)\r\n\t\t\tSELECT DISTINCT ContractID\r\n\t\t\tFROM Billing.vwBillingGroupContracts\r\n\t\t\tWHERE GroupID = ISNULL(@groupId, GroupID)\r\n\t\t\t\tAND FiscalYear = CASE WHEN ISNULL(@groupId, 0) > 0 THEN ISNULL(@fiscalYear, FiscalYear) ELSE FiscalYear END\r\n\t\t\t\tAND Active = @activeOnly;\r\n\t\tEND;\r\n\t\tELSE\r\n\t\tBEGIN\r\n\t\t\tINSERT INTO @tmpContracts (ContractID)\r\n\t\t\tSELECT DISTINCT ContractID\r\n\t\t\tFROM Billing.vwContracts;\r\n\t\tEND;\r\n\r\n\t\tSELECT C.ContractID\r\n\t\t\t,C.ContractNumber\r\n\t\t\t,C.[Name]\r\n\t\t\t,C.[Description]\r\n\t\t\t,C.ProviderID\r\n\t\t\t,C.FiscalYearStart\r\n\t\t\t,C.FiscalYearEnd\r\n\t\t\t,C.ActualAmount\r\n\t\t\t,Billing.UDF_GetContractAmountPaid(C.ContractID, GETDATE()) AS CloseAmount\r\n\t\t\t,C.ActualAmount - Billing.UDF_GetContractAmountPaid(C.ContractID, GETDATE()) AS OpenAmount\r\n\t\t\t,C.AvailableAmount\r\n\t\t\t,C.CreatedDate\r\n\t\t\t,C.CreatedBy\r\n\t\t\t,C.ModifiedDate\r\n\t\t\t,C.ModifiedBy\r\n\t\t\t,C.Active\r\n\t\t\t,C.CapacityAmount\r\n\t\t\t,P.ProviderName\r\n\t\tFROM Billing.vwContracts AS C\r\n\t\t\tINNER JOIN @tmpContracts AS GC ON GC.ContractID = C.ContractID\r\n\t\t\tINNER JOIN Billing.vwProviders AS P ON C.ProviderID = P.ProviderID\r\n\t\tWHERE C.ContractID = ISNULL(@contractId, C.ContractID)\r\n\t\t\tAND C.ContractNumber = ISNULL(@contractNumber, C.ContractNumber)\r\n\t\t\tAND C.ProviderID = ISNULL(@providerId, C.ProviderID)\r\n\t\t\tAND CASE WHEN ISNULL(@fiscalYear, '') <> '' THEN CASE WHEN @fiscalYearStart BETWEEN C.FiscalYearStart AND C.FiscalYearEnd THEN 1 ELSE 0 END ELSE 1 END = 1\r\n\t\t\tAND C.Active = CASE WHEN @activeOnly = 1 THEN 1 ELSE C.Active END\r\n\t\tORDER BY C.FiscalYearStart DESC, C.ContractNumber DESC;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to retrieve utility provider contracts', @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}