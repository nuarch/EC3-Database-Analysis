        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_InvoiceContractLineDelete]\n\t@invoiceId\t\t\t\t\tINT = NULL\r\n\t,@invoiceContractLineId\tINT = NULL\r\n\t,@comments\t\t\t\t\tVARCHAR(500) = NULL\r\n\t,@correlationId\t\t\tVARCHAR(128) = NULL\r\n\t,@machineName\t\t\t\tVARCHAR(128) = NULL\r\n\t,@processName\t\t\t\tVARCHAR(128) = NULL\r\n\t,@domain\t\t\t\t\tVARCHAR(25) = NULL\r\n\t,@username\t\t\t\t\tVARCHAR(25) = NULL\r\n\t,@userId\t\t\t\t\tINT = NULL\r\n\t,@exceptionDetails\t\t\tVARCHAR(MAX) = NULL OUTPUT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @invoiceId\t\t\t\t= NULLIF(@invoiceId, 0);\r\n\tSET @invoiceContractLineId\t= NULLIF(@invoiceContractLineId, 0);\r\n\tSET @comments\t\t\t\t= ISNULL(NULLIF(@comments, ''), 'Deleted invoice contract line');\r\n\r\n\tDECLARE @action VARCHAR(25), @severity VARCHAR(25), @message VARCHAR(255), @messageDetails VARCHAR(MAX)\r\n\t\t, @startTime DATETIME, @elapsedTime INT, @sendEmail BIT;\r\n\tSET @startTime\t\t\t= GETDATE();\r\n\t\r\n\tSET @correlationId\t\t= ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()));\r\n\tSET @machineName\t\t= COALESCE(@machineName, @@SERVERNAME, 'NA');\r\n\tSET @processName\t\t= COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\tSET @action\t\t\t\t= 'DELETE';\r\n\tSET @severity\t\t\t= 'INFO';\r\n\tSET @message\t\t\t= 'Deleted invoice contract line';\r\n\tSET @messageDetails\t\t= CONVERT(VARCHAR(MAX), (SELECT @invoiceContractLineId AS BILLID, @comments AS CMT FOR XML PATH('INPUTS')));\r\n\tSET @sendEmail\t\t\t= 0;\r\n\tSET @userId\t\t\t\t= ISNULL(@userId, 0);\r\n\r\n\tIF ISNULL(@invoiceId, 0) = 0 AND ISNULL(@invoiceContractLineId, 0) <> 0\r\n\t\tSELECT @invoiceId = InvoiceID FROM [Invoice].[vwInvoiceContractLines] CL WHERE CL.InvoiceContractLineID = @invoiceContractLineId;\r\n\r\n\tIF ISNULL(@invoiceId, 0) = 0 AND ISNULL(@invoiceContractLineId, 0) = 0\r\n\t\tRAISERROR (N'Please specify either the invoice or the line', 16, 1);\r\n\r\n\tDECLARE @noteFiller VARCHAR(500);\r\n\tIF @invoiceContractLineId > 0\r\n\t\tSELECT @noteFiller = (CL.ContractNumber + ',' + CONVERT(VARCHAR(10), CL.CommodityLine) + ',' + CONVERT(VARCHAR(10), CL.AccountingLine))\r\n\t\tFROM [Invoice].[vwInvoiceContractLines] CL\r\n\t\tWHERE CL.InvoiceContractLineID = @invoiceContractLineId;\r\n\tELSE IF @invoiceId > 0\r\n\t\tSET @noteFiller = 'All lines';\r\n\r\n\tDECLARE @tranName VARCHAR(128);\r\n\tSET @tranName = CONVERT(VARCHAR(128), NEWID());\r\n\r\n\tBEGIN TRY\r\n\t\tBEGIN TRAN @tranName;\r\n\r\n\t\t-- Delete invoice contract line allocations\r\n\t\tIF ISNULL(@invoiceContractLineId, 0) > 0\r\n\t\tBEGIN\r\n\t\t\t-- By specific invoice contract line\r\n\t\t\tDELETE FROM [Invoice].[InvoiceContractLineAllocation]\r\n\t\t\tWHERE InvoiceContractLineID\t= @invoiceContractLineId;\r\n\t\tEND\r\n\t\t\r\n\t\tELSE IF ISNULL(@invoiceId, 0) > 0\r\n\t\tBEGIN\r\n\t\t\t-- By invoice\r\n\t\t\tDELETE ICLA\r\n\t\t\tFROM [Invoice].[InvoiceContractLineAllocation] ICLA\r\n\t\t\tJOIN [Invoice].[InvoiceContractLine] ICL ON ICLA.InvoiceContractLineID = ICL.InvoiceContractLineID\r\n\t\t\tWHERE ICL.InvoiceID\t\t\t= @invoiceId;\r\n\t\tEND\r\n\r\n\t\t-- Delete invoice contract line\r\n\t\tDELETE FROM [Invoice].[InvoiceContractLine]\r\n\t\tWHERE InvoiceID\t\t\t\t\t= ISNULL(@invoiceId, InvoiceID)\r\n\t\t\tAND InvoiceContractLineID\t= ISNULL(@invoiceContractLineId, InvoiceContractLineID);\r\n\r\n\t\t-- Create deletion record\r\n\t\tINSERT INTO [Common].[Note] (ParentType, ParentID, [Action], Note, CreatedBy)\r\n\t\t\tVALUES ('INVOICE', @invoiceId, 'REMOVE CONTRACT LINE', @noteFiller, @userId);\r\n\r\n\t\t-- Create audit note\r\n\t\tINSERT INTO [Common].[Note] (ParentType, ParentID, [Action], Note, CreatedBy)\r\n\t\t\tVALUES ('INVOICE', @invoiceId, 'REMOVE CONTRACT LINE', @comments, @userId);\r\n\r\n\t\tCOMMIT TRAN @tranName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tIF XACT_STATE() <> 0 ROLLBACK TRAN @tranName;\r\n\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE();\r\n\t\tSET @severity\t= 'ERROR';\r\n\t\tSET @message\t= 'Failed to delete invoice contract line';\r\n\t\tSET @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}