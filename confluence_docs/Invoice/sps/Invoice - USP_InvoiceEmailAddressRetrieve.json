        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_InvoiceEmailAddressRetrieve]\n(\r\n\t@invoiceID AS INT = 0,@paymentNumber AS VARCHAR(255) = NULL,@applicationId AS VARCHAR(128) = NULL,@correlationId AS VARCHAR(128) = NULL,@machineName AS VARCHAR(128) = NULL,@processName AS VARCHAR(128) = NULL,@domain AS VARCHAR(25) = NULL,@username AS VARCHAR(25) = NULL,@userId AS INT = NULL,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\t\r\n\tSELECT @invoiceID = NULLIF(@invoiceID, 0), @paymentNumber = NULLIF(@paymentNumber, '');\r\n\r\n\tIF (ISNULL(@invoiceId, 0) <= 0 AND @paymentNumber IS NULL)\r\n\tBEGIN\r\n RAISERROR (N'The invoiceId or paymentNumber must be specified', 16, 1);\r\n\tEND;\r\n\r\n\tDECLARE @action AS VARCHAR(25), @severity AS VARCHAR(25), @message AS VARCHAR(255), @messageDetails AS VARCHAR(MAX),@startTime AS DATETIME, @elapsedTime AS INT, @sendEmail AS BIT, @ConEdSteamProfileId AS INT, @ConEdOtherProfileId AS INT,@LIPAProfileId AS INT, @NGridEastProfileId AS INT, @PAofNYNJProfileId AS INT, @MetroProfileId AS INT, @NavyYardProfileId AS INT,@GenerateHudson AS INT, @CentralHudson AS INT, @NYCEDC AS INT, @NYSOMH AS INT, @notificationProfileId AS INT = 0;\r\n\tDECLARE @providerList AS TABLE (providerID INT NOT NULL);\r\n\tSELECT @startTime = GETDATE(), @applicationId = ISNULL(@applicationId, 103), @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())),@machineName = COALESCE(@machineName, @@SERVERNAME, 'NA'), @processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA'),@action = 'RETRIEVE', @severity = 'INFO', @message = 'Invoices - Retrieved Provider Emails by InvoiceID', @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @invoiceID AS InvoiceID FOR XML PATH('INPUTS'))),@sendEmail = 0, @userId = ISNULL(@userId, 0);\r\n \r\n\tINSERT INTO @providerList (providerID) VALUES (1), (2), (3), (7), (9), (12), (16), (18), (19), (43), (47), (69);\r\n\r\n\tBEGIN TRY\r\n SELECT @ConEdSteamProfileId = [Con Edison Steam Payment Notification],\r\n @ConEdOtherProfileId = [Con Edison Payment Notification],\r\n @LIPAProfileId = [PSEG Payment Notification],\r\n @NGridEastProfileId = [National Grid East Payment Notification],\r\n @PAofNYNJProfileId = [PAofNYNJ Payment Notification],\r\n @MetroProfileId = [Metro Energy Payment Notification],\r\n @NavyYardProfileId = [Brooklyn Navy Yard Payment Notification],\r\n @GenerateHudson = [Generate Hudson Payment Notification],\r\n @CentralHudson = [Central Hudson Payment Notification],\r\n @NYCEDC = [NYCEDC Payment Notification],\r\n @NYSOMH = [NYSOMH Payment Notification]\r\n FROM (SELECT [MassDistributionProfileSeqid], shortDesc\r\n FROM Billing.EC3_Contact_MassDistributionProfile\r\n WHERE shortDesc IN ('PSEG Payment Notification', 'Con Edison Steam Payment Notification', 'Con Edison Payment Notification', 'National Grid East Payment Notification',\r\n 'PAofNYNJ Payment Notification', 'Metro Energy Payment Notification', 'Brooklyn Navy Yard Payment Notification', 'Generate Hudson Payment Notification',\r\n 'Central Hudson Payment Notification', 'NYCEDC Payment Notification', 'NYSOMH Payment Notification')\r\n ) AS SourceTable\r\n PIVOT (MAX([MassDistributionProfileSeqid]) FOR shortDesc IN ([PSEG Payment Notification], [Con Edison Steam Payment Notification], [Con Edison Payment Notification],\r\n [National Grid East Payment Notification], [PAofNYNJ Payment Notification], [Metro Energy Payment Notification], [Brooklyn Navy Yard Payment Notification],\r\n [Generate Hudson Payment Notification], [Central Hudson Payment Notification], [NYCEDC Payment Notification], [NYSOMH Payment Notification])\r\n ) AS pvt;\r\n\r\n SELECT @notificationProfileId =\r\n CASE WHEN ProviderID = 3 THEN @LIPAProfileId\r\n WHEN ProviderID = 18 THEN @NGridEastProfileId\r\n WHEN ProviderID = 43 THEN @PAofNYNJProfileId\r\n WHEN ProviderID = 1 THEN @MetroProfileId\r\n WHEN ProviderID = 19 THEN @NavyYardProfileId\r\n WHEN ProviderID = 69 THEN @GenerateHudson\r\n WHEN ProviderID = 12 THEN @CentralHudson\r\n WHEN ProviderID = 47 THEN @NYCEDC\r\n WHEN ProviderID = 16 THEN @NYSOMH\r\n WHEN (ProviderID = 9 AND GroupID = 10) THEN @ConEdSteamProfileId \r\n ELSE @ConEdOtherProfileId\r\n END\r\n FROM (SELECT DISTINCT a.ProviderID, MIN(IIF(b.GroupID = 10, 10, 0)) AS groupid\r\n FROM Invoice.Invoice AS a\r\n INNER JOIN Invoice.InvoicePayment AS b ON a.InvoiceID = b.InvoiceID\r\n WHERE a.InvoiceID = ISNULL(@invoiceID, a.invoiceid) AND b.PaymentNumber = ISNULL(@paymentNumber, b.PaymentNumber)\r\n AND a.ProviderID IN (SELECT providerID FROM @providerList)\r\n GROUP BY a.ProviderID) AS tmp;\r\n\r\n SELECT a.MassDistributionProfileSeqid,c.ContactAddressSeqid,c.ContactName,c.ContactLastName,c.ContactFirstName,c.ContactMI,c.Curtsytitle,c.ContactTelephoneNumber,c.EmailAddress,CAST(IIF(c.UtilityCompanyContact IS NOT NULL, 1, 0) AS BIT) AS IsTo,CAST(IIF(c.AgencyContact IS NOT NULL, 1, 0) AS BIT) AS IsCc\r\n FROM Billing.EC3_Contact_MassDistributionProfile AS a\r\n INNER JOIN Billing.EC3_Contact_MassDistributionListReportContext AS b ON a.MassDistributionProfileSeqid = b.MassDistributionProfileSeqid\r\n INNER JOIN Billing.EC3_Contact_ContactAddress AS c ON b.ContactAddressSeqid = c.ContactAddressSeqid\r\n WHERE a.MassDistributionProfileSeqid = @notificationProfileId AND c.IsRemoved = 'N'\r\n ORDER BY a.MassDistributionProfileSeqid, IsTo DESC, c.ContactLastName, c.ContactFirstName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n SELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Invoices - Failed to retrieve Provider Emails by InvoiceID', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId,@machineName = @machineName,@processName = @processName,@action = @action,@severity = @severity,@message = @message,@messageDetails = @messageDetails,@exceptionDetails = @exceptionDetails,@domain = @domain,@username = @username,@userId = @userId,@sendEmail\t= @sendEmail,@elapsedTime = @elapsedTime\r\nEND;\r"
        }
      ]
    }
  ]
}