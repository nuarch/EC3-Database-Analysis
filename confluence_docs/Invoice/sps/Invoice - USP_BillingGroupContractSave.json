        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_BillingGroupContractSave]\n(\r\n\t@groupId AS INT\r\n\t,@fiscalYear AS VARCHAR(4)\r\n\t,@contractIds AS VARCHAR(4000) = NULL\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'UPDATE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Saved billing group contract(s)'\r\n\t\t,@messageDetails AS VARCHAR(MAX), @startTime AS DATETIME = GETDATE(), @elapsedTime INT, @sendEmail AS BIT = 0\r\n\t\t,@tranName AS VARCHAR(128) = CONVERT(VARCHAR(128), NEWID());\r\n\r\n\tSELECT @groupId = NULLIF(@groupId, 0), @contractIds\t = NULLIF(@contractIds, ''),  @userId = ISNULL(@userId, 0);\r\n\tSELECT @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())), @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA')\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA')\r\n\t\t,@messageDetails = CONVERT(VARCHAR(MAX), (SELECT @groupId AS INVID, @contractIds AS FY FOR XML PATH('INPUTS')));\r\n\r\n\tBEGIN TRY\r\n\t\tBEGIN TRAN @tranName;\r\n\r\n\t\tDECLARE @newBGC TABLE(GroupID INT\r\n\t\t,FiscalYear VARCHAR(4)\r\n\t\t,ContractID VARCHAR(100)\r\n\t\t,ToBeAdded BIT\r\n\t\t,BGCID INT);\r\n\r\n\t\tDECLARE @oldBGC TABLE(GroupID INT\r\n\t\t,FiscalYear VARCHAR(4)\r\n\t\t,ContractID VARCHAR(100)\r\n\t\t,ToBeDeleted BIT\r\n\t\t,BGCID INT);\r\n\r\n\t\tINSERT INTO @newBGC\r\n\t\tSELECT @groupId\r\n\t\t\t,@fiscalYear\r\n\t\t\t,ID\r\n\t\t\t,CONVERT(BIT, 1)\r\n\t\t\t,CONVERT(INT, 0)\r\n\t\tFROM dbo.UDF_SplitToTable(@contractIds, ',');\r\n\r\n\t\tUPDATE T\r\n\t\tSET T.ToBeAdded = 0\r\n\t\t\t,T.BGCID = BGC.BillingGroupContractID\r\n\t\tFROM @newBGC AS T\r\n\t\t\tINNER JOIN Billing.BillingGroupContract AS BGC ON BGC.GroupID = T.GroupID\r\n\t\t\t\tAND BGC.FiscalYear = T.FiscalYear AND BGC.ContractID = T.ContractID;\r\n\r\n\t\tINSERT INTO @oldBGC\r\n\t\tSELECT GroupID\r\n\t\t\t,FiscalYear\r\n\t\t\t,ContractID\r\n\t\t\t,CONVERT(BIT, 0)\r\n\t\t\t,BillingGroupContractID\r\n\t\tFROM Billing.BillingGroupContract\r\n\t\tWHERE GroupID = @groupId AND FiscalYear = @fiscalYear;\r\n\r\n\t\tUPDATE T\r\n\t\tSET T.ToBeDeleted = 1\r\n\t\tFROM @oldBGC AS T\r\n\t\tWHERE T.BGCID NOT IN (SELECT BGCID FROM @newBGC);\r\n\r\n\t\tIF EXISTS (SELECT 1 FROM @newBGC WHERE ToBeAdded = 1)\r\n\t\tBEGIN\r\n\t\t\tINSERT INTO Billing.BillingGroupContract (GroupID, FiscalYear, ContractID, CreatedBy)\r\n\t\t\tSELECT GroupID, FiscalYear, ContractID, @userId\r\n\t\t\tFROM @newBGC\r\n\t\t\tWHERE ToBeAdded = 1;\r\n\t\tEND;\r\n\r\n\t\tIF EXISTS (SELECT 1 FROM @newBGC WHERE ToBeAdded = 0)\r\n\t\tBEGIN\r\n\t\t\tUPDATE BGC\r\n\t\t\tSET BGC.Active = 1\r\n\t\t\t\t,BGC.ModifiedBy = @userId\r\n\t\t\t\t,BGC.ModifiedDate = GETDATE()\r\n\t\t\tFROM Billing.BillingGroupContract AS BGC\r\n\t\t\t\tINNER JOIN @newBGC AS T ON T.BGCID = BGC.BillingGroupContractID;\r\n\t\tEND;\r\n\r\n\t\tIF EXISTS (SELECT 1 FROM @oldBGC WHERE ToBeDeleted = 1)\r\n\t\tBEGIN\r\n\t\t\tUPDATE BGC\r\n\t\t\tSET BGC.Active = 0\r\n\t\t\t\t,BGC.ModifiedBy = @userId\r\n\t\t\t\t,BGC.ModifiedDate = GETDATE()\r\n\t\t\tFROM Billing.BillingGroupContract AS BGC\r\n\t\t\t\tINNER JOIN @oldBGC AS T ON T.BGCID = BGC.BillingGroupContractID\r\n\t\t\tWHERE ToBeDeleted = 1;\r\n\t\tEND;\r\n\r\n\t\tCOMMIT TRAN @tranName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tIF (XACT_STATE() <> 0)\r\n\t\tBEGIN\r\n\t\t\tROLLBACK TRAN @tranName;\r\n\t\tEND;\r\n\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to save billing group contract(s)', @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}