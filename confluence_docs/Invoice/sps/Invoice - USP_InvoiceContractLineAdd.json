      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_InvoiceContractLineAdd]\n\t@invoiceId INT,@contractLineId INT,@amount DECIMAL(18, 2),@allocations [Invoice].[UDT_InvoiceContractLineAllocation] READONLY,@correlationId AS VARCHAR(128) = NULL,@machineName AS VARCHAR(128) = NULL,@processName AS VARCHAR(128) = NULL,@domain AS VARCHAR(25) = NULL,@username AS VARCHAR(25) = NULL,@userId AS INT = NULL,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @invoiceId = NULLIF(@invoiceId, 0);\r\n\tSET @contractLineId = NULLIF(@contractLineId, 0);\r\n\tSET @amount = NULLIF(@amount, 0);\r\n\r\n\tDECLARE @action VARCHAR(25), @severity VARCHAR(25), @message VARCHAR(255), @messageDetails VARCHAR(MAX), @startTime DATETIME, @elapsedTime INT, @sendEmail BIT;\r\n\tSET @startTime = GETDATE();\r\n\t\r\n\tSET @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()));\r\n\tSET @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA');\r\n\tSET @processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\tSET @action = 'CREATE';\r\n\tSET @severity = 'INFO';\r\n\tSET @message = 'Added invoice contract line';\r\n\tSET @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @invoiceId AS INVID, @contractLineId AS CLINEID, @amount AS AMT FOR XML PATH('INPUTS')));\r\n\tSET @sendEmail = 0;\r\n\tSET @userId = ISNULL(@userId, 0);\r\n\r\n\tDECLARE @tranName VARCHAR(128);\r\n\tSET @tranName = CONVERT(VARCHAR(128), NEWID());\r\n\r\n\tDECLARE @noteFiller VARCHAR(500);\r\n\tSELECT @noteFiller = (CL.ContractNumber + ',' + CONVERT(VARCHAR(10), CL.CommodityLine) + ',' + CONVERT(VARCHAR(10), CL.AccountingLine))\r\n\tFROM [Billing].[vwContractLines] CL\r\n\tWHERE CL.ContractLineID = @contractLineId;\r\n\tSET @noteFiller = ISNULL(@noteFiller, CONVERT(VARCHAR(10), @contractLineId) + ',NA,NA');\r\n\r\n\tDECLARE @invoiceIsSplitFY BIT;\r\n\tSELECT\t@invoiceIsSplitFY = [Billing].[UDF_IsSplitFYInvoice](@invoiceId);\r\n\r\n\tDECLARE @invoiceContractLineTable TABLE (InvoiceContractLineID INT, FiscalYear VARCHAR(4), Amount DECIMAL(18, 2));\r\n\r\n\tBEGIN TRY\r\n BEGIN TRAN @tranName;\r\n\r\n IF EXISTS (SELECT 1 FROM Invoice.InvoiceContractLine WHERE InvoiceID = @invoiceId AND ContractLineID = @contractLineId)\r\n BEGIN\r\n -- Update contract line(s)\r\n UPDATE [Invoice].[InvoiceContractLine]\r\n SET\tAmountPaid = AmountPaid + @amount, ModifiedBy = @userId, ModifiedDate = GETDATE()\r\n OUTPUT INSERTED.InvoiceContractLineID, INSERTED.FiscalYear, INSERTED.AmountPaid INTO @invoiceContractLineTable\r\n WHERE InvoiceID = @invoiceId AND ContractLineID = @contractLineId;\r\n \r\n\r\n -- Update default contract line allocation(s)\r\n IF @invoiceIsSplitFY =1\r\n UPDATE [Invoice].[InvoiceContractLineAllocation]\r\n SET\tAllocationAmount = AllocationAmount + b.Amount, ModifiedBy = @userId, ModifiedDate = GETDATE()\r\n FROM [Invoice].[InvoiceContractLineAllocation] a JOIN @allocations b ON a.AllocationFiscalYear=b.FiscalYear\r\n WHERE a.InvoiceContractLineID = (SELECT InvoiceContractLineID FROM @invoiceContractLineTable);\r\n\r\n ELSE\r\n UPDATE [Invoice].[InvoiceContractLineAllocation]\r\n SET\tAllocationAmount = AllocationAmount + @amount, ModifiedBy = @userId, ModifiedDate = GETDATE()\r\n WHERE InvoiceContractLineID = (SELECT InvoiceContractLineID FROM @invoiceContractLineTable) \r\n AND AllocationFiscalYear = (SELECT FiscalYear FROM @invoiceContractLineTable);\r\n \r\n -- Create audit note\r\n INSERT INTO [Common].[Note] (ParentType, ParentID, [Action], Note, CreatedBy)\r\n VALUES ('INVOICE', @invoiceId, 'UPDATE CONTRACT LINE', 'Via Inserting New Payment, Contract line (' + @noteFiller + ') updated', @userId);\r\n END;\r\n ELSE\r\n BEGIN\r\n -- Add contract line(s)\r\n INSERT INTO [Invoice].[InvoiceContractLine] (InvoiceID, ContractLineID, ContractID, FiscalYear, CommodityLine, AccountingLine, BudgetCode, ObjectCode, UoA, AmountPaid, CreatedBy)\r\n OUTPUT INSERTED.InvoiceContractLineID, INSERTED.FiscalYear, INSERTED.AmountPaid INTO @invoiceContractLineTable\r\n SELECT @invoiceId, CL.ContractLineId, CL.ContractID, CL.FiscalYear, CL.CommodityLine, CL.AccountingLine, CL.BudgetCode, CL.ObjectCode, CL.UoA, @amount, @userId\r\n FROM [Billing].[ContractLine] CL\r\n WHERE CL.ContractLineID = @contractLineId;\r\n\r\n -- Add default contract line allocation(s)\r\n IF @invoiceIsSplitFY =1\r\n INSERT INTO [Invoice].[InvoiceContractLineAllocation] (InvoiceContractLineID, AllocationFiscalYear, AllocationAmount, Comments, CreatedBy)\r\n SELECT (SELECT ICL.InvoiceContractLineID FROM @invoiceContractLineTable ICL) AS InvoiceContractLineID, FiscalYear, Amount, Comments, @userId\r\n FROM @allocations;\r\n\r\n ELSE\r\n INSERT INTO [Invoice].[InvoiceContractLineAllocation] (InvoiceContractLineID, AllocationFiscalYear, AllocationAmount, CreatedBy)\r\n SELECT ICL.InvoiceContractLineID, ICL.FiscalYear, ICL.Amount, @userId \r\n FROM @invoiceContractLineTable ICL;\r\n\r\n -- Create audit note\r\n INSERT INTO [Common].[Note] (ParentType, ParentID, [Action], Note, CreatedBy)\r\n VALUES ('INVOICE', @invoiceId, 'ADD CONTRACT LINE', 'Contract line (' + @noteFiller + ') added', @userId);\r\n\r\n END;\r\n\r\n COMMIT TRAN @tranName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n IF XACT_STATE() <> 0 ROLLBACK TRAN @tranName;\r\n\r\n SELECT @exceptionDetails = ERROR_MESSAGE();\r\n SET @severity\t= 'ERROR';\r\n SET @message\t= 'Failed to add invoice contract line';\r\n SET @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId,@machineName = @machineName,@processName = @processName,@action = @action,@severity = @severity,@message = @message,@messageDetails = @messageDetails,@exceptionDetails = @exceptionDetails,@domain = @domain,@username = @username,@userId = @userId,@sendEmail = @sendEmail,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}