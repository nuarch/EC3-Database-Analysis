      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[USP_InvoiceContractLineAllocationRetrieve]\n(\r\n\t@invoiceId AS INT = NULL\r\n\t,@invoiceNumber AS VARCHAR(25) = NULL\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT @invoiceId = NULLIF(@invoiceId, 0), @invoiceNumber = NULLIF(@invoiceNumber, ''), @userId\t= ISNULL(@userId, 0)\r\n\t\t,@correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())), @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA')\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\r\n\tIF (ISNULL(@invoiceId, 0) <= 0 AND ISNULL(@invoiceNumber, '') = '')\r\n\tBEGIN\r\n\t\tRAISERROR (N'Either the invoice identifier or the invoice number must be specified', 16, 1);\r\n\tEND;\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Reports - Retrieved invoice contract line allocations'\r\n\t\t,@messageDetails AS VARCHAR(MAX), @startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0;\r\n\t\r\n\tSELECT @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @invoiceId AS INVID, @invoiceNumber AS INVNUM FOR XML PATH('INPUTS')));\r\n\r\n\tBEGIN TRY\r\n\t\tDECLARE @invoiceIsSplitFY AS BIT = Billing.UDF_IsSplitFYInvoice(@invoiceId);\r\n\r\n\t\tIF (@invoiceIsSplitFY = 0)\r\n\t\tBEGIN\r\n\t\t\tSELECT ICLA.InvoiceContractLineAllocationID\r\n\t\t\t\t,ICLA.AllocationFiscalYear\r\n\t\t\t\t,ICLA.AllocationAmount\r\n\t\t\t\t,ICLA.InvoiceContractLineID\r\n\t\t\t\t,ICLA.InvoiceID\r\n\t\t\t\t,ICLA.InvoiceNumber\r\n\t\t\t\t,ICLA.ContractID\r\n\t\t\t\t,ICLA.ContractNumber\r\n\t\t\t\t,ICLA.FiscalYear\r\n\t\t\t\t,ICLA.CommodityLine\r\n\t\t\t\t,ICLA.AccountingLine\r\n\t\t\t\t,ICLA.BudgetCode\r\n\t\t\t\t,ICLA.ObjectCode\r\n\t\t\t\t,ICLA.UoA\r\n\t\t\t\t,ICLA.AmountPaid\r\n\t\t\t\t,ICLA.Comments\r\n\t\t\t\t,ICLA.CreatedBy\r\n\t\t\t\t,b.CreatedDate AS CreatedDate\r\n\t\t\t\t,ICLA.ModifiedBy\r\n\t\t\t\t,ICLA.ModifiedDate\r\n\t\t\t\t,ICLA.Active\r\n\t\t\tFROM Invoice.vwInvoiceContractLineAllocations AS ICLA\r\n\t\t\t\tINNER JOIN Invoice.Invoice AS b ON ICLA.InvoiceID = b.InvoiceID\r\n\t\t\tWHERE ICLA.InvoiceID = ISNULL(@invoiceId, ICLA.InvoiceID)\r\n\t\t\t\tAND ICLA.InvoiceNumber = ISNULL(@invoiceNumber, ICLA.InvoiceNumber)\r\n\t\t\tORDER BY ICLA.ContractNumber ASC\r\n\t\t\t\t,ICLA.FiscalYear DESC\r\n\t\t\t\t,ICLA.CommodityLine ASC\r\n\t\t\t\t,ICLA.AccountingLine ASC;\r\n\t\tEND;\r\n\t\tELSE\r\n\t\tBEGIN\r\n\t\t\tWITH tmpTbl AS (SELECT InvoiceContractLineAllocationID\r\n\t\t\t\t\t,AllocationFiscalYear\r\n\t\t\t\t\t,AmountPaid AS AllocationAmount\r\n\t\t\t\t\t,InvoiceContractLineID\r\n\t\t\t\t\t,InvoiceID\r\n\t\t\t\t\t,InvoiceNumber\r\n\t\t\t\t\t,ContractID\r\n\t\t\t\t\t,ContractNumber\r\n\t\t\t\t\t,FiscalYear\r\n\t\t\t\t\t,CommodityLine\r\n\t\t\t\t\t,AccountingLine\r\n\t\t\t\t\t,BudgetCode\r\n\t\t\t\t\t,ObjectCode\r\n\t\t\t\t\t,UoA\r\n\t\t\t\t\t,AllocationAmount AS AmountPaid\r\n\t\t\t\t\t,Comments\r\n\t\t\t\t\t,CreatedBy\r\n\t\t\t\t\t,CreatedDate\r\n\t\t\t\t\t,ModifiedBy\r\n\t\t\t\t\t,ModifiedDate\r\n\t\t\t\t\t,Active\r\n\t\t\t\tFROM Invoice.vwInvoiceContractLineAllocations\r\n\t\t\t\tWHERE InvoiceID\t= ISNULL(@invoiceId, InvoiceID)\r\n\t\t\t\t\tAND InvoiceNumber\t= ISNULL(@invoiceNumber, InvoiceNumber)\r\n\t\t\t\t\tAND AllocationAmount <> 0)\r\n\t\t\t,fyTbl AS (SELECT ContractID\r\n\t\t\t\t\t,FiscalYear = STUFF((SELECT DISTINCT ', ' + AllocationFiscalYear\r\n\t\t\t\t\t\tFROM tmpTbl b \r\n\t\t\t\t\t\tWHERE b.ContractID = a.ContractID \r\n\t\t\t\t\t\tFOR XML PATH('')), 1, 2, '')\r\n\t\t\t\tFROM tmpTbl AS a\r\n\t\t\t\tGROUP BY ContractID)\r\n\t\t\tSELECT R.InvoiceContractLineAllocationID\r\n\t\t\t\t,R.AllocationFiscalYear\r\n                ,R.AllocationAmount\r\n                ,R.InvoiceContractLineID\r\n                ,R.InvoiceID\r\n                ,R.InvoiceNumber\r\n                ,R.ContractID\r\n                ,R.ContractNumber\r\n\t\t\t\t,R.FiscalYear\r\n                ,R.CommodityLine\r\n                ,R.AccountingLine\r\n                ,R.BudgetCode\r\n                ,R.ObjectCode\r\n                ,R.UoA\r\n                ,R.AmountPaid\r\n                ,R.Comments\r\n                ,R.CreatedBy\r\n                ,R.CreatedDate\r\n                ,R.ModifiedBy\r\n                ,R.ModifiedDate\r\n                ,R.Active\r\n\t\t\tFROM tmpTbl AS R\r\n\t\t\tORDER BY R.ContractNumber ASC\r\n\t\t\t\t,R.FiscalYear DESC\r\n\t\t\t\t,R.CommodityLine ASC\r\n\t\t\t\t,R.AccountingLine ASC;\r\n\t\tEND;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Reports - Failed to retrieve invoice contract line allocations', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}