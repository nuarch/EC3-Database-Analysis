        {
          "type": "text",
          "text": "CREATE PROCEDURE [Billing].[USP_ContractLineRetrieve]\n(\r\n\t@contractLineId AS INT = NULL\r\n\t,@contractId AS INT = NULL\r\n\t,@contractNumber AS VARCHAR(25) = NULL\r\n\t,@fiscalYear AS VARCHAR(4) = NULL\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSELECT @contractLineId = NULLIF(@contractLineId, 0), @contractId = NULLIF(@contractId, 0), @contractNumber = NULLIF(@contractNumber, '')\r\n\t\t,@fiscalYear = NULLIF(@fiscalYear, ''), @userId = ISNULL(@userId, 0), @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()))\r\n\t\t,@machineName = COALESCE(@machineName, @@SERVERNAME, 'NA')\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Retrieved utility provider contract line(s)'\r\n\t\t,@messageDetails AS VARCHAR(MAX), @startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0;\r\n\t\r\n\tSELECT @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @contractLineId AS CLID, @contractId AS CID, @contractNumber AS NUM, @fiscalYear AS FY FOR XML PATH('INPUTS')));\r\n\r\n\tBEGIN TRY\r\n\t\tSELECT ContractLineID\r\n\t\t\t,FiscalYear\r\n\t\t\t,ContractID\r\n\t\t\t,ContractNumber\r\n\t\t\t,CommodityLine\r\n\t\t\t,AccountingLine\r\n\t\t\t,BudgetCode\r\n\t\t\t,ObjectCode\r\n\t\t\t,UoA\r\n\t\t\t,ActualAmount\r\n\t\t\t,Billing.UDF_GetContractLineAmountPaid(ContractLineID, GETDATE()) AS CloseAmount\r\n\t\t\t,ActualAmount - Billing.UDF_GetContractLineAmountPaid(ContractLineID, GETDATE()) AS OpenAmount\r\n\t\t\t,AvailableAmount\r\n\t\t\t,CreatedDate\r\n\t\t\t,CreatedBy\r\n\t\t\t,ModifiedDate\r\n\t\t\t,ModifiedBy\r\n\t\t\t,Active\r\n\t\t\t,Billing.UDF_GetContractLineAmountPaid(ContractLineID, GETDATE()) AS AmountPaid\r\n\t\tFROM Billing.vwContractLines\r\n\t\tWHERE ContractLineID = ISNULL(@contractLineId, ContractLineID)\r\n\t\t\tAND ContractID = ISNULL(@contractId, ContractID)\r\n\t\t\tAND ContractNumber = ISNULL(@contractNumber, ContractNumber)\r\n\t\t\tAND FiscalYear = ISNULL(@fiscalYear, FiscalYear)\r\n\t\tORDER BY FiscalYear, CommodityLine ASC, AccountingLine ASC;\r\n\r\n\t\t-- For History Popup\r\n\t\tSELECT TransactionType\r\n\t\t\t,ContractLineID\r\n\t\t\t,ActualAmount\r\n\t\t\t,StartDate\r\n\t\t\t,EndDate\r\n\t\t\t,ModifiedBy\r\n\t\t\t,ActualAmount + ChangeAmount AS TotalAmount\r\n\t\t\t,ChangeAmount\r\n\t\tFROM Billing.HistoricalContractLine\r\n\t\tWHERE ContractLineId = @ContractLineID AND TransactionType = 1;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to retrieve utility provider contract line(s)', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
