      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_InvoiceAccountBillingRetrieve]\n\t@invoiceId\t\t\t\tINT = NULL\r\n\t,@groupId\t\t\t\tINT = NULL\r\n\t,@invoiceGroupId\t\tINT = NULL\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @invoiceId = NULLIF(@invoiceId, 0);\r\n\tSET @groupId = NULLIF(@groupId, 0);\r\n\tSET @invoiceGroupId = NULLIF(@invoiceGroupId, 0);\r\n\r\n\tIF ISNULL(@invoiceId, 0) <= 0 AND ISNULL(@groupId, 0) <= 0 AND ISNULL(@invoiceGroupId, 0) <= 0\r\n\t\tRAISERROR (N'The invoiceId or the groupId or invoiceGroupId must be specified', 16, 1);\r\n\r\n\tDECLARE @action VARCHAR(25), @severity VARCHAR(25), @message VARCHAR(255), @messageDetails VARCHAR(MAX)\r\n\t\t, @startTime DATETIME, @elapsedTime INT, @sendEmail BIT;\r\n\tSET @startTime\t\t\t= GETDATE();\r\n\tSET @correlationId\t\t= ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()));\r\n\tSET @machineName\t\t= COALESCE(@machineName, @@SERVERNAME, 'NA');\r\n\tSET @processName\t\t= COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\tSET @action\t\t\t\t= 'RETRIEVE';\r\n\tSET @severity\t\t\t= 'INFO';\r\n\tSET @message\t\t\t= 'Invoice - Retrieved invoice accounts / billings';\r\n\tSET @messageDetails\t\t= CONVERT(VARCHAR(MAX), (SELECT ISNULL(@invoiceId, 0) AS INVID, ISNULL(@groupId, 0) AS GRPID, ISNULL(@invoiceGroupId, 0) AS INVGRPID\r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tFOR XML PATH('INPUTS')));\r\n\tSET @sendEmail\t\t\t= 0;\r\n\tSET @userId\t\t\t\t= ISNULL(@userId, 0);\r\n\r\n\tBEGIN TRY\r\n\t\tSELECT IAB.InvoiceBillingID, IAB.InvoiceGroupContractID, IAB.InvoiceID, IAB.InvoiceNumber\r\n\t\t\t, IAB.GroupID, IAB.GroupName, IAB.ContractID, IAB.ContractNumber, IAB.FiscalYear\r\n\t\t\t, IAB.AccountID, IAB.AccountNumber, IAB.FormattedAccountNumber\r\n\t\t\t, IAB.BillingID, IAB.BillingPeriod, IAB.BillingPeriodDays, IAB.BillingPeriodDaysPriorFY, IAB.BillingPeriodDaysCurrentFY\r\n\t\t\t, IAB.IsEstimated, IAB.IsManualEntry\r\n\t\t\t, IAB.AmountBilled, IAB.AmountPaid, IAB.AmountPaidPriorFY, IAB.AmountPaidCurrentFY\r\n\t\t\t, IAB.EnergyUsage, IAB.EnergyUsagePriorFY, IAB.EnergyUsageCurrentFY\r\n\t\t\t, IAB.DemandUsage, IAB.DemandUsagePriorFY, IAB.DemandUsageCurrentFY\r\n\t\t\t, IAB.DateBilled, IAB.DateDue, IAB.FromDate, IAB.ToDate, IAB.SplitFY\r\n\t\t\t, IAB.CreatedDate, IAB.CreatedBy, IAB.ModifiedDate, IAB.ModifiedBy, IAB.Active\r\n\t\tFROM [Invoice].[vwInvoiceAccountBillings] IAB\r\n\t\tWHERE IAB.InvoiceID = ISNULL(@invoiceId, IAB.InvoiceID)\r\n\t\t\tAND IAB.GroupID\t= ISNULL(@groupId, IAB.GroupID)\r\n\t\t\tAND IAB.InvoiceGroupContractID = ISNULL(@invoiceGroupId, IAB.InvoiceGroupContractID)\r\n\t\tORDER BY IAB.GroupName ASC, IAB.ContractNumber ASC, IAB.FiscalYear DESC, IAB.AccountNumber ASC;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE();\r\n\t\tSET @severity\t= 'ERROR';\r\n\t\tSET @message\t= 'Invoice - Failed to retrieve invoice accounts / billings';\r\n\t\tSET @sendEmail\t= 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
