      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Invoice].[USP_ContractAvailableByInvoiceBillingGroupRetrieve]\n(\r\n\t@invoiceId AS INT\r\n\t,@groupId AS INT\r\n\t,@fiscalYear AS VARCHAR(4)\r\n\t,@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL OUTPUT\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSELECT @invoiceId = NULLIF(@invoiceId, 0), @groupId = NULLIF(@groupId, 0), @fiscalYear = NULLIF(@fiscalYear, '')\r\n\t\t,@correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID()))\r\n\t\t,@machineName = COALESCE(@machineName, @@SERVERNAME, 'NA'), @userId = ISNULL(@userId, 0)\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA');\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Retrieved utility provider contracts by invoice and billing groups'\r\n\t\t,@messageDetails AS VARCHAR(MAX), @startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0;\r\n\t\r\n\tSELECT @messageDetails = CONVERT(VARCHAR(MAX), (SELECT @invoiceId AS INVID, @groupId AS GRPID, @fiscalYear AS FY FOR XML PATH('INPUTS')));\r\n\r\n\tBEGIN TRY\r\n\t\tDECLARE @fiscalYearStart AS VARCHAR(8), @fiscalYearEnd AS VARCHAR(8), @providerId AS INT;\r\n\r\n\t\tIF (ISNULL(@fiscalYear, '') <> '')\r\n\t\tBEGIN\r\n\t\t\tSELECT @fiscalYearStart = CONVERT(VARCHAR(4), (CONVERT(INT, @fiscalYear) - 1)) + '0701';\r\n\t\t\tSELECT @fiscalYearEnd = @fiscalYear + '0630';\r\n\t\tEND;\r\n\r\n\t\tSELECT @providerId = ProviderID\r\n\t\tFROM Billing.vwBillingGroups\r\n\t\tWHERE GroupID = @groupId;\r\n\r\n\t\tDECLARE @tmpInvoiceContracts TABLE(ContractID INT NOT NULL);\r\n\t\tDECLARE @tmpGroupContracts TABLE(ContractID INT NOT NULL);\r\n\r\n\t\tINSERT INTO @tmpInvoiceContracts\r\n\t\tSELECT DISTINCT ContractID\r\n\t\tFROM Invoice.vwInvoiceGroupContracts\r\n\t\tWHERE InvoiceID = ISNULL(@invoiceId, InvoiceID) AND GroupID = ISNULL(@groupId, GroupID) AND FiscalYear = ISNULL(@fiscalYear, FiscalYear);\r\n\r\n\t\tINSERT INTO @tmpGroupContracts\r\n\t\tSELECT DISTINCT ContractID\r\n\t\tFROM Billing.vwBillingGroupContracts\r\n\t\tWHERE GroupID = ISNULL(@groupId, GroupID) AND FiscalYear = ISNULL(@fiscalYear, FiscalYear);\r\n\r\n\t\tSELECT C.ContractID\r\n\t\t\t,C.ContractNumber\r\n\t\t\t,C.[Name]\r\n\t\t\t,C.[Description]\r\n\t\t\t,C.ProviderID\r\n\t\t\t,C.ProviderName\r\n\t\t\t,C.FiscalYearStart\r\n\t\t\t,C.FiscalYearEnd\r\n\t\t\t,C.ActualAmount\r\n\t\t\t,C.CloseAmount\r\n\t\t\t,C.OpenAmount\r\n\t\t\t,C.AvailableAmount\r\n\t\t\t,C.CreatedDate\r\n\t\t\t,C.CreatedBy\r\n\t\t\t,C.ModifiedDate\r\n\t\t\t,C.ModifiedBy\r\n\t\t\t,C.Active\r\n\t\t\t,C.CapacityAmount\r\n\t\tFROM Billing.vwContracts AS C\r\n\t\tWHERE C.ProviderID = @providerId\r\n\t\t\tAND C.FiscalYearStart <= @fiscalYearStart AND C.FiscalYearEnd >= @fiscalYearEnd\r\n\t\t\tAND C.ContractID NOT IN (SELECT ContractID FROM @tmpInvoiceContracts)\r\n\t\tORDER BY C.ContractNumber ASC;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to retrieve utility provider contracts by invoice and billing groups', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
