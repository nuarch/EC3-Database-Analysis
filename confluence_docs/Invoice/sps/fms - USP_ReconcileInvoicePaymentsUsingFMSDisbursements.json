      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [fms].[USP_ReconcileInvoicePaymentsUsingFMSDisbursements]\n(\r\n\t@correlationId AS VARCHAR(128) = NULL\r\n\t,@machineName AS VARCHAR(128) = NULL\r\n\t,@processName AS VARCHAR(128) = NULL\r\n\t,@domain AS VARCHAR(25) = NULL\r\n\t,@username AS VARCHAR(25) = NULL\r\n\t,@userId AS INT = NULL\r\n\t,@exceptionDetails AS VARCHAR(MAX) = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Reconciled invoice payments using FMS disbursements(s)'\r\n\t\t,@messageDetails AS VARCHAR(MAX) = NULL, @startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0, @tranName AS VARCHAR(128) = CONVERT(VARCHAR(128), NEWID());\r\n\t\r\n\tSELECT @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())), @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA')\r\n\t\t,@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA'), @userId = ISNULL(@userId, 0);\r\n\r\n\tBEGIN TRY\r\n\t\tDECLARE @paymentRequests AS TABLE (InvoiceID INT NOT NULL, InvoiceGroupContractID INT NOT NULL, ContractNumber VARCHAR(30) NOT NULL);\r\n\t\tDECLARE @paidInvoiceGroups AS TABLE (InvoiceID INT NOT NULL, InvoiceGroupContractID INT NOT NULL);\r\n\r\n\t\tBEGIN TRAN @tranName;\r\n\r\n\t\t-- Find only new payment requests\r\n\t\tINSERT INTO @paymentRequests (InvoiceID, InvoiceGroupContractID, ContractNumber)\r\n\t\tSELECT IGC.InvoiceID, IGC.InvoiceGroupContractID, IGC.ContractNumber\r\n\t\tFROM Invoice.vwInvoiceGroupContracts AS IGC\r\n\t\t\tINNER JOIN fms.PaymentRequest AS FMSPR ON FMSPR.InvoiceNumber = IGC.InvoiceNumber AND FMSPR.ContractNumber = IGC.ContractNumber\r\n\t\t\tINNER JOIN Invoice.vwInvoices AS INV ON INV.InvoiceID = IGC.InvoiceID\r\n\t\tWHERE INV.StatusID IN (4, 5, 6);\r\n\r\n\t\t-- Update status to payment initiated\r\n\t\tUPDATE INV\r\n\t\tSET INV.StatusID = 7\r\n\t\tFROM Invoice.Invoice AS INV\r\n\t\t\tINNER JOIN @paymentRequests AS PR ON PR.InvoiceID = INV.InvoiceID;\r\n\r\n\t\t-- Create audit note for all invoices that have payment requests initiated\r\n\t\tINSERT INTO Common.Note(ParentType, ParentID, [Action], Note, CreatedBy)\r\n\t\tSELECT 'INVOICE', T.InvoiceID, 'FMS PAYMENT REQUEST', 'Payment request initiated in FMS', 0\r\n\t\tFROM (SELECT DISTINCT InvoiceID FROM @paymentRequests) AS T;\r\n\r\n\t\t-- Insert only new payment records\r\n\t\tINSERT INTO Invoice.InvoicePayment(InvoiceGroupContractID, InvoiceID, BillingGroupContractID, GroupID, ContractID, FiscalYear\r\n\t\t\t,PaymentType, PaymentNumber, PaymentAmount, DateIssued, DateCleared, CreatedBy)\r\n\t\tOUTPUT INSERTED.InvoiceID, INSERTED.InvoiceGroupContractID INTO @paidInvoiceGroups\r\n\t\tSELECT IGC.InvoiceGroupContractID, IGC.InvoiceID, IGC.BillingGroupContractID, IGC.GroupID, IGC.ContractID, IGC.FiscalYear\r\n\t\t\t,FMSDB.PaymentType, FMSDB.CHK_EFT_NO, FMSDB.CHK_EFT_AM, FMSDB.CHK_EFT_ISS_DT, FMSDB.CLR_DT, 0\r\n\t\tFROM Invoice.vwInvoiceGroupContracts AS IGC\r\n\t\t\tINNER JOIN fms.Disbursement AS FMSDB ON FMSDB.InvoiceNumber = IGC.InvoiceNumber AND FMSDB.ContractNumber = IGC.ContractNumber\r\n\t\tWHERE (IGC.ContractNumber + '-' + IGC.InvoiceNumber) NOT IN (SELECT DISTINCT (ContractNumber + '-' + InvoiceNumber) FROM Invoice.vwInvoicePayments);\r\n\r\n\t\t-- Do we need to update existing records?\r\n\r\n\t\t-- Update invoice group contracts to paid\r\n\t\tUPDATE IGC\r\n\t\tSET IGC.Paid = 1, ModifiedDate = GETDATE(), ModifiedBy = 0\r\n\t\tFROM Invoice.InvoiceGroupContract AS IGC\r\n\t\t\tINNER JOIN @paidInvoiceGroups AS T ON T.InvoiceGroupContractID = IGC.InvoiceGroupContractID;\r\n\r\n\t\t-- Update invoices to paid if all associated groups are paid\r\n\t\tUPDATE INV\r\n\t\tSET INV.StatusID = 8, ModifiedDate = GETDATE(), ModifiedBy = 0\r\n\t\tFROM Invoice.Invoice AS INV\r\n\t\tWHERE (INV.StatusID <> 8 AND INV.StatusID <> 9)\r\n\t\t\tAND INV.InvoiceID IN (SELECT DISTINCT InvoiceID\r\n\t\t\t\tFROM Invoice.InvoiceGroupContract\r\n\t\t\t\tWHERE Paid = 1\r\n\t\t\t\tGROUP BY InvoiceID\r\n\t\t\t\tHAVING COUNT(1) = COUNT(PAID));\r\n\r\n\t\t-- Create audit note for all paid invoices\r\n\t\tINSERT INTO Common.Note(ParentType, ParentID, [Action], Note, CreatedBy)\r\n\t\tSELECT 'INVOICE', T.InvoiceID, 'FMS PAYMENT ISSUED', 'Payment issued in FMS', 0\r\n\t\tFROM (SELECT DISTINCT InvoiceID FROM @paidInvoiceGroups) AS T;\r\n\r\n\t\tCOMMIT TRAN @tranName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n\t\tIF (XACT_STATE() <> 0)\r\n\t\tBEGIN\r\n\t\t\tROLLBACK TRAN @tranName;\r\n\t\tEND;\r\n\r\n\t\tSELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to reconcile invoice payments using FMS disbursements(s)', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId\r\n\t\t,@machineName = @machineName\r\n\t\t,@processName = @processName\r\n\t\t,@action = @action\r\n\t\t,@severity = @severity\r\n\t\t,@message = @message\r\n\t\t,@messageDetails = @messageDetails\r\n\t\t,@exceptionDetails = @exceptionDetails\r\n\t\t,@domain = @domain\r\n\t\t,@username = @username\r\n\t\t,@userId = @userId\r\n\t\t,@sendEmail = @sendEmail\r\n\t\t,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}