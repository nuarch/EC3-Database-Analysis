      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [fms].[USP_ReconcileInvoicePaymentsUsingFMSDisbursements]\n(\r\n\t@correlationId AS VARCHAR(128) = NULL,@machineName AS VARCHAR(128) = NULL,@processName AS VARCHAR(128) = NULL,@domain AS VARCHAR(25) = NULL,@username AS VARCHAR(25) = NULL,@userId AS INT = NULL,@exceptionDetails AS VARCHAR(MAX) = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @action AS VARCHAR(25) = 'RETRIEVE', @severity AS VARCHAR(25) = 'INFO', @message AS VARCHAR(255) = 'Reconciled invoice payments using FMS disbursements(s)',@messageDetails AS VARCHAR(MAX) = NULL, @startTime AS DATETIME = GETDATE(), @elapsedTime AS INT, @sendEmail AS BIT = 0, @tranName AS VARCHAR(128) = CONVERT(VARCHAR(128), NEWID());\r\n\t\r\n\tSELECT @correlationId = ISNULL(@correlationId, CONVERT(VARCHAR(128), NEWID())), @machineName = COALESCE(@machineName, @@SERVERNAME, 'NA'),@processName = COALESCE(@processName, QUOTENAME(OBJECT_SCHEMA_NAME(@@PROCID)) + '.' + QUOTENAME(OBJECT_NAME(@@PROCID)), 'NA'), @userId = ISNULL(@userId, 0);\r\n\r\n\tBEGIN TRY\r\n DECLARE @paymentRequests AS TABLE (InvoiceID INT NOT NULL, InvoiceGroupContractID INT NOT NULL, ContractNumber VARCHAR(30) NOT NULL);\r\n DECLARE @paidInvoiceGroups AS TABLE (InvoiceID INT NOT NULL, InvoiceGroupContractID INT NOT NULL);\r\n\r\n BEGIN TRAN @tranName;\r\n\r\n -- Find only new payment requests\r\n INSERT INTO @paymentRequests (InvoiceID, InvoiceGroupContractID, ContractNumber)\r\n SELECT IGC.InvoiceID, IGC.InvoiceGroupContractID, IGC.ContractNumber\r\n FROM Invoice.vwInvoiceGroupContracts AS IGC\r\n INNER JOIN fms.PaymentRequest AS FMSPR ON FMSPR.InvoiceNumber = IGC.InvoiceNumber AND FMSPR.ContractNumber = IGC.ContractNumber\r\n INNER JOIN Invoice.vwInvoices AS INV ON INV.InvoiceID = IGC.InvoiceID\r\n WHERE INV.StatusID IN (4, 5, 6);\r\n\r\n -- Update status to payment initiated\r\n UPDATE INV\r\n SET INV.StatusID = 7\r\n FROM Invoice.Invoice AS INV\r\n INNER JOIN @paymentRequests AS PR ON PR.InvoiceID = INV.InvoiceID;\r\n\r\n -- Create audit note for all invoices that have payment requests initiated\r\n INSERT INTO Common.Note(ParentType, ParentID, [Action], Note, CreatedBy)\r\n SELECT 'INVOICE', T.InvoiceID, 'FMS PAYMENT REQUEST', 'Payment request initiated in FMS', 0\r\n FROM (SELECT DISTINCT InvoiceID FROM @paymentRequests) AS T;\r\n\r\n -- Insert only new payment records\r\n INSERT INTO Invoice.InvoicePayment(InvoiceGroupContractID, InvoiceID, BillingGroupContractID, GroupID, ContractID, FiscalYear,PaymentType, PaymentNumber, PaymentAmount, DateIssued, DateCleared, CreatedBy)\r\n OUTPUT INSERTED.InvoiceID, INSERTED.InvoiceGroupContractID INTO @paidInvoiceGroups\r\n SELECT IGC.InvoiceGroupContractID, IGC.InvoiceID, IGC.BillingGroupContractID, IGC.GroupID, IGC.ContractID, IGC.FiscalYear,FMSDB.PaymentType, FMSDB.CHK_EFT_NO, FMSDB.CHK_EFT_AM, FMSDB.CHK_EFT_ISS_DT, FMSDB.CLR_DT, 0\r\n FROM Invoice.vwInvoiceGroupContracts AS IGC\r\n INNER JOIN fms.Disbursement AS FMSDB ON FMSDB.InvoiceNumber = IGC.InvoiceNumber AND FMSDB.ContractNumber = IGC.ContractNumber\r\n WHERE (IGC.ContractNumber + '-' + IGC.InvoiceNumber) NOT IN (SELECT DISTINCT (ContractNumber + '-' + InvoiceNumber) FROM Invoice.vwInvoicePayments);\r\n\r\n -- Do we need to update existing records?\r\n\r\n -- Update invoice group contracts to paid\r\n UPDATE IGC\r\n SET IGC.Paid = 1, ModifiedDate = GETDATE(), ModifiedBy = 0\r\n FROM Invoice.InvoiceGroupContract AS IGC\r\n INNER JOIN @paidInvoiceGroups AS T ON T.InvoiceGroupContractID = IGC.InvoiceGroupContractID;\r\n\r\n -- Update invoices to paid if all associated groups are paid\r\n UPDATE INV\r\n SET INV.StatusID = 8, ModifiedDate = GETDATE(), ModifiedBy = 0\r\n FROM Invoice.Invoice AS INV\r\n WHERE (INV.StatusID <> 8 AND INV.StatusID <> 9)\r\n AND INV.InvoiceID IN (SELECT DISTINCT InvoiceID\r\n FROM Invoice.InvoiceGroupContract\r\n WHERE Paid = 1\r\n GROUP BY InvoiceID\r\n HAVING COUNT(1) = COUNT(PAID));\r\n\r\n -- Create audit note for all paid invoices\r\n INSERT INTO Common.Note(ParentType, ParentID, [Action], Note, CreatedBy)\r\n SELECT 'INVOICE', T.InvoiceID, 'FMS PAYMENT ISSUED', 'Payment issued in FMS', 0\r\n FROM (SELECT DISTINCT InvoiceID FROM @paidInvoiceGroups) AS T;\r\n\r\n COMMIT TRAN @tranName;\r\n\tEND TRY\r\n\tBEGIN CATCH\r\n IF (XACT_STATE() <> 0)\r\n BEGIN\r\n ROLLBACK TRAN @tranName;\r\n END;\r\n\r\n SELECT @exceptionDetails = ERROR_MESSAGE(), @severity = 'ERROR', @message = 'Failed to reconcile invoice payments using FMS disbursements(s)', @sendEmail = 1;\r\n\tEND CATCH;\r\n\r\n\tSET @elapsedTime = DATEDIFF(MILLISECOND, GETDATE(), @startTime);\r\n\r\n\tEXEC dbo.USP_AppLogCreate @correlationId = @correlationId,@machineName = @machineName,@processName = @processName,@action = @action,@severity = @severity,@message = @message,@messageDetails = @messageDetails,@exceptionDetails = @exceptionDetails,@domain = @domain,@username = @username,@userId = @userId,@sendEmail = @sendEmail,@elapsedTime = @elapsedTime;\r\nEND;\r"
        }
      ]
    }
  ]
}