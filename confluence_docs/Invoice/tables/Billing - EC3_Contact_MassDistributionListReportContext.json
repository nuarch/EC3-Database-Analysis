                {
                  "type": "text",
                  "text": "MassDistributionProfile",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table and a "
                },
                {
                  "type": "text",
                  "text": "ContactAddress",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
