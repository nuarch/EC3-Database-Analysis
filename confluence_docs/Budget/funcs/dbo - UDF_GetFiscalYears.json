    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[UDF_GetFiscalYears]\n(\r\n)\r\nRETURNS @Output TABLE (FY INT)\r\nAS\r\nBEGIN\r\n\tDECLARE @fyCtr INT, @maxFY INT;\r\n\r\n\tSET @fyCtr = 2017;\r\n\tSET @maxFY = YEAR(GETDATE()) + 2;\r\n\r\n\tWHILE @fyCtr <= @maxFY\r\n\tBEGIN\r\n\t\tINSERT INTO @Output(FY) VALUES (@fyCtr);\r\n\t\tSET @fyCtr = @fyCtr + 1;\r\n\tEND;\r\n \r\n\tRETURN;\r\nEND;\r"
        }
