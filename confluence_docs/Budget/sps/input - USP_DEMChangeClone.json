    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Insert New DEMChange Record",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": A new record is inserted into the "
                },
                {
                  "type": "text",
                  "text": "DEMChange",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table with the provided fiscal year, name, description, and actionBy. The ID of this new record is captured using the "
                },
                {
                  "type": "text",
                  "text": "OUTPUT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " clause."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Identify Related DEMChangeDetail Records",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure selects distinct "
                },
                {
                  "type": "text",
                  "text": "DEMChangeDetailEntityId",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " values from the "
                },
                {
                  "type": "text",
                  "text": "DEMChangeDetail",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table where the "
                },
                {
                  "type": "text",
                  "text": "DEMChangeID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " matches the "
                },
                {
                  "type": "text",
                  "text": "@originalId",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". These IDs are stored in a temporary table "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Detail_ID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Filter Obsolete Records",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": It identifies obsolete records by checking if the "
                },
                {
                  "type": "text",
                  "text": "AgencyCode",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "BudgetGroupID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " are not valid for the given fiscal year. These obsolete records are stored in another temporary table "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Obsolete",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Remove Obsolete Records",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure deletes obsolete records from "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Detail_ID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Clone DEMChangeDetail Records",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": It iterates over the remaining "
                },
                {
                  "type": "text",
                  "text": "DEMChangeDetailEntityId",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " values in "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Detail_ID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", cloning each record into the "
                },
                {
                  "type": "text",
                  "text": "DEMChangeDetail",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table with adjustments to the "
                },
                {
                  "type": "text",
                  "text": "BillingPeriod",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and a new "
                },
                {
                  "type": "text",
                  "text": "DEMChangeDetailEntityId",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Return Obsolete Records",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Finally, it selects and returns the "
                },
                {
                  "type": "text",
                  "text": "AgencyCode",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "BudgetGroupID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " of the obsolete records."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Cleanup",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Temporary tables "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Obsolete",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "#TEMP_Detail_ID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " are dropped if they exist."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Temporary Tables",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of temporary tables can impact performance, especially if the tables grow large. Indexing these tables could improve performance but is not implemented here."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Iterative Cloning",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure uses a loop to clone records, which can be inefficient for large datasets. A set-based approach might be more performant."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Concurrency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure does not use explicit transaction management, which could lead to concurrency issues if multiple instances run simultaneously."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Integrity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Without explicit transaction handling, partial updates could occur if an error interrupts the procedure."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The iterative approach to cloning details may not scale well with large numbers of records."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Obsolete Record Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The logic for identifying obsolete records relies on external tables ("
                },
                {
                  "type": "text",
                  "text": "Preload.Agency",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "Preload.BudgetGroupByFY",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "), which could lead to issues if these tables are not maintained correctly."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Hardcoded Logic",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The logic for adjusting "
                },
                {
                  "type": "text",
                  "text": "BillingPeriod",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is hardcoded and may not accommodate future changes in business rules without modification."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_DEMChangeClone]\n\t@fy\t\t\t\t\tINT\r\n\t, @name\t\t\t\tVARCHAR(250)\r\n\t, @description\t\tVARCHAR(500)\r\n\t, @actionBy\t\t\tVARCHAR(25) = 'SYSTEM'\r\n\t, @id\t\t\t\tINT OUTPUT\r\n\t, @originalId\t\tINT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @IdOutput TABLE (ID INT);\r\n\tDECLARE @DId INT\r\n\tDECLARE @DEMChangeDetailEntityIdMaxPlusOne   INT\r\n\r\n\tINSERT INTO input.DEMChange (FY, Name, [Description], CreatedBy, ModifiedBy)\r\n\t\t\tOUTPUT INSERTED.ID INTO @IdOutput\r\n\t\tVALUES (@fy, @name, @description, @actionBy, @actionBy);\r\n\r\n\tSELECT @id = ID FROM @IdOutput;\r\n\r\n\tSELECT DISTINCT DEMChangeDetailEntityId\r\n\tINTO #TEMP_Detail_ID\r\n\tFROM [input].[DEMChangeDetail]\r\n    WHERE DEMChangeID = @originalId\r\n\r\n    SELECT DISTINCT [AgencyCode],\r\n        [BudgetGroupID],\r\n        DEMChangeDetailEntityId\r\n    INTO #TEMP_Obsolete\r\n    FROM [input].[DEMChangeDetail]\r\n    WHERE DEMChangeDetailEntityId IN (SELECT DEMChangeDetailEntityId FROM #TEMP_Detail_ID)\r\n        AND (AgencyCode NOT IN (SELECT a.AgencyCodeOEC\r\n            FROM Preload.Agency AS a\r\n                INNER JOIN Preload.AgencyByFY AS abf ON a.AgencyId = abf.AgencyId\r\n            WHERE abf.FY = @fy)\r\n            OR BudgetGroupID NOT IN (SELECT BudgetGroupID FROM Preload.BudgetGroupByFY WHERE FY = @fy));\r\n\r\n    DELETE #TEMP_Detail_ID\r\n    WHERE DEMChangeDetailEntityId IN (SELECT DEMChangeDetailEntityId FROM #TEMP_Obsolete);\r\n\r\n\tWHILE (SELECT COUNT(*) FROM #TEMP_Detail_ID) > 0\r\n\tBEGIN\r\n\tSELECT TOP 1 @DId = DEMChangeDetailEntityId FROM #TEMP_Detail_ID\r\n    SELECT @DEMChangeDetailEntityIdMaxPlusOne = MAX(DEMChangeDetailEntityId) + 1  FROM input.DEMChangeDetail\r\n\t   \r\n\tINSERT INTO [input].[DEMChangeDetail]\r\n        ([DEMChangeID]\r\n        ,[AgencyCode]\r\n        ,[BudgetGroupID]\r\n        ,[BillingPeriod]\r\n        ,[DistinctAccountCount]\r\n        ,[DemandUsage]\r\n        ,[EnergyUsage]\r\n        ,[EnergyUsagePeakPercentage]\r\n        ,[EnergyUsageReactivePower]\r\n        ,[StreetLightingFacilityPoints]\r\n        ,[DEMChangeDetailEntityId]\r\n\t\t,[Notes])\r\n     SELECT @id\r\n        ,AgencyCode\r\n        ,BudgetGroupID\r\n        , CASE\r\n            WHEN 6 < BillingPeriod % 100 THEN\r\n        (@fy - 1) * 100 + BillingPeriod % 100\r\n            ELSE\r\n                @fy * 100 + BillingPeriod % 100\r\n        END AS BillingPeriod\r\n        ,DistinctAccountCount\r\n        ,DemandUsage\r\n        ,EnergyUsage\r\n        ,EnergyUsagePeakPercentage\r\n        ,EnergyUsageReactivePower\r\n        ,StreetLightingFacilityPoints\r\n        ,@DEMChangeDetailEntityIdMaxPlusOne\r\n\t\t,Notes\r\n    FROM input.DEMChangeDetail\r\n\tWHERE  DEMChangeDetailEntityId = @DId\r\n\r\n\tDELETE #TEMP_Detail_ID WHERE DEMChangeDetailEntityId = @DId\r\n\tEND\r\n\r\n\tSELECT [AgencyCode],\r\n        [BudgetGroupID]\r\n    FROM #TEMP_Obsolete;\r\n\t\r\n\tDROP TABLE IF EXISTS #TEMP_Obsolete, #TEMP_Detail_ID;\r\nEND;\r"
        }
      ]
    }
