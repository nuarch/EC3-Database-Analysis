    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_AgencyChangeClone]\n(\r\n    @fy AS INT,\r\n    @name AS VARCHAR(250),\r\n    @description AS VARCHAR(500),\r\n    @actionBy AS VARCHAR(25) = 'SYSTEM',\r\n    @id AS INT OUTPUT,\r\n    @originalId AS INT\r\n)\r\nAS\r\nBEGIN\r\n    SET NOCOUNT ON;\r\n\r\n    DECLARE @IdOutput TABLE (ID INT);\r\n    DECLARE @DId AS INT, @AgencyChangeDetailEntityIdMaxPlusOne AS INT;\r\n\r\n    -- insert into agencyChange table \r\n    INSERT INTO input.AgencyChange (FY,\r\n        [Name],\r\n        [Description],\r\n        CreatedBy,\r\n        ModifiedBy)\r\n    OUTPUT INSERTED.ID\r\n    INTO @IdOutput\r\n    VALUES (@fy, @name, @description, @actionBy, @actionBy);\r\n\r\n    -- get new agencyChangeId\r\n    SELECT @id = ID FROM @IdOutput;\r\n\r\n    -- get all old AgencyChangeDetailEntityId that are associated with the old AgencyChangeId\r\n    SELECT DISTINCT AgencyChangeDetailEntityId\r\n    INTO #TEMP_Detail_ID\r\n    FROM input.AgencyChangeDetail\r\n    WHERE AgencyChangeID = @originalId;\r\n\r\n    -- persist the excluded items for return -- Mei 6/30/2017\r\n    SELECT DISTINCT AgencyCode, BudgetGroupID, AgencyChangeDetailEntityId\r\n    INTO #TEMP_Obsolete\r\n    FROM input.AgencyChangeDetail\r\n    WHERE AgencyChangeDetailEntityId IN (SELECT AgencyChangeDetailEntityId FROM #TEMP_Detail_ID)\r\n        AND (AgencyCode NOT IN (SELECT a.AgencyCodeOEC\r\n                FROM Preload.Agency AS a\r\n                    INNER JOIN Preload.AgencyByFY AS abf ON a.AgencyId = abf.AgencyId\r\n                WHERE abf.FY = @fy)\r\n            OR BudgetGroupID NOT IN (SELECT BudgetGroupID FROM Preload.BudgetGroupByFY WHERE FY = @fy));\r\n\r\n    -- purge #TEMP_Detail_ID to exclude entries with obsolete agencies and budget groups\r\n    DELETE #TEMP_Detail_ID\r\n    WHERE AgencyChangeDetailEntityId IN (SELECT AgencyChangeDetailEntityId FROM #TEMP_Obsolete);\r\n\r\n    -- insert into the agencychangedetail with the new agencyChangeId and new agencyChangeDetailEntityId \r\n    WHILE ((SELECT COUNT(*) FROM #TEMP_Detail_ID) > 0)\r\n    BEGIN\r\n        SELECT TOP 1 @DId = AgencyChangeDetailEntityId FROM #TEMP_Detail_ID;\r\n\r\n        SELECT @AgencyChangeDetailEntityIdMaxPlusOne = MAX(AgencyChangeDetailEntityId) + 1\r\n        FROM input.AgencyChangeDetail;\r\n\r\n        INSERT INTO input.AgencyChangeDetail(AgencyChangeID,\r\n            AgencyCode,\r\n            FacilityNo,\r\n            FacilityName,\r\n            BudgetGroupID,\r\n            BillingPeriod,\r\n            IsNewAddition,\r\n            EntireYear,\r\n            Notes,\r\n            DemandUsage,\r\n            EnergyUsage,\r\n            EnergyUsagePeakPercentage,\r\n            EnergyUsageReactivePower,\r\n            StreetLightingFacilityPoints,\r\n            AgencyChangeDetailEntityId)\r\n        SELECT @id,\r\n            AgencyCode,\r\n            FacilityNo,\r\n            FacilityName,\r\n            BudgetGroupID,\r\n            CASE WHEN 6 < BillingPeriod % 100 THEN (@fy - 1) * 100 + BillingPeriod % 100\r\n                ELSE @fy * 100 + BillingPeriod % 100 END AS BillingPeriod,\r\n            IsNewAddition,\r\n            EntireYear,\r\n            Notes,\r\n            DemandUsage,\r\n            EnergyUsage,\r\n            EnergyUsagePeakPercentage,\r\n            EnergyUsageReactivePower,\r\n            StreetLightingFacilityPoints,\r\n            @AgencyChangeDetailEntityIdMaxPlusOne\r\n        FROM input.AgencyChangeDetail\r\n        WHERE AgencyChangeDetailEntityId = @DId;\r\n\r\n        DELETE #TEMP_Detail_ID WHERE AgencyChangeDetailEntityId = @DId;\r\n    END;\r\n\r\n    SELECT AgencyCode, BudgetGroupID FROM #TEMP_Obsolete;\r\n\t\r\n\tDROP TABLE IF EXISTS #TEMP_Obsolete;\r\n    DROP TABLE IF EXISTS #TEMP_Detail_ID;\r\nEND;\r"
        }
      ]
    }
  ]
}