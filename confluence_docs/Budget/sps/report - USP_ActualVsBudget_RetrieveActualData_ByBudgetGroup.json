      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [report].[USP_ActualVsBudget_RetrieveActualData_ByBudgetGroup]\n(\r\n\t@startBillingPeriod AS VARCHAR(6)\r\n\t,@endBillingPeriod AS VARCHAR(6)\r\n\t,@IsProjection AS BIT = 0\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @fy AS INT, @notImplTNCReportIDs AS input.UDT_IDList, @notShowUsageReportIDs AS input.UDT_IDList, @ShowNonNYPAEnergyUsage_CutOffDate AS VARCHAR(6) = '202107';\r\n\t\r\n\tDECLARE @ActualByBudgetGroup TABLE (BudgetGroupID INT\r\n\t\t,ReportDataTypeId INT\r\n\t\t,ActualDollar MONEY\r\n\t\t,ActualUsage DECIMAL(20, 2)\r\n\t\t,ActualRate MONEY\r\n\t\t,ActualAdjustmentSurplus MONEY\r\n\t\t,LastMonthActualDollar MONEY);\r\n\r\n\tIF CAST(RIGHT(@endBillingPeriod, 2) AS INT) < 7\r\n\t\tSET @fy = CAST(LEFT(@endBillingPeriod, 4) AS INT)\r\n\tELSE\t\r\n\t\tSET @fy = CAST(LEFT(@endBillingPeriod, 4) AS INT) + 1\r\n\r\n\tINSERT INTO @notImplTNCReportIDs (ID) SELECT ReportDataTypeId FROM report.ReportDataType WHERE TypeLevel3 LIKE 'Reactive P%';\r\n\t\r\n\tIF @startBillingPeriod < @ShowNonNYPAEnergyUsage_CutOffDate\r\n\t\tINSERT INTO @notShowUsageReportIDs (ID) SELECT ReportDataTypeId FROM report.ReportDataType WHERE TypeLevel2 LIKE 'Non-NYPA E%';\r\n\r\n\tINSERT INTO @ActualByBudgetGroup (BudgetGroupID, ReportDataTypeId, ActualDollar, ActualUsage, ActualRate, ActualAdjustmentSurplus, LastMonthActualDollar)\r\n\tSELECT BudgetGroupID, ReportDataTypeId, ActualDollar, ActualUsage, ActualRate, AdjustmentSurplus, LastMonthActualDollar\r\n\tFROM dbo.UDF_ActualVsBudget_GetActual_ByBudgetGroup (@startBillingPeriod, @endBillingPeriod);\r\n\r\n\tSELECT a.BudgetGroupID\r\n\t\t,b.BudgetGroupName\r\n\t\t,@startBillingPeriod AS StartBillingPeriod\r\n\t\t,@endBillingPeriod AS EndBillingPeriod\r\n\t\t,a.ReportDataTypeId\r\n\t\t,@IsProjection AS IsProjection\r\n\t\t,ISNULL(a.ActualDollar, 0) AS ActualDollar\r\n\t\t,CASE WHEN a.ReportDataTypeId IN (SELECT ID FROM @notImplTNCReportIDs) THEN ISNULL(a.ActualUsage, 0)\r\n\t\t\tWHEN a.ReportDataTypeId IN (SELECT ID FROM @notShowUsageReportIDs) THEN 0\r\n\t\t\tELSE ISNULL(a.ActualUsage, 0) END AS ActualUsage\r\n\t\t,CASE WHEN a.ReportDataTypeId IN (SELECT ID FROM @notImplTNCReportIDs) THEN\r\n\t\t\tCASE WHEN ISNULL(a.ActualUsage, 0) <> 0 THEN ISNULL(a.ActualDollar, 0)/ISNULL(a.ActualUsage, 0)\r\n\t\t\t\tELSE 0 END \r\n\t\t\tWHEN a.ReportDataTypeId IN (SELECT ID FROM @notShowUsageReportIDs) THEN 0\r\n\t\t\tELSE\r\n\t\t\t\tCASE WHEN ISNULL(a.ActualUsage, 0) <> 0 THEN (ISNULL(a.ActualDollar, 0) + ISNULL(a.ActualAdjustmentSurplus, 0)) / ISNULL(a.ActualUsage, 0)\r\n\t\t\t\tELSE 0 END \r\n\t\t\tEND AS ActualRate\r\n\t\t,ISNULL(a.ActualAdjustmentSurplus, 0) AS ActualAdjustmentSurplus\r\n\t\t,0 AS ActualTenantDollar\r\n\t\t,0 AS ActualTenantUsage\r\n\t\t,ISNULL(a.LastMonthActualDollar, 0) AS LastMonthActualDollar\r\n\tFROM @ActualByBudgetGroup a\r\n\t\tINNER JOIN preload.BudgetGroup b ON a.BudgetGroupID = b.BudgetGroupID\r\n\t\tINNER JOIN preload.BudgetGroupByFY c ON b.BudgetGroupID = c.BudgetGroupID\r\n\tWHERE c.FY = @fy;\r\nEND;\r"
        }
      ]
    }
  ]
}