      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_RateFactorTypesRetrieve]\n\t@code AS VARCHAR(25) = null\r\n\t,@type AS CHAR(1) = null\r\n\tWITH EXEC AS CALLER\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSET @type = ISNULL(NULLIF(@type, ''), 'B');\r\n\r\n\tSELECT RateFactorCode, RateFactorDescription, IsMultiplier, DefaultValue, Active\r\n\tFROM input.RateFactorType\r\n\tWHERE 1 = CASE WHEN ISNULL(@code, '') <> '' THEN CASE WHEN RateFactorCode = @code THEN 1 ELSE 0 END ELSE 1 END\r\n\t\tAND 1 = CASE WHEN @type = 'A' AND IsMultiplier = 0 THEN 1 WHEN @type = 'M' AND IsMultiplier = 1 THEN 1 ELSE 1 END\r\n\t\tAND Active = 1\r\n\tORDER BY RateFactorDescription ASC;\r\nEND\r"
        }
      ]
