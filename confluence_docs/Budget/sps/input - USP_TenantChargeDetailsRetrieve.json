    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " isolation level can lead to dirty reads, where uncommitted changes from other transactions are visible, potentially causing data inconsistencies."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The use of "
                },
                {
                  "type": "text",
                  "text": "ISNULL",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " in the WHERE clause can hinder index utilization, impacting query performance, especially on large datasets."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "If the view "
                },
                {
                  "type": "text",
                  "text": "vwTenantChargeDetails",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is complex or involves multiple joins, it could affect performance and should be reviewed for optimization."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The procedure does not handle exceptions or errors, which could lead to unhandled failures if issues arise during execution."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_TenantChargeDetailsRetrieve]\n\t@id AS INT = NULL\r\n\t,@tenantChargeId AS INT = NULL\r\n\t,@budgetGroupId AS INT = NULL\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSET @id = NULLIF(ISNULL(@id, 0), 0);\r\n\tSET @tenantChargeId = NULLIF(ISNULL(@tenantChargeId, 0), 0);\r\n\tSET @budgetGroupId = NULLIF(ISNULL(@budgetGroupId, 0), 0);\r\n\r\n\tSELECT DetailID, TenantChargeID, AgencyCode, BudgetGroupID, ChargePercentage\r\n\tFROM input.vwTenantChargeDetails\r\n\tWHERE DetailID = ISNULL(@id, DetailID)\r\n\t\tAND TenantChargeID = ISNULL(@tenantChargeId, TenantChargeID)\r\n\t\tAND BudgetGroupID = ISNULL(@budgetGroupId, BudgetGroupID)\r\n\tORDER BY BudgetGroupID ASC, AgencyCode ASC;\r\nEND\r"
        }
      ]
    }
