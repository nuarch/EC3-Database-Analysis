    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_AgencyChangeDelete]\n(\r\n\t@id AS INT\r\n\t,@actionBy AS VARCHAR(25) = 'SYSTEM'\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDELETE FROM input.AgencyChangeDetail WHERE AgencyChangeID = @id;\r\n\tDELETE FROM input.AgencyChange WHERE ID = @id;\r\nEND;\r"
        }
      ]
