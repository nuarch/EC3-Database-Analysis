    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_TenantChargeClone]\n\t@fy AS INT\r\n\t,@name AS VARCHAR(250)\r\n\t,@description AS VARCHAR(500)\r\n\t,@actionBy AS VARCHAR(25) = 'SYSTEM'\r\n\t,@id AS INT OUTPUT\r\n\t,@originalId AS INT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @IdOutput TABLE (ID INT);\r\n\r\n\tINSERT INTO input.TenantCharge (FY, [Name], [Description], CreatedBy, ModifiedBy)\r\n    OUTPUT INSERTED.ID INTO @IdOutput\r\n    VALUES (@fy, @name, @description, @actionBy, @actionBy);\r\n\r\n\tSELECT @id = ID FROM @IdOutput;\r\n\r\n\t-----------------------------------------------------------------------------------------------------\r\n    -- get all old DetailID that are associated with the old TenantChargeID\r\n    SELECT [DetailID]\r\n    INTO #TEMP_Detail_ID\r\n    FROM [input].TenantChargeDetail\r\n    WHERE TenantChargeID = @originalId;\r\n\r\n    -- persist the excluded items for return -- Mei 6/30/2017\r\n    SELECT DISTINCT [DetailID], [AgencyCode], [BudgetGroupID]\r\n    INTO #TEMP_Obsolete\r\n    FROM input.TenantChargeDetail\r\n    WHERE [DetailID] IN (SELECT [DetailID] FROM #TEMP_Detail_ID)\r\n          AND\r\n          (\r\n              [AgencyCode] NOT IN\r\n              (\r\n                  SELECT a.AgencyCodeOEC\r\n                  FROM Preload.Agency a INNER JOIN [Preload].[AgencyByFY] abf ON a.AgencyID = abf.AgencyID\r\n                  WHERE abf.FY = @fy\r\n              )\r\n              OR [BudgetGroupID] NOT IN\r\n                 (\r\n                     SELECT [BudgetGroupID] FROM [Preload].[BudgetGroupByFY] WHERE FY = @fy\r\n                 )\r\n          );\r\n\r\n    -- purge #TEMP_Detail_ID to exclude entries with obsolete agencies and budget groups\r\n    DELETE #TEMP_Detail_ID\r\n    WHERE [DetailID] IN (SELECT [DetailID] FROM #TEMP_Obsolete);\r\n    -----------------------------------------------------------------------------------------------------\r\n\r\n\tINSERT INTO input.TenantChargeDetail (TenantChargeID, AgencyCode, BudgetGroupID, ChargePercentage)\r\n\tSELECT @id, AgencyCode, BudgetGroupID, ChargePercentage\r\n\tFROM input.TenantChargeDetail\r\n\tWHERE TenantChargeID = @originalId\r\n\t\t  AND [DetailID] IN (SELECT [DetailID] FROM #TEMP_Detail_ID);\r\n\r\n    SELECT [AgencyCode], [BudgetGroupID]\r\n    FROM #TEMP_Obsolete;\r\n\r\n    DROP TABLE #TEMP_Obsolete, #TEMP_Detail_ID;\r\nEND\r"
        }
      ]
    }
