          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_ProjectChangeDetailUpdate]\n\t@id AS INT\r\n\t,@demProjectId AS VARCHAR(25)\r\n\t,@agencyCode AS VARCHAR(6)\r\n\t,@facilityNo AS VARCHAR(10) = NULL\r\n\t,@facilityName AS VARCHAR(100) = NULL\r\n\t,@facilityAddress AS VARCHAR(100) = NULL\r\n\t,@accountNo AS VARCHAR(26) = NULL\r\n\t,@budgetGroupId AS INT\r\n\t,@effectiveDate AS DATETIME\r\n\t,@demandSavings AS DECIMAL(18, 5) = 0.0\r\n\t,@energySavings\tAS DECIMAL(18, 5) = 0.0\r\n\t,@energySavingsPeakPercentage AS DECIMAL(10, 5) = 1.0\r\n\t,@actionBy AS VARCHAR(25) = 'SYSTEM'\r\n\t,@programTypeId\tAS INT\r\n\t,@patternTypeId\tAS INT\r\n\t,@notes AS VARCHAR(500) = NULL\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @facilityNo = NULLIF(ISNULL(@facilityNo, ''), '');\r\n\tSET @facilityName = NULLIF(ISNULL(@facilityName, ''), '');\r\n\tSET @facilityAddress = NULLIF(ISNULL(@facilityAddress, ''), '');\r\n\tSET @accountNo = NULLIF(ISNULL(@accountNo, ''), '');\r\n\tSET @notes = NULLIF(ISNULL(@notes, ''), '');\r\n\r\n\tDECLARE @projectChangeId AS INT;\r\n\tSELECT @projectChangeId=MAX(ProjectChangeID)\r\n\tFROM input.ProjectChangeDetail\r\n\tWHERE DetailID = @id;\r\n\r\n\tUPDATE input.ProjectChangeDetail\r\n\tSET DEMProjectID = @demProjectID\r\n\t\t,AgencyCode\t= @agencyCode\r\n\t\t,FacilityNo\t= @facilityNo\r\n\t\t,FacilityName = @facilityName\r\n\t\t,FacilityAddress = @facilityAddress\r\n\t\t,AccountNo = @accountNo\r\n\t\t,BudgetGroupID = @budgetGroupId\r\n\t\t,EffectiveDate = @effectiveDate\r\n\t\t,DemandSavings = @demandSavings\r\n\t\t,EnergySavings = @energySavings\r\n\t\t,EnergySavingsPeakPercentage = @energySavingsPeakPercentage\r\n\t\t,ProgramTypeID = @programTypeId\r\n\t\t,PatternTypeID = @patternTypeId\r\n\t\t,Notes = @notes\r\n\t\t,ModifiedBy = @actionBy\r\n\t\t,ModifiedDate = GETDATE()\r\n\tWHERE DetailID = @id;\r\n\r\n--------------------------------------------------------------------\r\n-- Update ProjectChange and Scenario status if possible\r\n--------------------------------------------------------------------\r\n\t\r\n\tUPDATE input.ProjectChange\r\n\tSET ModifiedDate = GETDATE(), ModifiedBy = @actionBy\r\n\tWHERE id = @projectChangeId;\r\n\r\n\tUPDATE budget.Scenario\r\n\tSET [Status] = 'REQUIRERECALC'\r\n\tWHERE ProjectChangeID = @projectChangeId AND [Status] IN ('CALCULATED');\r\nEND\r"
        }
