      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_AgencyChangeDetailsRetrieveBillingPeriodDetails]\n(\r\n     @AgencyChangeDetailEntityId AS INT = -1\r\n\t ,@agencyChangeId AS INT = -1\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tIF (@AgencyChangeDetailEntityId > -1)\r\n\tBEGIN\r\n\t\tSELECT AgencyChangeDetailEntityID, \r\n\t\t    AgencyChangeID, \r\n\t\t\tAgencyCode, \r\n\t\t\tFacilityNo, \r\n\t\t\tFacilityName,\r\n\t\t\tBudgetGroupID, \r\n\t\t\tIsNewAddition, \r\n\t\t\tEntireyear,\r\n\t\t\tActionDescription,\r\n\t\t\tAddRemoveFacility,\r\n\t\t\tEffectiveDate,\r\n\t        Notes, \r\n\t\t\tBillingPeriod, \r\n\t\t\tDemandUsage, \r\n\t\t\tEnergyUsage, \r\n\t\t\tEnergyUsagePeakPercentage, \r\n\t\t\tEnergyUsagePeak, \r\n\t\t\tEnergyUsageOffPeak, \r\n\t\t\tEnergyUsageReactivePower, \r\n\t\t\tStreetLightingFacilityPoints\r\n\t\tFROM input.vwAgencyChangeDetails\r\n\t\tWHERE AgencyChangeDetailEntityID = @AgencyChangeDetailEntityId\r\n\t\tORDER BY AgencyCode ASC, FacilityNo ASC, BudgetGroupID ASC, BillingPeriod ASC;\r\n\tEND;\r\n\tELSE IF  (@agencyChangeId > -1)\r\n\tBEGIN\r\n\t\tSELECT AgencyChangeDetailEntityID, \r\n\t\t    AgencyChangeID, \r\n\t\t\tAgencyCode, \r\n\t\t\tFacilityNo, \r\n\t\t\tFacilityName,\r\n\t\t\tBudgetGroupID, \r\n\t\t\tIsNewAddition,\r\n\t\t\tEntireyear,\r\n\t\t\tActionDescription,\r\n\t\t\tAddRemoveFacility,\r\n\t\t\tEffectiveDate,\r\n\t        Notes, \r\n\t\t\tBillingPeriod, \r\n\t\t\tDemandUsage, \r\n\t\t\tEnergyUsage, \r\n\t\t\tEnergyUsagePeakPercentage, \r\n\t\t\tEnergyUsagePeak, \r\n\t\t\tEnergyUsageOffPeak, \r\n\t\t\tEnergyUsageReactivePower, \r\n\t\t\tStreetLightingFacilityPoints\r\n\t\tFROM input.vwAgencyChangeDetails\r\n\t\tWHERE  AgencyChangeID = @agencyChangeId\r\n\t\tORDER BY AgencyChangeDetailEntityID ASC, AgencyCode ASC, FacilityNo ASC, BudgetGroupID ASC, BillingPeriod ASC;\r\n\tEND;\r\n\tELSE\r\n\tBEGIN\r\n\t    SELECT AgencyChangeDetailEntityID, \r\n\t\t    AgencyChangeID, \r\n\t\t\tAgencyCode, \r\n\t\t\tFacilityNo, \r\n\t\t\tFacilityName,\r\n\t\t\tBudgetGroupID, \r\n\t\t\tIsNewAddition, \r\n\t\t\tEntireyear,\r\n\t\t\tActionDescription,\r\n\t\t\tAddRemoveFacility,\r\n\t\t\tEffectiveDate,\r\n\t        Notes, \r\n\t\t\tBillingPeriod, \r\n\t\t\tDemandUsage, \r\n\t\t\tEnergyUsage, \r\n\t\t\tEnergyUsagePeakPercentage, \r\n\t\t\tEnergyUsagePeak, \r\n\t\t\tEnergyUsageOffPeak, \r\n\t\t\tEnergyUsageReactivePower, \r\n\t\t\tStreetLightingFacilityPoints\r\n\t\tFROM input.vwAgencyChangeDetails\r\n\t\tORDER BY AgencyChangeID ASC, AgencyChangeDetailEntityID ASC, AgencyCode ASC, FacilityNo ASC, BudgetGroupID ASC, BillingPeriod ASC;\r\n\tEND;\r\nEND;\r"
        }
      ]
    }
  ]
