    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_BaseRateDetailsRetrieve]\n\t@baseRateId\t\t\t\tINT = NULL\r\n\t,@budgetGroupId\t\tINT = NULL\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSELECT BaseRateID, BudgetGroupID\r\n\tFROM input.vwBaseRateDetailsDistinct\r\n\tWHERE BaseRateID\t= ISNULL(@baseRateId, BaseRateID)\r\n\t\tAND BudgetGroupID\t= ISNULL(@budgetGroupId, BudgetGroupID)\r\n\tORDER BY BaseRateID ASC, BudgetGroupID ASC;\r\nEND;\r"
        }
      ]
