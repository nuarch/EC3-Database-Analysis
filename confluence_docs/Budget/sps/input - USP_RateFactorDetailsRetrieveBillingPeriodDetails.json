    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_RateFactorDetailsRetrieveBillingPeriodDetails]\n\t@rateFactorId AS INT = NULL\r\n\t,@budgetGroupId AS INT = NULL\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT RateFactorID\r\n\t\t,a.BudgetGroupID\r\n\t\t,BillingPeriod\r\n\t\t,CommodityAdder\r\n\t\t,CommodityMultiplier\r\n\t\t,DeliveryMultiplier\r\n\t\t,GRTMultiplier\r\n\t\t,DemandAdjustmentMultiplier\r\n\t\t,b.BudgetGroupName\r\n\tFROM input.vwRateFactorDetails a JOIN preload.BudgetGroup b ON a.BudgetGroupID=b.BudgetGroupID\r\n\tWHERE RateFactorID = ISNULL(@rateFactorId, RateFactorID)\r\n\t\tAND a.BudgetGroupID = ISNULL(@budgetGroupId, a.BudgetGroupID)\r\n\tORDER BY RateFactorID ASC, a.BudgetGroupID ASC, BillingPeriod ASC;\r\nEND\r"
        }
      ]
    }
