    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "4. Business Logic and Workflow"
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Retrieve Current Values",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure starts by selecting the current values of specific fields for the scenario identified by "
                },
                {
                  "type": "text",
                  "text": "@id",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Update Scenario",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": It updates the scenario with new values provided through the input parameters."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Conditional Status Update",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "If related records exist in "
                        },
                        {
                          "type": "text",
                          "text": "budget.StagingBudget",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", it checks if any of the change identifiers have been modified. If so, it sets the status to 'REQUIRERECALC'."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It updates the status based on the "
                        },
                        {
                          "type": "text",
                          "text": "@isCurrentProjection",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and "
                        },
                        {
                          "type": "text",
                          "text": "@isCurrentBudget",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " flags, handling various combinations to set appropriate status values such as 'CRNT BDGT', 'CRNT PROJ', 'CRNT PROJ And CRNT BDGT', 'PREV PROJ And PREV BDGT', 'PREV BDGT', and 'PREV PROJ'."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Indexing",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Ensure that the "
                },
                {
                  "type": "text",
                  "text": "ID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " column in the "
                },
                {
                  "type": "text",
                  "text": "budget.Scenario",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table is indexed to optimize the SELECT and UPDATE operations."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Conditional Logic",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The multiple conditional checks can impact performance, especially if the table contains a large number of records."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Concurrency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Consider the impact of concurrent updates to the same scenario, which could lead to race conditions or deadlocks."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Integrity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": If the procedure is called with incorrect or incomplete parameters, it could lead to inconsistent data states."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Concurrency Issues",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Without proper transaction handling, concurrent updates might lead to data anomalies."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Error Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure lacks explicit error handling, which could result in unhandled exceptions and incomplete updates."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Status Logic Complexity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The logic for updating the status is complex and could lead to incorrect status updates if not thoroughly tested."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Null Values",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of NULL default values for several parameters requires careful handling to avoid unintended updates."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [budget].[USP_ScenarioUpdate]\n(\r\n\t@id AS INT\r\n\t,@fy AS INT\r\n\t,@name AS VARCHAR(250)\r\n\t,@description AS VARCHAR(500)\r\n\t,@baseYear AS INT\r\n\t,@publishedBillingPeriod AS INT\r\n\t,@agencyChangeId AS INT = NULL\r\n\t,@demChangeId AS INT = NULL\r\n\t,@projectChangeId AS INT = NULL\r\n\t,@tenantChargeId AS INT = NULL\r\n\t,@baseRateId AS INT = NULL\r\n\t,@rateFactorId AS INT = NULL\r\n\t,@status AS VARCHAR(25)\r\n\t,@active AS BIT\r\n\t,@actionBy AS VARCHAR(25) = 'SYSTEM'\r\n\t,@isCurrentProjection AS BIT = 0\r\n\t,@isCurrentBudget AS BIT = 0\r\n\t,@actualDataEndMonth AS INT = 0\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @oldAgencyChangeId AS INT, @oldDemChangeId AS INT, @oldProjectChangeId AS INT\r\n\t\t,@oldTenantChargeId AS INT, @oldBaseRateId AS INT, @oldRateFactorId AS INT, @oldIsCurrentProjection AS BIT, @oldIsCurrentBudget AS BIT;\r\n\r\n\tSELECT @oldAgencyChangeId = AgencyChangeID\r\n\t\t,@oldDemChangeId = DEMChangeID\r\n\t\t,@oldProjectChangeId = ProjectChangeID\r\n\t\t,@oldTenantChargeId = TenantChargeID\r\n\t\t,@oldBaseRateId = BaseRateID\r\n\t\t,@oldRateFactorId = RateFactorID\r\n\t\t,@oldIsCurrentProjection = IsCurrentProjection\r\n\t\t,@oldIsCurrentBudget = IsCurrentBudget\r\n\tFROM Budget.Scenario\r\n\tWHERE ID = @id;\r\n\r\n\tUPDATE budget.Scenario\r\n\tSET [Name] = @name\r\n\t\t,[Description] = @description\r\n\t\t,FY = @fy\r\n\t\t,BaseYear = @baseYear\r\n\t\t,PublishedBillingPeriod\t= @publishedBillingPeriod\r\n\t\t,AgencyChangeID = @agencyChangeId\r\n\t\t,DEMChangeID = @demChangeId\r\n\t\t,ProjectChangeID = @projectChangeId\r\n\t\t,TenantChargeID = @tenantChargeId\r\n\t\t,BaseRateID = @baseRateId\r\n\t\t,RateFactorID = @rateFactorId\r\n\t\t,[Status] = @status\r\n\t\t,Active = @active\r\n\t\t,ModifiedBy = @actionBy\r\n\t\t,ModifiedDate = GETDATE()\r\n\t\t,IsCurrentProjection = @isCurrentProjection\r\n\t\t,IsCurrentBudget = @isCurrentBudget\r\n\t\t,ActualDataEndMonth = @actualDataEndMonth\r\n\tWHERE ID = @id;\r\n\r\n\tIF EXISTS(SELECT 1 FROM budget.StagingBudget WHERE ScenarioId = @id)\r\n\tBEGIN\r\n\t\tIF (@oldAgencyChangeId <> @agencyChangeId OR\r\n\t\t\t@oldDemChangeId <> @demChangeId OR\r\n\t\t\t@oldProjectChangeId <> @projectChangeId OR\r\n\t\t\t@oldTenantChargeId <> @tenantChargeId OR\r\n\t\t\t@oldBaseRateId <> @baseRateId OR\r\n\t\t\t@oldRateFactorId <> @rateFactorId)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = 'REQUIRERECALC'\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@isCurrentBudget = 1 AND @isCurrentProjection = 0)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = 'CRNT BDGT'\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@isCurrentProjection = 1 AND @isCurrentBudget = 0)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = 'CRNT PROJ'\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@isCurrentBudget = 1 AND @isCurrentProjection = 1)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = 'CRNT PROJ And CRNT BDGT'\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@oldIsCurrentBudget = 1 AND @oldIsCurrentProjection = 1 AND @isCurrentBudget = 0 AND @isCurrentProjection = 0)\r\n\t\tBEGIN \r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = 'PREV PROJ And PREV BDGT'\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@oldIsCurrentBudget = 1 AND @isCurrentBudget = 0)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = REPLACE([Status], 'CRNT BDGT', 'PREV BDGT')\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\t\tELSE IF (@oldIsCurrentProjection = 1 AND @isCurrentProjection = 0)\r\n\t\tBEGIN\r\n\t\t\tUPDATE budget.Scenario\r\n\t\t\tSET [Status] = REPLACE([Status], 'CRNT PROJ', 'PREV PROJ')\r\n\t\t\tWHERE ID = @id;\r\n\t\tEND;\r\n\tEND;\r\nEND;\r"
        }
      ]
    }
  ]
