    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [input].[USP_RateFactorClone]\n\t@fy AS INT\r\n\t,@name AS VARCHAR(250)\r\n\t,@description AS VARCHAR(500)\r\n\t,@actionBy AS VARCHAR(25) = 'SYSTEM'\r\n\t,@id AS INT OUTPUT\r\n\t,@originalId AS INT\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tDECLARE @IdOutput TABLE (ID INT);\r\n\r\n\tINSERT INTO input.RateFactor (FY, [Name], [Description], CreatedBy, ModifiedBy)\r\n    OUTPUT INSERTED.ID INTO @IdOutput\r\n    VALUES (@fy, @name, @description, @actionBy, @actionBy);\r\n\r\n\tSELECT @id = ID FROM @IdOutput;\r\n\r\n    -----------------------------------------------------------------------------------------------------\r\n    -- get all old [BudgetGroupID] that are associated with the old RateFactorID\r\n    SELECT DISTINCT [BudgetGroupID]\r\n    INTO #TEMP_Detail_ID\r\n    FROM [input].[RateFactorDetail]\r\n    WHERE [RateFactorID] = @originalId;\r\n\r\n    -- persist the excluded items for return -- Mei 6/30/2017\r\n    SELECT DISTINCT '' AS [AgencyCode], [BudgetGroupID]\r\n    INTO #TEMP_Obsolete\r\n    FROM input.RateFactorDetail\r\n    WHERE [BudgetGroupID] IN\r\n          (\r\n              SELECT [BudgetGroupID] FROM #TEMP_Detail_ID\r\n          )\r\n          AND [BudgetGroupID] NOT IN\r\n              (\r\n                  SELECT [BudgetGroupID] FROM [Preload].[BudgetGroupByFY] WHERE FY = @fy\r\n              );\r\n\r\n    -- purge #TEMP_Detail_ID to exclude entries with obsolete agencies and budget groups\r\n    DELETE #TEMP_Detail_ID\r\n    WHERE [BudgetGroupID] IN (SELECT [BudgetGroupID] FROM #TEMP_Obsolete);\r\n\r\n    -----------------------------------------------------------------------------------------------------\r\n\r\n\tINSERT INTO input.RateFactorDetail (RateFactorID, BudgetGroupID, BillingPeriod\r\n        ,CommodityAdder, CommodityMultiplier, DeliveryMultiplier, GRTMultiplier, DemandAdjustmentMultiplier)\r\n\tSELECT @id, BudgetGroupID\r\n        ,CASE WHEN 6 < BillingPeriod % 100 THEN (@fy - 1) * 100 + BillingPeriod % 100 ELSE @fy * 100 + BillingPeriod % 100 END AS BillingPeriod\r\n        ,CommodityAdder, CommodityMultiplier, DeliveryMultiplier, GRTMultiplier, DemandAdjustmentMultiplier\r\n\tFROM input.RateFactorDetail\r\n\tWHERE RateFactorID=@originalId AND BudgetGroupID IN (SELECT BudgetGroupID FROM #TEMP_Detail_ID);\r\n\r\n    SELECT [AgencyCode], [BudgetGroupID]\r\n    FROM #TEMP_Obsolete;\r\n\r\n    DROP TABLE #TEMP_Obsolete, #TEMP_Detail_ID; \r\nEND\r"
        }
      ]
    }
  ]
}