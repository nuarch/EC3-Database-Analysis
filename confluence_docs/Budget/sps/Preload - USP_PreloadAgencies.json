    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Preload].[USP_PreloadAgencies]\n(\r\n\t@fy AS INT = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\r\n\tSET @fy = ISNULL(@fy, DATEPART(yyyy, GETDATE()) + 1);\r\n\r\n\t-- if the agency list is populated already for the given FY, do not do it again\r\n\tIF (@fy = SOME(SELECT FY FROM Preload.AgencyByFY))\r\n\tBEGIN\r\n\t\tRETURN;\r\n\tEND;\r\n\r\n\tINSERT INTO Preload.Agency(AgencyId,\r\n\t\tAgencyCodeOEC,\r\n\t\tAgencyName,\r\n\t\tAgencyDescription,\r\n\t\tCreatedDate,\r\n\t\tCreatedBy,\r\n\t\tModifiedDate,\r\n\t\tModifiedBy,\r\n\t\tAgencyFundingSeqId,\r\n\t\tFundingSourceDescription)\r\n\tSELECT vw.AgencyId,\r\n\t\tvw.AgencyCodeOEC,\r\n\t\tvw.AgencyName,\r\n\t\tvw.AgencyDescription,\r\n\t\tGETDATE() AS CreatedDate,\r\n\t\t'dbo' AS CreatedBy,\r\n\t\tGETDATE() AS ModifiedDate,\r\n\t\t'dbo' AS ModifiedBy,\r\n\t\tafa.AgencyFundingSeqId,\r\n\t\tfund.FundingSourceDescription\r\n\tFROM EC3DB.BudgetApp_vwAgencies AS vw\r\n\t\tLEFT JOIN EC3DB.Billing_AgencyFundingAssignments AS afa ON vw.AgencyId = afa.AgencyDivisionSeqId\r\n\t\tLEFT JOIN EC3DB.Billing_AgencyFunding AS fund ON afa.AgencyFundingSeqId = fund.AgencyFundingSeqId\r\n\tWHERE vw.Active = 1 AND vw.AgencyId NOT IN (SELECT AgencyId FROM Preload.Agency);\r\n\t\r\n\tINSERT INTO Preload.AgencyByFY(FY, AgencyId)\r\n\tSELECT @fy, AgencyId\r\n\tFROM EC3DB.BudgetApp_vwAgencies\r\n\tWHERE Active = 1;\r\nEND;\r"
        }
      ]
    }
  ]
}