        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Indexes",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The clustered primary key index on "
                },
                {
                  "type": "text",
                  "text": "FacilityPropertySeqid",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " optimizes lookups by this key. However, there are no additional indexes, which impact performance for queries frequently filtering or joining on "
                },
                {
                  "type": "text",
                  "text": "FacilitySeqid",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " or "
