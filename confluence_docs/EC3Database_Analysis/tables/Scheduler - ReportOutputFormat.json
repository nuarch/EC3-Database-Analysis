                {
                  "type": "text",
                  "text": "nvarchar",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " with a larger length to accommodate longer names."
                }
              ]
            }
