    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "NOLOCK",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can result in reading uncommitted data, which may lead to inaccurate user counts if transactions are not yet completed."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Case Sensitivity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure uses "
                },
                {
                  "type": "text",
                  "text": "LOWER",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " to ensure case-insensitive comparison of "
                },
                {
                  "type": "text",
                  "text": "ApplicationName",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". This assumes that the database collation supports case-insensitive comparisons."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Time Zone Considerations",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure relies on "
                },
                {
                  "type": "text",
                  "text": "@CurrentTimeUtc",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " being accurately provided in UTC. Any discrepancies in time zone handling could affect the accuracy of the user count."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": As the number of users grows, the performance of the "
                },
                {
                  "type": "text",
                  "text": "COUNT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " operation may degrade if the database is not properly indexed or optimized."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [dbo].[aspnet_Membership_GetNumberOfUsersOnline]\n    @ApplicationName            nvarchar(256),\r\n    @MinutesSinceLastInActive   int,\r\n    @CurrentTimeUtc             datetime\r\nAS\r\nBEGIN\r\n    DECLARE @DateActive datetime\r\n    SELECT  @DateActive = DATEADD(minute,  -(@MinutesSinceLastInActive), @CurrentTimeUtc)\r\n\r\n    DECLARE @NumOnline int\r\n    SELECT  @NumOnline = COUNT(*)\r\n    FROM dbo.aspnet_Users u(NOLOCK),\r\n            dbo.aspnet_Applications a(NOLOCK),\r\n            dbo.aspnet_Membership m(NOLOCK)\r\n    WHERE   u.ApplicationId = a.ApplicationId                  AND\r\n            LastActivityDate > @DateActive                     AND\r\n            a.LoweredApplicationName = LOWER(@ApplicationName) AND\r\n            u.UserId = m.UserId\r\n    RETURN(@NumOnline)\r\nEND"
        }
      ]
    }
  ]
