      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[USP_AgencyDashboard_AnnualEnergyUsageRetrieveAgencyVsCitywideRatio]\n(\r\n\t@agencyCode AS VARCHAR(6)\r\n\t,@period AS INT = NULL\r\n\t,@debugMode AS BIT = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @usageTable TABLE (BillingPeriod INT\r\n\t\t,BillingPeriodDate\tDATETIME NULL\r\n\t\t,AgencyBTU DECIMAL(38, 6) DEFAULT(0.00)\r\n\t\t,CitywideBTU DECIMAL(38, 6) DEFAULT(0.00)\r\n\t\t,BTURatio DECIMAL(38, 6) DEFAULT(0.00)\r\n\t\t,AgencyBTURatio DECIMAL(38, 6) DEFAULT(0.00)\r\n\t\t,CitywideBTURatio DECIMAL(38, 6) DEFAULT(0.00));\r\n\r\n\t-- Set rolling 60 months if period is not specified\r\n\tSET @period = ISNULL(@period, 60);\r\n\r\n\t-- Get the agency hierarchy\r\n\tSELECT AgencyCodeOEC INTO #tmpAgencies FROM [Billing].[UDF_GetAgencyHierarchy](@agencyCode);\r\n\r\n\tDECLARE @publishedBillingPeriod VARCHAR(6), @currentFY INT;\r\n\tSELECT @publishedBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData WITH (NOLOCK);\r\n\tSELECT @currentFY = dbo.CalculateFiscalYear(@publishedBillingPeriod);\r\n\r\n\tDECLARE @startPeriod VARCHAR(6);\r\n\tSET @startPeriod = CONVERT(INT, @publishedBillingPeriod) - 500;\r\n\r\n\tIF RIGHT(@startPeriod, 2) = '12'\r\n\t\tSET @startPeriod = CONVERT(VARCHAR(4), CONVERT(INT, LEFT(@startPeriod, 4) + 1)) + '01';\r\n\tELSE\r\n\t\tSET @startPeriod = CONVERT(VARCHAR(6), CONVERT(INT, @startPeriod) + 1);\r\n\r\n\tDECLARE @periodCtr AS INT, @ctr AS INT;\r\n\tSET @periodCtr = CONVERT(INT, @startPeriod);\r\n\tSET @ctr = 0;\r\n\r\n\tWHILE @periodCtr <= CONVERT(INT, @publishedBillingPeriod) AND @ctr < @period\r\n\tBEGIN\r\n\t\tINSERT INTO @usageTable (BillingPeriod, BillingPeriodDate)\r\n\t\t\tVALUES (@periodCtr, DATEFROMPARTS(LEFT(CONVERT(VARCHAR(6), @periodCtr), 4), SUBSTRING(CONVERT(VARCHAR(6), @periodCtr), 5, 2), 1));\r\n\t\tIF @periodCtr % 100 = 12\r\n\t\t\tSET @periodCtr = CONVERT(INT, CONVERT(VARCHAR(4), CONVERT(INT, LEFT(@periodCtr, 4) + 1)) + '01');\r\n\t\tELSE\r\n\t\t\tSET @periodCtr = @periodCtr + 1\r\n\tEND;\r\n\t\r\n\tUPDATE T\r\n\tSET T.AgencyBTU = A.BTU\r\n\tFROM @usageTable T\r\n\tJOIN (\r\n\t\tSELECT ALSA.BillingPeriod, AVG(ALSA.BTU) AS BTU\r\n\t\tFROM [Published].[TemporalAccountLevelSummaryByAgency] AS ALSA WITH (NOLOCK)\r\n\t\tJOIN #tmpAgencies TA ON TA.AgencyCodeOEC = ALSA.AgencyCodeOEC\r\n\t\tWHERE (ALSA.EffectiveStartPeriod <= @PublishedBillingPeriod AND ALSA.EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\tAND ALSA.BillingPeriod BETWEEN @startPeriod AND @publishedBillingPeriod\r\n\t\tGROUP BY ALSA.BillingPeriod\r\n\t) A ON A.BillingPeriod = T.BillingPeriod;\r\n\t\r\n\tUPDATE T\r\n\tSET T.CitywideBTU = A.BTU\r\n\tFROM @usageTable T\r\n\tJOIN (\r\n\t\tSELECT ALSA.BillingPeriod, AVG(ALSA.BTU) AS BTU\r\n\t\tFROM [Published].[TemporalAccountLevelSummaryByCityWide] AS ALSA WITH (NOLOCK)\r\n\t\tWHERE (EffectiveStartPeriod <= @PublishedBillingPeriod AND EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\tGROUP BY ALSA.BillingPeriod\r\n\t) A ON A.BillingPeriod = T.BillingPeriod;\r\n\t\r\n\tUPDATE T\r\n\tSET T.BTURatio = ROUND(CASE WHEN T.CitywideBTU > 0.00 THEN T.AgencyBTU / T.CitywideBTU ELSE T.BTURatio END, 2)\r\n\tFROM @usageTable T;\r\n\t\r\n\tDECLARE @minPeriod INT, @minAgencyBTU DECIMAL(38, 6), @minCitywideBTU DECIMAL(38, 6);\r\n\tSELECT @minPeriod = BillingPeriod\r\n\t\t,@minAgencyBTU = AgencyBTU\r\n\t\t,@minCitywideBTU = CitywideBTU\r\n\tFROM @usageTable\r\n\tWHERE BillingPeriod = (SELECT MIN(BillingPeriod) FROM @usageTable);\r\n\t\r\n\tUPDATE T\r\n\tSET T.AgencyBTURatio = ROUND(CASE WHEN @minAgencyBTU > 0.00 THEN T.AgencyBTU / @minAgencyBTU ELSE 0.00 END, 2)\r\n\t\t,T.CitywideBTURatio = ROUND(CASE WHEN @minCitywideBTU > 0.00 THEN T.CitywideBTU / @minCitywideBTU ELSE 0.00 END, 2)\r\n\tFROM @usageTable T;\r\n\t\r\n\tSELECT @agencyCode AS AgencyCodeOEC\r\n\t\t,CONVERT(VARCHAR(20), BillingPeriod) AS FiscalYear\r\n\t\t,AgencyBTU, CitywideBTU, BTURatio, AgencyBTURatio, CitywideBTURatio\r\n\tFROM @usageTable\r\n\tORDER BY BillingPeriodDate;\r\nEND;"
        }
      ]
    }
  ]
}