    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [ManualBill].[usp_GetTariffRatesByUtilityCompanyEnergySourceAndIsTOD]\n(\r\n\t@utilityCompanySeqid AS INT\r\n\t,@energyDeliveryTypeSeqid AS INT\r\n\t,@IsTOD AS VARCHAR(1)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT UTR.EnergyDeliveryType\r\n\t\t,UTR.[Description] AS RateDescription\r\n\t\t,UTR.DeliveryTariffRate\r\n\t\t,UTR.UtilityTariffRateInformationSeqid\r\n\tFROM Billing.EnergyDeliveryType AS EDT\r\n\t\tINNER JOIN Billing.UtilityTariffRateInformation AS UTR ON EDT.EnergyType = UTR.EnergyDeliveryType\r\n\tWHERE EDT.EnergyDeliveryType = @energyDeliveryTypeSeqid\r\n\t\tAND UTR.UtilityCompanyTariff = @utilityCompanySeqid AND UTR.IsTod = @IsTOD;\r\nEND;"
        }
      ]
