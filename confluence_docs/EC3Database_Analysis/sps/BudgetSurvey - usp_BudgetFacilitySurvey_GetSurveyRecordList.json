    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [BudgetSurvey].[usp_BudgetFacilitySurvey_GetSurveyRecordList]\n(\r\n\t@Email AS VARCHAR(75)\r\n\t,@ActionTypeId AS INT\r\n)\r\nAS \r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\t\r\n    SELECT DISTINCT AD.AgencyDivisionSeqid AS AgencyDivisionSeqId\r\n\t\t,F.FacilitySeqid AS FacilitySeqId\t\t\r\n\t\t,AFS.AgencyFacilitySurveyId\r\n\t\t,CASE WHEN F.FacilitySeqid IS NULL THEN CONCAT(AFS.FacilityName, ' (', AFS.FacilityAddress, ')') ELSE CONCAT(F.OecFacilityNumber, ' - ', F.FacilityName, ' (', ISNULL(F.Address1, ''), ')') END AS Facility\r\n\t\t,CONCAT(AD.AgencyCodeOEC, ' - ', AD.AgencyName ,' (', AD.AgencyShortDesc, ')') AS Agency\r\n        ,SAT.[Name] AS ActionType\r\n\t\t,SAT.[Description] AS ActionTypedDescription\r\n    FROM BudgetSurvey.AgencyFacilitySurvey AS AFS\r\n\t\tINNER JOIN BudgetSurvey.AgencyBudgetFacilitySurveyAttribute AS A ON A.FiscalYear = AFS.FiscalYear\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON AD.AgencyDivisionSeqid = AFS.AgencyDivisionSeqId\r\n        INNER JOIN Billing.uftn_TableGetAgencyChildrenByAgencyCodeOEC('*', @Email) AS FAD ON AFS.AgencyDivisionSeqId = FAD.AgencyDivisionSeqID\r\n        LEFT JOIN Billing.Facility AS F ON F.FacilitySeqid = AFS.FacilitySeqId AND AD.AgencyDivisionSeqid = AFS.AgencyDivisionSeqId\r\n        LEFT JOIN BudgetSurvey.ActionType AS SAT ON AFS.ActionTypeId = SAT.ActionTypeId\r\n\tWHERE ((@ActionTypeId = 5 AND SAT.ActionTypeId = @ActionTypeId) OR (@ActionTypeId <> 5 AND SAT.ActionTypeId NOT IN (3, 5)));\r\nEND;"
        }
      ]
    }
  ]
}