      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- alter date: <alter Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [Contact].[usp_MassDist_AddPrimaryRecipient]\r\n\t@profileID int,\r\n\t@contactID int,\r\n\t@authenticatedUserID int\r\nAS\r\nBEGIN\r\n\tif(not exists(select * from Contact.MassDistributionListReportContext where MassDistributionProfileSeqid = @profileID and ContactAddressSeqid =@contactID ))\r\n\tbegin\r\n\t\tinsert into\r\n        Contact.MassDistributionListReportContext\r\n        (MassDistributionProfileSeqid, ContactAddressSeqid, AuthenticatedUserID)\r\n        values\r\n        (@profileID,@contactID,@authenticatedUserID)\r\n\tend\r\nEND"
        }
