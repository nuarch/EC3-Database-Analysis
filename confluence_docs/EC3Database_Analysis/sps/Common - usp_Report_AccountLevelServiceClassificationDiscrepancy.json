      "content": [
        {
          "type": "text",
          "text": "CREATE  PROCEDURE [Common].[usp_Report_AccountLevelServiceClassificationDiscrepancy] @BillingPeriod varchar(6)\n   \r\n AS --**************************************************************************************\r\n--* Name:         \r\n--*\r\n--* Description:  Report Account Level Service Classification Discrepancy\r\n---*               \r\n--* Exec:       \t\t[Common].[usp_Report_AccountLevelServiceClassificationDiscrepancy]\r\n--*\r\n--* Parameter(s):         @Status int output\r\n--*\r\n--*\tSet ANSI_null on\r\n--*\tgo\r\n\r\n--* Database:     OEC\r\n--*\r\n--* Return:\t    0 Success\r\n--*                   9 Failure\r\n--*\r\n--* AUTHOR:       Peter Heller (PAH)\r\n--* Created On:   08/22/2011\r\n--**************************************************************************************\r\n--* Date         Tech Description of Change\r\n--* ---------- ----  -------------------------------------------------------------\r\n--* 10/26/2005 PAH  First Version \r\n--**************************************************************************************\r\n\r\n--**************************************************************************************\r\n--Declare Variables\r\n--**************************************************************************************\r\n--\r\n--**************************************************************************************\r\n--Set defaults\r\n--**************************************************************************************\r\n\r\n--**************************************************************************************\r\n--Main Process\r\n--**************************************************************************************\r\n--\r\nbegin\r\nSELECT Billing.AccountLevelServiceClassificationDiscrepancy.BillingPeriod, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.AccountLevelServiceClassificationDiscrepancySeqid, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.AccountIsUpdated, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.DeliveryTariffRateExistsInEC3, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.CurrentInvoiceAccountGroup, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.UtilityTariffRateInformationSeqid, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.CurrentAccountNumber, Billing.AccountLevelServiceClassificationDiscrepancy.AccountSeqid, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.UtilityServiceAddress, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.UtilityCompanyShortDesc, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.AgencyCodeOEC, Billing.AccountLevelServiceClassificationDiscrepancy.AgencyShortDesc, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.OecFacilityNumber, Billing.AccountLevelServiceClassificationDiscrepancy.FacilityName, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.Address1, Billing.AccountLevelServiceClassificationDiscrepancy.BudgetRateClassDescription, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.Description, Billing.AccountLevelServiceClassificationDiscrepancy.ShortDesc, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.AccountStatus, Billing.AccountLevelServiceClassificationDiscrepancy.AccountStatusCodePeriod, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.IsTimeOfDayAccount, Billing.AccountLevelServiceClassificationDiscrepancy.DeliveryTariffRate, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.TariffCode, Billing.AccountLevelServiceClassificationDiscrepancy.UtilityCompanySeqid, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.IsTOD, Billing.AccountLevelServiceClassificationDiscrepancy.AuthenticatedUserID, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.Notes, Billing.AccountLevelServiceClassificationDiscrepancy.DateAdded, \r\n                      Billing.AccountLevelServiceClassificationDiscrepancy.LastUpdate, Billing.UtilityTariffRateInformation.UtilityTariffRateInformationSeqid AS newAccountUtilityTariffRateInformationSeqid, \r\n                      Billing.UtilityTariffRateInformation.DeliveryTariffRate AS newAccountDeliveryTariffRate, Billing.UtilityTariffRateInformation.IsTod AS newAccountUtilityTariffRateInformation, \r\n                      Billing.UtilityTariffRateInformation.UtilityCompanyTariff as newAccountUtilityCompanySeqid\r\nFROM Billing.UtilityTariffRateInformation INNER JOIN\r\n                      Billing.Account ON \r\n                      Billing.UtilityTariffRateInformation.UtilityTariffRateInformationSeqid = Billing.Account.UtilityTariffRateInformationSeqid RIGHT JOIN\r\n                      Billing.AccountLevelServiceClassificationDiscrepancy ON \r\n                      Billing.Account.AccountSeqid = Billing.AccountLevelServiceClassificationDiscrepancy.AccountSeqid\r\nWHERE (Billing.AccountLevelServiceClassificationDiscrepancy.BillingPeriod = @BillingPeriod)\r\nend"
        }
      ]
    }
