      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[ups_BudgetSurvey_ActiveAccountList]\n\t@EmailAddress emailaddr,\r\n\t@AgencyCodeOEC AgencyCodeOEC\r\nAS\r\nBEGIN\r\n\t\t\r\n\tSELECT DISTINCT\r\n\t\t\t'N' AS Inactive ,\r\n\t\t\tAgencyDivision.AgencyCodeOEC ,\r\n\t\t\tAgencyDivision.AgencyName ,\r\n\t\t\tAccount.Borough ,\r\n\t\t\tAccount.Block ,\r\n\t\t\tAccount.LotNumber ,\r\n\t\t\tAccount.AccountStatus ,\r\n\t\t\tFacility.OecFacilityNumber ,\r\n\t\t\tFacility.FacilityName ,\r\n\t\t\tFacility.Address1 ,\r\n\t\t\tAccount.CurrentAccountNumber ,\r\n\t\t\tAccount.UtilityServiceAddress ,\r\n\t\t\tAccount.CityPlanningBIN ,\r\n\t\t\tUtilityCompany.ShortDesc ,\r\n\t\t\tEnergyDeliveryType.EnergyType AS EnergyDeliveryType ,\r\n\t\t\tAccount.OriginalAccountNumber ,\r\n\t\t\tAccntNumber.LastPeriodBilled\r\n\tFROM Billing.Account AS Account\r\n\t\t\tINNER JOIN\r\n\t\t\tBilling.uftn_TableGetAgencyChildrenByAgencyCodeOEC(@AgencyCodeOEC,@EmailAddress) \r\n\t\t\t  AS AgencyDivision ON Account.AgencyAccount = AgencyDivision.AgencyDivisionSeqid\r\n\t\t\tINNER JOIN Billing.Facility AS Facility ON Account.FacilityAccount = Facility.FacilitySeqid\r\n\t\t\tINNER JOIN Billing.UtilityCompany AS UtilityCompany ON Account.UtilityAccountProvider = UtilityCompany.UtilityCompanySeqid\r\n\t\t\tINNER JOIN Billing.EnergyDeliveryType AS EnergyDeliveryType ON Account.EnergyAccountDescription = EnergyDeliveryType.EnergyDeliveryType\r\n\t\t\tLEFT JOIN ( SELECT    OriginalAccountNumber ,\r\n\t\t\t\t\t\t\t\t\t\tMAX(BillingPeriod) AS LastPeriodBilled\r\n\t\t\t\t\t\t\t  FROM   Billing.AccountBilling\r\n\t\t\t\t\t\t\t  GROUP BY  OriginalAccountNumber\r\n\t\t\t\t\t\t\t) AccntNumber ON AccntNumber.OriginalAccountNumber = Account.OriginalAccountNumber\r\n\tWHERE    ( Account.AccountStatus = '46'\r\n\t\t\t\t  OR Account.AccountStatus = '47'\r\n\t\t\t\t  OR Account.AccountStatus = 'AC'\r\n\t\t\t\t  OR Account.AccountStatus = 'LA'\r\n\t\t\t\t  OR Account.AccountStatus = 'UA'\r\n\t\t\t\t)\r\n\r\nEND"
        }
      ]
    }
  ]
