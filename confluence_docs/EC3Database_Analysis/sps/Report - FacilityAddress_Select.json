    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[FacilityAddress_Select]\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT DISTINCT F.OecFacilityNumber, F.FacilityName + ', ' + F.Address1 as FacilityAddress\r\n\tFROM Billing.Facility AS F INNER JOIN Billing.Property AS P ON LTRIM(RTRIM(F.CityPlanningBIN)) = LTRIM(RTRIM(P.BIN))\r\n\tWHERE P.Rating IS NOT NULL AND F.FacilityName IS NOT NULL\r\n\tORDER BY F.OecFacilityNumber;\r\nEND;"
        }
      ]
    }
  ]
