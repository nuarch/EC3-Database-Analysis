    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_GetUtilityCompanyTariffEnergyTypeLookup]\n(\r\n\t@EmailAddress AS VARCHAR(75)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT DISTINCT\r\n\t\tEDT.EnergyDeliveryType AS EnergyDeliveryTypeSeqId \r\n\t\t,EDT.EnergyType\r\n\t\t,UC.UtilityCompanySeqid AS UtilityCompanySeqId\r\n\t\t,UC.[Description] AS UtilityCompany\r\n\t\t,UTC.Id AS UtilityTariffRateSeqId\r\n\t\t,UTC.[Name] AS TariffClass\r\n\tFROM Billing.Account AS A\r\n\t\tINNER JOIN Billing.UtilityCompany AS UC ON UC.UtilityCompanySeqid = A.UtilityAccountProvider\r\n\t\tINNER JOIN Billing.EnergyDeliveryType AS EDT ON EDT.EnergyDeliveryType = A.EnergySource\r\n\t\tINNER JOIN Billing.UtilityTariffRateInformation AS UTRI ON UTRI.UtilityTariffRateInformationSeqid = A.UtilityTariffRateInformationSeqid\r\n\t\tINNER JOIN Billing.UtilityTariffClass AS UTC ON UTRI.ShortDesc = UTC.[Name]\r\n\t\tINNER JOIN dbo.uftn_TableGetAgencyByEmailAddressAgencyAccessAction(@EmailAddress) AS AG ON AG.AgencyDivisionSeqid = A.AgencyAccount\r\n\tWHERE A.IsActive = 1 AND A.IsCurrentRecord = 'Y';\r\nEND;"
        }
      ]
    }
  ]
}