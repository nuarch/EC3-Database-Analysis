    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [Scheduler].[usp_DistributionContactThatNeedsMembership]\r\nAS\r\nBEGIN\r\n\r\nDECLARE @primaryContact TABLE\r\n(\r\n\tprimaryContact seqid\r\n)\r\n\r\nINSERT INTO @primaryContact (primaryContact)\r\n\tSELECT  DISTINCT prim.ContactAddressSeqid\r\n\tFROM Scheduler.DistributionJobDistributionProfile AS job\r\n\t\tINNER JOIN Contact.MassDistributionListReportContext AS prim \r\n\t\t\tON job.DistributionProfileSeqID = prim.MassDistributionProfileSeqid\r\n\t\tINNER JOIN Contact.ContactAddress primAddress \r\n\t\t\tON prim.ContactAddressSeqid = primAddress.ContactAddressSeqid\r\n\t\tLEFT JOIN Contact.MassDistributionListEmailRecipient as sec\r\n\t\t\tON sec.MassDistributionProfileSeqid = prim.MassDistributionProfileSeqid\r\n\t\t\tAND sec.ContactAddressSeqid = prim.ContactAddressSeqid\r\n\t\tLEFT JOIN Contact.ContactAddress secAddress\r\n\t\tON secAddress.ContactAddressSeqid = sec.SecondaryEmailRecipient \r\n\r\n\r\nINSERT INTO @primaryContact\r\n        ( primaryContact )\r\n\tSELECT  distinct Contact.ContactAddress.ContactAddressSeqid\r\n\tFROM Scheduler.DistributionJobDistributionProfile AS DistributionJob\r\n\t\t\tINNER JOIN Contact.MassDistributionProfile AS MassDistributionProfile ON DistributionJob.DistributionProfileSeqID = MassDistributionProfile.MassDistributionProfileSeqid\r\n\t\t\tINNER JOIN Contact.ContactProfile ON MassDistributionProfile.ContactProfileSeqID = Contact.ContactProfile.ContactProfileSeqid\r\n\t\t\tINNER JOIN Contact.ContactProfileAddress ON Contact.ContactProfile.ContactProfileSeqid = Contact.ContactProfileAddress.ContactProfile\r\n\t\t\tINNER JOIN Contact.ContactAddress ON Contact.ContactProfileAddress.ContactAddress = Contact.ContactAddress.ContactAddressSeqid\r\n\tWHERE   ( MassDistributionProfile.ContactProfileSeqID IS not NULL )\r\n\r\nSELECT ca.ContactFirstName, ca.ContactFirstName, ca.ContactLastName, ca.EmailAddress, ad.AgencyName\r\nFROM Contact.ContactAddress AS ca  \r\nINNER JOIN\r\n@primaryContact AS pc\r\nON ca.ContactAddressSeqid = pc.primaryContact\r\nINNER JOIN  Billing.AgencyDivision AS ad\r\nON ca.AgencyContact = ad.AgencyDivisionSeqid\r\nLEFT JOIN\r\nMembership.EC3Membership AS m\r\nON ca.EmailAddress = m.Email\r\nWHERE m.AuthenticatedUserId IS NULL\r\n\r\n\r\nend"
        }
      ]
    }
  ]
