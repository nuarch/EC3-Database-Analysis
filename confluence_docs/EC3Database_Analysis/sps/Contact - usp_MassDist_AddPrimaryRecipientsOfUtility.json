      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- alter date: <alter Date,,>\r\n-- Description:\tAdd all primary recipients of particular utility to distribution\r\n-- =============================================\r\nCREATE PROCEDURE [Contact].[usp_MassDist_AddPrimaryRecipientsOfUtility]\r\n\t@utilityID int,\r\n\t@distributionProfileID int,\r\n\t@authenticatedUserID int\r\nAS\r\nBEGIN\r\n\tinsert into Contact.MassDistributionListReportContext\r\n\t(\r\n\t\tMassDistributionProfileSeqid, \r\n\t\tContactAddressSeqid,\r\n\t\tauthenticatedUserID\r\n\t)\r\n\tselect \r\n\t\t@distributionProfileID,\r\n\t\tContactAddressSeqid, \r\n\t\t@authenticatedUserID \r\n\tfrom Contact.ContactAddress\r\n\twhere @utilityID = UtilityCompanyContact\r\n\tand ContactAddressSeqid not in\r\n\t(\r\n\t\tselect ContactAddressSeqid \r\n\t\tfrom Contact.MassDistributionListReportContext\r\n\t\twhere MassDistributionProfileSeqid = @distributionProfileID\t\r\n\t)\r\nEND"
        }
