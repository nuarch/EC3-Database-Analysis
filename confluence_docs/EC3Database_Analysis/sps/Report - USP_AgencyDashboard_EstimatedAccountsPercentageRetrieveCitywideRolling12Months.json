    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[USP_AgencyDashboard_EstimatedAccountsPercentageRetrieveCitywideRolling12Months]\n(\r\n\t@publishedBillingPeriod AS VARCHAR(6) = NULL\r\n\t,@debugMode AS INT = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @latestBillingPeriod AS VARCHAR(6);\r\n\t\r\n\tSELECT @latestBillingPeriod = MAX(PublishedBillingPeriod), @publishedBillingPeriod = ISNULL(NULLIF(@publishedBillingPeriod, ''), '')\r\n\tFROM Published.MonthlyPublishedSummaryData WITH (NOLOCK);\r\n\r\n\tIF (@publishedBillingPeriod = '')\r\n\tBEGIN\r\n\t\tSELECT @publishedBillingPeriod = @latestBillingPeriod;\r\n\tEND;\r\n\r\n\tSELECT 'CITY' AS AgencyCodeOEC,\r\n\t\t@PublishedBillingPeriod AS BillingPeriod,\r\n\t\tcounts.Category,\r\n\t\t'Count of estimated accounts' Label,\r\n\t\tCONVERT(DECIMAL(18, 4),ISNULL(counts.cnt,0)) AS [Count],\r\n\t\tCONVERT(DECIMAL(18, 4),ISNULL(citywide.totalcnt,0)) AS TotalCount,\r\n\t\tCONVERT(DECIMAL(18,2),ROUND(((CONVERT(DECIMAL(18, 4),ISNULL(counts.cnt,0)) / CONVERT(DECIMAL(18, 4),ISNULL(citywide.totalcnt,0))) * 100),0)) AS [Percentage]  \r\n\tFROM (SELECT CASE \r\n\t\t\t\tWHEN TAM.UtilityCompanySeqid = 7 THEN 'NYPA' \r\n\t\t\t\tWHEN TAM.UtilityCompanySeqid = 2 THEN 'NGrid West' \r\n\t\t\t\tWHEN TAM.UtilityCompanySeqid = 9 AND TAM.EnergyType='GAS' THEN 'Con Edison GAS'  \r\n\t\t\t\tELSE 'other' END AS Category,\r\n\t\t\tCOUNT(DISTINCT TAM.CurrentAccountNumber) cnt\r\n\t\tFROM Published.TemporalAccountMeterLevelRawDataForCurrentPeriod AS TAM WITH (NOLOCK)\r\n\t\tWHERE TAM.EffectiveStartPeriod <= @PublishedBillingPeriod AND TAM.EffectiveEndPeriod > @PublishedBillingPeriod\r\n\t\t\tAND TAM.BillingPeriod = @PublishedBillingPeriod\r\n\t\t\tAND TAM.ConsecutivePeriodsEstimatedReading >= 1\r\n\t\tGROUP BY TAM.UtilityCompanySeqid, TAM.EnergyType) AS counts\r\n\t\tRIGHT JOIN\r\n\t\t(SELECT DISTINCT CASE \r\n\t\t\t\tWHEN UC.UtilityCompanySeqid = 7 THEN 'NYPA' \r\n\t\t\t\tWHEN UC.UtilityCompanySeqid = 2 THEN 'NGrid West' \r\n\t\t\t\tWHEN UC.UtilityCompanySeqid = 9 AND EDT.EnergyType='GAS' THEN 'Con Edison GAS'  \r\n\t\t\t\tELSE 'other' END AS Category,\r\n\t\t\tCOUNT(DISTINCT A.CurrentAccountNumber) AS totalcnt\r\n\t\tFROM Billing.Account AS A WITH (NOLOCK)\r\n\t\t\tINNER JOIN Billing.AgencyDivision AS AD WITH (NOLOCK) ON A.AgencyAccount = AD.AgencyDivisionSeqid\r\n\t\t\tINNER JOIN Billing.Facility AS F WITH (NOLOCK) ON A.FacilityAccount = F.FacilitySeqid\r\n\t\t\tINNER JOIN Billing.UtilityCompany AS UC WITH (NOLOCK) ON A.UtilityAccountProvider = UC.UtilityCompanySeqid\r\n\t\t\tINNER JOIN Billing.EnergyDeliveryType AS EDT WITH (NOLOCK) ON A.EnergyAccountDescription = EDT.EnergyDeliveryType\r\n\t\t\tLEFT JOIN ( SELECT OriginalAccountNumber, MAX(BillingPeriod) AS LastPeriodBilled\r\n\t\t\t\tFROM Billing.AccountBilling WITH (NOLOCK)\r\n\t\t\t\tGROUP BY  OriginalAccountNumber) AS AccntNumber\r\n\t\t\t\tON AccntNumber.OriginalAccountNumber = A.OriginalAccountNumber\r\n\t\tWHERE AD.Inactive = 'N' AND A.AccountStatus IN ('46', '47', 'AC', 'LA', 'UA')\r\n\t\tGROUP BY UC.UtilityCompanySeqid, EDT.EnergyType) AS citywide\r\n\t\tON counts.category = citywide.category;\r\nEND;"
        }
      ]
    }
  ]
