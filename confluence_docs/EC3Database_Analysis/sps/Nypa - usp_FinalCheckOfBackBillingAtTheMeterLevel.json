      "content": [
        {
          "type": "text",
          "text": "CREATE    procedure [Nypa].[usp_FinalCheckOfBackBillingAtTheMeterLevel] (@Status varchar(1) output)\nas\r\n\r\nupdate  Nypa.UploadNYPAMeterBilling\r\nset     MeterStatus = Billing.MeterBilling.MeterBillingStatus ,\r\n        MeterBillingSeqid = Billing.MeterBilling.MeterBillingSeqid ,\r\n        MeterBillingElectricSeqid = Billing.MeterBillingElectric.MeterBillingElectricSeqid\r\nFROM Billing.MeterBilling\r\n        inner join Billing.AccountExchangeMeterTrack on Billing.MeterBilling.AccountExchangeMeterTrackSeqid = Billing.AccountExchangeMeterTrack.AccountExchangeMeterTrackSeqid\r\n        inner join Billing.MeterBillingElectric on Billing.MeterBilling.MeterBillingSeqid = Billing.MeterBillingElectric.MeterBillingRowId\r\n        inner join Nypa.UploadNYPAMeterBilling on Billing.MeterBilling.BillingPeriodRevision = Nypa.UploadNYPAMeterBilling.RevisedBillingPeriod\r\n        inner join Billing.Account on Billing.AccountExchangeMeterTrack.OriginalAccountSeqid = Billing.Account.AccountSeqid\r\n                                      and Nypa.UploadNYPAMeterBilling.CurrentAccountNumber = Billing.Account.CurrentAccountNumber\r\n        inner join Billing.Meter on Billing.AccountExchangeMeterTrack.OriginalMeterSeqid = Billing.Meter.MeterSeqid\r\n                                    and Nypa.UploadNYPAMeterBilling.CurrentMeter = Billing.Meter.CurrentMeterNumber\r\nwhere   ( Nypa.UploadNYPAMeterBilling.MeterStatus = 'BA' )\r\n        and ( Nypa.UploadNYPAMeterBilling.Demand <> 0 )\r\n        and ( Nypa.UploadNYPAMeterBilling.CancelBilledDemand <> 0 )\r\n        and ( Nypa.UploadNYPAMeterBilling.Consumption <> 0 )\r\n        and ( Nypa.UploadNYPAMeterBilling.CancelConsumption <> 0 )\r\n\r\n\r\n\r\n--FROM Billing.MeterBilling INNER JOIN\r\n--                      Billing.AccountExchangeMeterTrack ON \r\n--                      Billing.MeterBilling.AccountExchangeMeterTrackSeqid = Billing.AccountExchangeMeterTrack.AccountExchangeMeterTrackSeqid INNER JOIN\r\n--                      Billing.MeterBillingElectric ON Billing.MeterBilling.MeterBillingSeqid = Billing.MeterBillingElectric.MeterBillingRowId INNER JOIN\r\n--                      Nypa.UploadNYPAMeterBilling ON Billing.MeterBilling.OriginalAccountNumber = Nypa.UploadNYPAMeterBilling.CurrentAccountNumber AND \r\n--                      Billing.MeterBilling.OriginalMeterNumber = Nypa.UploadNYPAMeterBilling.CurrentMeter AND \r\n--                      Billing.MeterBilling.BillingPeriodRevision = Nypa.UploadNYPAMeterBilling.RevisedBillingPeriod\r\n--WHERE (Nypa.UploadNYPAMeterBilling.MeterStatus = 'BA')\r\n\r\n\r\nprint 'Number of Erroneous back bills corrected'+str(@@rowcount)"
        }
      ]
    }
