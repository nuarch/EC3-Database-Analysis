                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The "
                        },
                        {
                          "type": "text",
                          "text": "DataType",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " is set to "
                        },
                        {
                          "type": "text",
                          "text": "0",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to indicate the source is from the SBMR data."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Second Insertion Block",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "Similarly, it inserts records into "
                        },
                        {
                          "type": "text",
                          "text": "@BBLInconsistencies",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " from the "
                        },
                        {
                          "type": "text",
                          "text": "Facility",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " table where discrepancies exist."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The "
                        },
                        {
                          "type": "text",
                          "text": "DataSource",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " string indicates the source as \"EC3 Facility\" with a facility number."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The "
                        },
                        {
                          "type": "text",
                          "text": "DataType",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " is set to "
                        },
                        {
                          "type": "text",
                          "text": "1",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to indicate the source is from the Facility data."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Comparison Logic",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "Both insertion blocks use "
                        },
                        {
                          "type": "text",
                          "text": "ISNULL",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to handle NULL values and ensure that comparisons are made against empty strings if necessary."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The "
                        },
                        {
                          "type": "text",
                          "text": "WHERE",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " clause ensures that only records with discrepancies between the "
                        },
                        {
                          "type": "text",
                          "text": "SBMRHeader",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and "
                        },
                        {
                          "type": "text",
                          "text": "Facility",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " tables are selected."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Result Set",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure concludes by selecting all records from the "
                        },
                        {
                          "type": "text",
                          "text": "@BBLInconsistencies",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " table, effectively returning the identified inconsistencies."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Use of ISNULL",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The frequent use of "
                },
                {
                  "type": "text",
                  "text": "ISNULL",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can impact performance, especially if the tables are large. It ensures that NULL values do not cause mismatches, but it adds overhead to each comparison."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Table Variable",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of a table variable is suitable for small to medium-sized result sets. However, if the result set grows large, it may be more efficient to use a temporary table with indexing options."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Joins and Filters",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure performs inner joins and filters based on the "
                },
                {
                  "type": "text",
                  "text": "SBMRHeaderSeqID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", which should be indexed to improve performance."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "NULL Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure assumes that NULL values should be treated as empty strings for comparison purposes. This may not always align with business rules or data integrity requirements."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": As the volume of data grows, the use of table variables without indexing may lead to performance bottlenecks."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Integrity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure relies on the assumption that discrepancies are meaningful and require attention. If the data sources are not synchronized or have known acceptable differences, this could lead to false positives."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Error Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure lacks explicit error handling, which could be a risk if unexpected data conditions occur. Adding error handling mechanisms would improve robustness."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [SBMR].[usp_Review_GetBBLInconsistencies]\r\n\t@SBMRHeaderSeqID INT\r\nAS\r\nBEGIN\r\n\r\n\tDECLARE @BBLInconsistencies TABLE\r\n\t(\r\n\t\tDataSource VARCHAR(200),\r\n\t\tBlock Block,\r\n\t\tLotNumber LotNumber,\r\n\t\tBin CityPlanningBIN,\r\n\t\tBoro int,\r\n\t\tIsRental yesno,\r\n\t\tDataType SMALLINT -- 0 SBMR 1 Current Facility Information\r\n\t)\r\n\t\r\n\tINSERT INTO @BBLInconsistencies\r\n\t        ( DataSource ,\r\n\t          Block ,\r\n\t          LotNumber ,\r\n\t          Bin ,\r\n\t          Boro ,\r\n\t          IsRental,\r\n\t          DataType\r\n\t        )\r\n\tSELECT  \r\n            'SBMR Information - ' + sb.SbmrReferenceNumber AS DataSource,\r\n            ISNULL(sb.Block, '') as Block,\r\n            ISNULL(sb.LotNumber, '') as LotNumber,\r\n            ISNULL(sb.CityPlanningBIN, '') as Bin,\r\n            ISNULL(sb.Borough, '') as Boro,\r\n            ISNULL(sb.isRentalSpace, '') AS IsRentalSpace,\r\n            0\r\n    FROM SBMR.SBMRHeader AS sb\r\n            INNER JOIN Billing.Facility AS f ON sb.FacilitySeqid = f.FacilitySeqid\r\n    WHERE   SBMRHeaderSeqid = @SBMRHeaderSeqid\r\n            AND NOT ( ISNULL(sb.Block, '') = ISNULL(f.Block, '')\r\n                      AND ISNULL(sb.LotNumber, '') = ISNULL(f.LotNumber, '')\r\n                      AND ISNULL(sb.CityPlanningBIN, '') = ISNULL(f.CityPlanningBIN, '')\r\n                      AND ISNULL(sb.Borough, '') = ISNULL(f.Borough, '')\r\n                      AND ISNULL(sb.isRentalSpace, '') = ISNULL(f.isRentalSpace, '')\r\n                    )\r\n                            \r\n     \r\n\tINSERT INTO @BBLInconsistencies\r\n        ( DataSource ,\r\n          Block ,\r\n          LotNumber ,\r\n          Bin ,\r\n          Boro ,\r\n          IsRental,\r\n          DataType\r\n        )\r\n    SELECT\r\n        'EC3 Facility #' + f.OecFacilityNumber AS DataSource,\r\n            ISNULL(f.Block, '') as Block,\r\n            ISNULL(f.LotNumber, '') as LotNumber,\r\n            ISNULL(f.CityPlanningBIN, '') as Bin,\r\n            ISNULL(f.Borough, '') as Boro,\r\n            ISNULL(f.isRentalSpace, '') AS IsRentalSpace,\r\n            1\r\n    FROM SBMR.SBMRHeader AS sb\r\n            INNER JOIN Billing.Facility AS f ON sb.FacilitySeqid = f.FacilitySeqid\r\n    WHERE   SBMRHeaderSeqid = @SBMRHeaderSeqid\r\n            AND NOT ( ISNULL(sb.Block, '') = ISNULL(f.Block, '')\r\n                      AND ISNULL(sb.LotNumber, '') = ISNULL(f.LotNumber, '')\r\n                      AND ISNULL(sb.CityPlanningBIN, '') = ISNULL(f.CityPlanningBIN, '')\r\n                      AND ISNULL(sb.Borough, '') = ISNULL(f.Borough, '')\r\n                      AND ISNULL(sb.isRentalSpace, '') = ISNULL(f.isRentalSpace, '')\r\n                    )\r\n\r\n    SELECT * FROM @BBLInconsistencies\r\nEND"
        }
      ]
    }
  ]
