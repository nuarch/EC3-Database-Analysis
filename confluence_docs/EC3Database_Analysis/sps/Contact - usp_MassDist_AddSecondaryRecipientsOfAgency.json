    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- alter date: <alter Date,,>\r\n-- Description:\tAdd all secondary recipients of particular agency to distribution list\r\n-- =============================================\r\nCREATE PROCEDURE [Contact].[usp_MassDist_AddSecondaryRecipientsOfAgency]\r\n\t@agencyID int,\r\n\t@primaryRecipientID int,\r\n\t@distributionProfileID int,\r\n\t@AuthenticatedUserID int\r\nAS\r\nBEGIN\r\n\r\n\tinsert into Contact.MassDistributionListEmailRecipient\r\n\t(\r\n\t\tMassDistributionProfileSeqid, \r\n\t\tContactAddressSeqid,\r\n\t\tSecondaryEmailRecipient,\r\n\t\tEmailRecipientType,\r\n\t\tAuthenticatedUserID\r\n\t)\r\n\tselect \r\n\t\t@distributionProfileID, \r\n\t\t@primaryRecipientID, \r\n\t\tContactAddressSeqID, \r\n\t\t'cc', \r\n\t\t@AuthenticatedUserID\r\n\tfrom Contact.ContactAddress\r\n\twhere @agencyID = AgencyContact\r\n\tand ContactAddressSeqid not in\r\n\t(\r\n\t\tselect SecondaryEmailRecipient \r\n\t\tfrom Contact.MassDistributionListEmailRecipient\r\n\t\twhere \r\n\t\t\tMassDistributionProfileSeqid = @distributionProfileID\r\n\t\t\tand\r\n\t\t\tContactAddressSeqid = @primaryRecipientID\t\r\n\t)\r\n\t\r\nEND"
        }
      ]
    }
  ]
