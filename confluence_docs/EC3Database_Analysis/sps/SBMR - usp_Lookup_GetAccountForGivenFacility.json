    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [SBMR].[usp_Lookup_GetAccountForGivenFacility]\r\n\t@FacilitySeqID seqid\r\nAS\r\nBEGIN\r\n\t\r\n SELECT acc.OriginalAccountNumber AS AccountNumber ,\r\n\t\tacc.AccountStatus ,\r\n        ad.AgencyName ,\r\n        uti.Description AS UtilityCompany,\r\n        fac.OecFacilityNumber,\r\n        fac.FacilitySeqid\r\n FROM Billing.Account AS acc\r\n        INNER JOIN Billing.Facility AS fac ON acc.FacilityAccount = fac.FacilitySeqid\r\n        INNER JOIN Billing.UtilityCompany AS uti ON acc.UtilityAccountProvider = uti.UtilityCompanySeqid\r\n        INNER JOIN Billing.AgencyDivision AS ad ON acc.AgencyAccount = ad.AgencyDivisionSeqid\r\n WHERE  acc.OriginalAccountNumber = acc.CurrentAccountNumber\r\n        AND fac.FacilitySeqID = @FacilitySeqID\r\n ORDER BY acc.OriginalAccountNumber\r\n               \r\nEND"
        }
      ]
    }
