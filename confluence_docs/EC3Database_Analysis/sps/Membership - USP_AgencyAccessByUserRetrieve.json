    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "--**************************************************************************************\n--* Description:     Agency Access: Retrieve hierarchical list of agencies input user \r\n--*                               [@authenticatedUserId], has access to.\r\n--* AUTHOR:                RAKESH MENON\r\n--* Created On:            01/11/2016\r\n--**************************************************************************************\r\n--* Date       Tech  Description of Change\r\n--* ---------- ----  -------------------------------------------------------------\r\n--* 01/11/2016 RM    Initial Version\r\n--* 02/24/2016 RM    Added MAX(IsAllAgencyAccess) to reduce count of rows returned\r\n--*                               for all agency access\r\n--**************************************************************************************\r\nCREATE PROCEDURE [Membership].[USP_AgencyAccessByUserRetrieve]\r\n       @authenticatedUserId INT\r\nAS\r\nBEGIN\r\n\r\n       SET NOCOUNT ON;\r\n\r\n       DECLARE @EmailAddress VARCHAR(100);\r\n       SELECT @EmailAddress = Email FROM [Membership].[EC3Membership] WHERE AuthenticatedUserID = @authenticatedUserId;\r\n\r\n       SELECT MBR.AuthenticatedUserID, MBR.UserFirstName, MBR.UserLastName, MBR.Email\r\n              , ISNULL((SELECT MAX(IsAllAgencyAccess) FROM [Membership].[MembershipAgencyAccess] WHERE AuthenticatedUserID = MBR.AuthenticatedUserID), 'N') AS IsAllAgencyAccess\r\n              , T.AgencyDivisionSeqID, T.AgencyCodeOEC, T.AgencyName, T.ParentAgencyDivisionSeqid, T.AgencyDivisionHierarchy\r\n       FROM [Membership].[EC3Membership] MBR, (\r\n              SELECT * FROM [Billing].[uftn_TableGetAgencyChildrenByAgencyCodeOEC]('*', @EmailAddress)\r\n       ) T\r\n       WHERE MBR.AuthenticatedUserID = @authenticatedUserId;\r\n\r\n       SET NOCOUNT OFF;\r\n\r\nEND"
        }
      ]
    }
  ]
