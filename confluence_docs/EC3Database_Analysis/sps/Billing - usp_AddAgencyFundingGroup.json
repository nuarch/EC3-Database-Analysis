      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [Billing].[usp_AddAgencyFundingGroup]\r\n\t@AgencyDivisionSeqID INT,\r\n\t@AgencyFundingSeqid INT,\r\n\t@AuthenticatedUserID INT\r\nAS\r\nBEGIN\r\n\tIF(NOT EXISTS(SELECT * FROM Billing.AgencyFundingAssignments WHERE AgencyDivisionSeqid = @AgencyDivisionSeqID AND AgencyFundingSeqid = @AgencyFundingSeqid))\t\r\n\t\tINSERT INTO \r\n\t\tbilling.AgencyFundingAssignments\r\n\t\t        ( AgencyFundingSeqid ,\r\n\t\t          AgencyDivisionSeqid ,\r\n\t\t          AuthenticatedUserID \r\n\t\t        )\r\n\t\tVALUES  ( \r\n\t\t\t\t\t@AgencyFundingSeqid,\r\n\t\t\t\t\t@AgencyDivisionSeqID,\r\n\t\t\t\t\t@AuthenticatedUserID\r\n\t\t        )\r\nEND"
        }
