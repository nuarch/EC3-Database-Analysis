      "content": [
        {
          "type": "text",
          "text": "CREATE   PROC [Batch].[usp_Process_NYPA_AddReativePowerData_Page3]\nAS\r\nBEGIN\r\n\r\n\t-- Process Nypa Add Reactive Power Data\r\n\tdeclare @RC int\r\n\tdeclare @AuthenticatedUserID int\r\n\tdeclare @StatusCode int\r\n\r\n\tEXECUTE @RC = [Nypa].[usp_ProcessNypaAddReactivePowerData]\r\n\t1 --@AuthenticatedUserID\r\n\t,0 --@StatusCode OUTPUT\r\n\r\n\t-- ProcessNypaAddPage3CutomerLevelAllocation\r\n\tset @StatusCode = 0\r\n\tdeclare\t@return_value int\r\n\r\n\tEXEC\t@return_value = [Nypa].[usp_ProcessNypaAddPage3CutomerLevelAllocation] @AuthenticatedUserID = 1, @StatusCode = @StatusCode OUTPUT\r\n         \r\n\t-- PreProcessingVerifyAccountsAndMetersExistence\r\n\tSET @return_value = 0\r\n\tDECLARE @Status int\r\n\r\n\tEXEC\t@return_value = [Nypa].[usp_PreProcessingVerifyAccountsAndMetersExistence]\t@Status = @Status output\r\n\r\n\tselect\t@Status as N'@Status'\r\n\tselect\t'Return Value' = @return_value\r\n\r\n\t-- check\r\n\t--AccountBilling Pre-Processing\r\n\t-- CUSTOMER TOTAL x TotalAmountDue field should match cover sheet \"Current Electric Charges Including Adjustments\" line \r\n\t-- This is comparing hard copy coversheet total to e-coversheet file total\r\n\tSELECT * FROM Nypa.uvw_PreProcessingCoversheetVsAccountVsMeterBillingSummary\r\n\t--  Display any FATAL exchange errors. expect no result , important check\r\n\t--\r\n\t-- #CHECK\t\r\n\tSELECT Description, SeverityError, ExchangeCode, CurrentAccountNumber, OriginalAccountNumber, CurrentMeterNumber, OriginalMeterNumber\r\n\tFROM Nypa.UploadNYPAReconcileExchangeProcessTransactionVerification\r\n\tWHERE SeverityError like 'F%'\r\n\r\nEND"
        }
      ]
    }
