        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE   PROC [Batch].[usp_Process_NYPARawData]\nAS\r\nBEGIN\r\n\r\n\t--6.\tThe loading of the raw data is done by executing the stored procedure “usp_ProcessNypaRawData”\r\n\tDECLARE @RC int\r\n\tDECLARE @Status int\r\n\tEXECUTE @RC = [Nypa].[usp_ProcessNypaRawData] @Status output\r\n\t\r\n\r\n\t-- 6.a. Transform Uploaded Data\r\n\tSET @RC = 0\r\n\tdeclare @AuthenticatedUserID int\r\n\tdeclare @StatusCode int\r\n\tset @AuthenticatedUserID = 1\r\n\t\r\n\texecute @RC = [Nypa].[usp_ProcessNYPATransformUploadedData] @AuthenticatedUserID, @StatusCode output\r\n\r\n\t--  Check Upload\r\n\tSELECT * FROM Nypa.uvw_BillingPeriodPreloadRawAndTransformedDataStatistics\r\n\tSELECT * FROM Common.ExchangeData WHERE BillingPeriod ='202305' AND AccountBillingGroupSeqid = 1\r\n\t-- Meter FIX\r\n\t-- NYPA sent  existing active meter numbers as new meters\r\n\t-- below 2 meters are reused meter numbers\r\n\t-- Richard approved renaming these meter renaming so we dont have 2 active meters with same name at the same time.\r\n\r\n\tUPDATE unmb SET unmb.CurrentMeter ='s2683757' FROM nypa.UploadNYPAMeterBilling AS unmb WHERE unmb.CurrentMeter = '2683757'\r\n\tUPDATE unmb SET unmb.CurrentMeter ='s2683836' FROM nypa.UploadNYPAMeterBilling AS unmb WHERE unmb.CurrentMeter = '2683836'\r\n\t\r\n\t-- Create Exchange Data \r\n\t-- SELECT * FROM Common.ExchangeData\r\n\tEXEC [Common].[CopyNYPAExchangeDataToCommon]\r\n\r\n\t--  Check Uploaded data tvisaually confirm its existance\r\n\tSELECT * FROM Nypa.uvw_BillingPeriodPreloadRawAndTransformedDataStatistics\r\n\tSELECT * FROM Common.ExchangeData WHERE BillingPeriod ='202305' AND AccountBillingGroupSeqid = 1 --AND CurrentMeterNumber = OriginalMeterNumber\r\n\r\n\t-- Confirm that there are no revised bills dated after the BillingPeriod.\r\n\t-- We don't expect to see any data for the next two sql statements\r\n\t-- #CHECK\r\n\tSELECT * FROM Nypa.UploadNYPAAccountBilling where BillingPeriod < RevisedBillingPeriod\r\n\tSELECT * FROM Nypa.UploadNYPAMeterBilling where BillingPeriod < RevisedBillingPeriod\r\n\t\r\n\t--TODO\r\n\t-- If there are revised bills dated after the BillingPeriod then remove and notify NYPA.\r\n\t--delete from Nypa.UploadNYPAAccountBilling where BillingPeriod <RevisedBillingPeriod\r\n\t--delete from Nypa.UploadNYPAMeterBilling where BillingPeriod <RevisedBillingPeriod\r\n\t\r\nEND"
        }
      ]
    }
