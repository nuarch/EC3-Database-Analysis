    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "1. Overview"
        }
      ]
    },
    {
      "type": "paragraph",
      "content": [
        {
          "type": "text",
          "text": "The stored procedure "
        },
        {
          "type": "text",
          "text": "usp_LoadFactor_GetSingleFacilityAccountsLoadFactorInformationExport",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " is designed to retrieve and export load factor information for accounts associated with a specific facility. It processes energy usage and demand data over a specified fiscal period, pivoting the data to present it in a month-by-month format. The procedure handles both energy usage and demand usage, and calculates the load factor, which is a measure of the efficiency of energy usage."
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "2. Complexity Level: Medium"
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "3. Input Parameters"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@FacilitySeqID AS INT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Identifies the specific facility for which the load factor information is being retrieved."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@AgencyDivisionSeqID AS INT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Identifies the agency division associated with the facility."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@IsAgencyUser AS BIT = 0",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": A flag indicating whether the user is an agency user. This affects how the "
                },
                {
                  "type": "text",
                  "text": "PublishedBillingPeriod",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is determined."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "4. Business Logic and Workflow"
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Transaction Isolation Level",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure sets the transaction isolation level to "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", allowing dirty reads to improve performance by avoiding locks."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Determine Published Billing Period",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "If "
                },
                {
                  "type": "text",
                  "text": "@IsAgencyUser",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is true, the procedure selects the current processing period from "
                },
                {
                  "type": "text",
                  "text": "Billing.ApplicationTimeFrame",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Otherwise, it selects the maximum published billing period from "
                },
                {
                  "type": "text",
                  "text": "Published.MonthlyPublishedSummaryData",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Fiscal Year Calculation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The current fiscal year is calculated using a user-defined function "
                },
                {
                  "type": "text",
                  "text": "dbo.CalculateFiscalYear",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The beginning fiscal year is determined by subtracting a fixed number of years (3) from the current fiscal year."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Retrieval and Pivoting",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The procedure retrieves data from "
                },
                {
                  "type": "text",
                  "text": "Published.TemporalAccountLevelRawDataForCurrentPeriod",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " for the specified facility and agency division."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "It filters data based on the effective period and fiscal year range."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Three separate queries are executed and unioned:"
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Energy Usage",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Summed and pivoted by month."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Demand Usage",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Summed and pivoted by month, with a transformation of the "
                },
                {
                  "type": "text",
                  "text": "EnergyUnit",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Load Factor",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Calculated as "
                },
                {
                  "type": "text",
                  "text": "AccountEnergyUsage / (AccountDemandUsage * BillingPeriodDays * 24)",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", summed and pivoted by month."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Output",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The results are ordered by account number, fiscal year, and a display index to differentiate between energy, demand, and load factor data."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Improves performance by reducing locking overhead but risks reading uncommitted data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Pivot Operations",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Pivoting large datasets can be resource-intensive, impacting performance."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Union of Queries",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Combining multiple datasets with "
                },
                {
                  "type": "text",
                  "text": "UNION",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can increase execution time, especially if the underlying tables are large."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Indexing",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Proper indexing on "
                },
                {
                  "type": "text",
                  "text": "FacilitySeqID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "text",
                  "text": "AgencyDivisionSeqID",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", and "
                },
                {
                  "type": "text",
                  "text": "PublishedBillingPeriod",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can improve query performance."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Using "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can lead to reading uncommitted or inconsistent data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Division by Zero",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The load factor calculation uses "
                },
                {
                  "type": "text",
                  "text": "NULLIF",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " to prevent division by zero, but any logical errors in data could still cause unexpected results."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Volume",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Large datasets could lead to performance bottlenecks, especially during pivot operations."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Hardcoded Values",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure uses hardcoded values for energy sources and the number of years of data, which may require updates if business rules change."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Concurrency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": High concurrency could lead to performance degradation due to the complexity of the queries and the use of "
                },
                {
                  "type": "text",
                  "text": "UNION",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_LoadFactor_GetSingleFacilityAccountsLoadFactorInformationExport]\n(\r\n\t@FacilitySeqID AS INT\r\n\t,@AgencyDivisionSeqID AS INT\r\n\t,@IsAgencyUser AS BIT = 0\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n    DECLARE @PublishedBillingPeriod AS BillingPeriod, @CurrentFiscalYear AS VARCHAR(4)\r\n\t\t,@BeginningFiscalYear AS VARCHAR(4), @NumberOfYearOfData AS INT = 3;\r\n\r\n\tIF (@IsAgencyUser = 1)\r\n\tBEGIN\r\n\t\tSELECT @PublishedBillingPeriod = PublishedBillingPeriod FROM Billing.ApplicationTimeFrame WHERE CurrentProcessingPeriod = 'Y';\r\n\tEND;\r\n\tELSE\r\n\tBEGIN\r\n\t\tSELECT @PublishedBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData;\r\n\tEND;\r\n\r\n    SET @CurrentFiscalYear = dbo.CalculateFiscalYear(@PublishedBillingPeriod)\r\n    SET @BeginningFiscalYear = CAST(CAST(@CurrentFiscalYear AS INT) - @NumberOfYearOfData + 1 AS VARCHAR(4));\r\n\r\n    SELECT CurrentAccountNumber,\r\n\t\tUtilityServiceAddress,\r\n        FiscalYear,\r\n        DataType,\r\n        July,\r\n        [August],\r\n        [September],\r\n        [October],\r\n        [November],\r\n        [December],\r\n        [January],\r\n        [February],\r\n        [March],\r\n        [April],\r\n        [May],\r\n        [June]\r\n    FROM (SELECT '1' AS displayIndex,\r\n\t\t\tCurrentAccountNumber,\r\n\t\t\tUtilityServiceAddress,\r\n\t\t\tFiscalYear,\r\n\t\t\tEnergyUnit DataType,\r\n\t\t\tJuly,\r\n\t\t\t[August],\r\n\t\t\t[September],\r\n\t\t\t[October],\r\n\t\t\t[November],\r\n\t\t\t[December],\r\n\t\t\t[January],\r\n\t\t\t[February],\r\n\t\t\t[March],\r\n\t\t\t[April],\r\n\t\t\t[May],\r\n\t\t\t[June]\r\n        FROM (SELECT FiscalYear,\r\n                BillingMonth,\r\n                CurrentAccountNumber,\r\n\t\t\t\tUtilityServiceAddress,\r\n                AccountEnergyUsage,\r\n\t\t\t\tEnergyUnit\r\n            FROM Published.TemporalAccountLevelRawDataForCurrentPeriod AS talrdfcp\r\n            WHERE FacilitySeqid = @FacilitySeqID AND AgencyDivisionSeqid = @AgencyDivisionSeqID\r\n\t\t\t\tAND (talrdfcp.EffectiveStartPeriod <= @PublishedBillingPeriod AND talrdfcp.EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\t\t\tAND FiscalYear BETWEEN @BeginningFiscalYear AND @CurrentFiscalYear AND (EnergySource = 1 OR EnergySource = 6)) AS SourceTable\r\n\t\t\tPIVOT (SUM(AccountEnergyUsage) FOR BillingMonth IN (July, [August], [September], [October], [November], [December]\r\n\t\t\t\t,[January], [February], [March], [April], [May], [June])) pivotTable\r\n    UNION\r\n    SELECT '2' AS displayIndex,\r\n\t\tCurrentAccountNumber,\r\n\t\tUtilityServiceAddress,\r\n        FiscalYear,\r\n        CASE WHEN EnergyUnit = 'kWh' THEN 'KW' WHEN EnergyUnit = 'MLbs' THEN 'MLbs/Hour' ELSE EnergyUnit end DataType,\r\n        July,\r\n        [August],\r\n        [September],\r\n        [October],\r\n        [November],\r\n        [December],\r\n        [January],\r\n        [February],\r\n        [March],\r\n        [April],\r\n        [May],\r\n        [June]\r\n    FROM (SELECT FiscalYear,\r\n\t\t\tBillingMonth,\r\n\t\t\tCurrentAccountNumber,\r\n\t\t\tUtilityServiceAddress,\r\n\t\t\tAccountDemandUsage,\r\n\t\t\tEnergyUnit\r\n        FROM Published.TemporalAccountLevelRawDataForCurrentPeriod AS talrdfcp2\r\n        WHERE FacilitySeqid = @FacilitySeqID AND AgencyDivisionSeqid = @AgencyDivisionSeqID\r\n            AND (talrdfcp2.EffectiveStartPeriod <= @PublishedBillingPeriod AND talrdfcp2.EffectiveEndPeriod > @PublishedBillingPeriod)                                       \r\n\t\t\tAND FiscalYear BETWEEN @BeginningFiscalYear AND @CurrentFiscalYear AND (EnergySource = 1 OR EnergySource = 6)) AS SourceTable\r\n\t\tPIVOT (SUM(AccountDemandUsage) FOR BillingMonth IN (July, [August], [September], [October], [November], [December]\r\n\t\t\t,[January], [February], [March], [April], [May], [June])) pivotTable\r\n    UNION\r\n    SELECT '3' AS displayIndex,\r\n\t\tCurrentAccountNumber,\r\n\t\tUtilityServiceAddress,\r\n        FiscalYear,\r\n        'Load Factor' DataType,\r\n        July,\r\n        [August],\r\n        [September],\r\n        [October],\r\n        [November],\r\n        [December],\r\n        [January],\r\n        [February],\r\n        [March],\r\n        [April],\r\n        [May],\r\n        [June]\r\n    FROM (SELECT FiscalYear,\r\n            BillingMonth,\r\n            CurrentAccountNumber,\r\n\t\t\tUtilityServiceAddress,\r\n            AccountEnergyUsage/NULLIF((AccountDemandUsage * BillingPeriodDays * 24), 0) AS LoadFactor\r\n        FROM Published.TemporalAccountLevelRawDataForCurrentPeriod AS talrdfcp3\r\n        WHERE FacilitySeqid = @FacilitySeqID AND AgencyDivisionSeqid = @AgencyDivisionSeqID\r\n            AND (talrdfcp3.EffectiveStartPeriod <= @PublishedBillingPeriod AND talrdfcp3.EffectiveEndPeriod > @PublishedBillingPeriod)                                       \r\n\t\t\tAND FiscalYear BETWEEN @BeginningFiscalYear AND @CurrentFiscalYear AND (EnergySource = 1 OR EnergySource = 6)) AS SourceTable\r\n\t\tPIVOT (SUM(LoadFactor) FOR BillingMonth IN (July, [August], [September], [October], [November], [December]\r\n\t\t\t,[January], [February], [March], [April], [May],[June])) pivotTable\r\n\t\t) AS DataExport\r\n\tORDER BY DataExport.CurrentAccountNumber,\r\n        DataExport.FiscalYear DESC,\r\n\t\tDataExport.displayIndex;\r\nEND;"
        }
      ]
    }
  ]
}