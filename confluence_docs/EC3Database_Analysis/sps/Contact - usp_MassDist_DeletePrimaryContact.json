    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- alter date: <alter Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [Contact].[usp_MassDist_DeletePrimaryContact]\r\n\t@contactAddressSeqID int,\r\n\t@profileID int\r\nAS\r\nBEGIN\r\n\r\n\tdelete from Contact.MassDistributionListEmailRecipient\r\n\twhere MassDistributionProfileSeqid = @profileID and ContactAddressSeqid = @contactAddressSeqID\r\n\t\r\n\r\n\tdelete from Contact.MassDistributionListReportContext\r\n\twhere MassDistributionProfileSeqid = @profileID and ContactAddressSeqid = @contactAddressSeqID\r\n\t\r\n\r\nEND"
        }
      ]
    }
