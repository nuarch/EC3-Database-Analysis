      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_GetAccountMeterExchangeHistoryForNuenergenAccounts]\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT DISTINCT EDT.EnergyType\r\n\t\t,UC.ShortDesc AS UtilityCompany\r\n\t\t,A.UniqueAccountSeqid AS UniqueAccountSeqId\r\n\t\t,M.UniqueMeterSeqid AS UniqueMeterSeqId\r\n\t\t,A.CurrentAccountNumber\r\n\t\t,STUFF((SELECT DISTINCT ',' + A2.OriginalAccountNumber\r\n\t\t\tFROM Billing.Account AS A2\r\n\t\t\tWHERE A.CurrentAccountNumber <> A2.OriginalAccountNumber\r\n\t\t\t\tAND A.UniqueAccountSeqid = A2.UniqueAccountSeqid\r\n\t\t\tFOR XML PATH('')), 1, 1, '') AS PreviousAccountNumbers\r\n\t\t,M.CurrentMeterNumber\r\n\t\t,STUFF((SELECT DISTINCT ',' + M2.OriginalMeterNumber\r\n\t\t\tFROM Billing.Meter AS M2\r\n\t\t\tWHERE M.CurrentMeterNumber <> M2.OriginalMeterNumber\r\n\t\t\t\tAND M.UniqueMeterSeqid = M2.UniqueMeterSeqid\r\n\t\t\tFOR XML PATH('')), 1, 1, '') AS PreviousMeterNumbers\r\n\tFROM Billing.Meter AS M\r\n\t\tINNER JOIN Solar.SiteHardware AS SSH ON SSH.UniqueMeterSeqID = M.UniqueMeterSeqid\r\n\t\t\tAND SSH.DataCategory = 2\r\n\t\tINNER JOIN Billing.Account AS A ON A.UniqueAccountSeqid = M.UniqueAccountSeqID\r\n\t\tINNER JOIN Billing.UtilityCompany AS UC ON UC.UtilityCompanySeqid = A.UtilityAccountProvider\r\n\t\tINNER JOIN Billing.EnergyDeliveryType AS EDT ON EDT.EnergyDeliveryType = A.EnergyAccountDescription\r\n\tWHERE M.IsCurrentRecord ='Y'\r\n\t\tAND M.IsActive = 1\r\n\t\tAND A.IsCurrentRecord = 'Y'\r\n\t\tAND A.IsActive = 1\r\n\tGROUP BY EDT.EnergyType\r\n\t\t,UC.ShortDesc\r\n\t\t,A.UniqueAccountSeqid\r\n\t\t,M.UniqueMeterSeqid\t\t\r\n\t\t,A.CurrentAccountNumber\r\n\t\t,M.CurrentMeterNumber\r\n\t\t,M.CurrentMeterNumber;\r\n\r\n\tSELECT F.FacilityName AS [Location]\r\n\t\t,A.OriginalAccountNumber AS [Account Number]\r\n\t\t,M.CurrentMeterNumber AS [Current Meter Number]\r\n\t\t,M.OriginalMeterNumber AS [Old Meter Number]\r\n\t\t,M.MeterEffectiveOnDate AS [Period Start]\r\n\t\t,ISNULL(M.MeterEffectiveOffDate, '99991231') AS [Period End]\r\n\tFROM Billing.Account AS A\r\n\t\tINNER JOIN Billing.Meter AS M ON M.UniqueAccountSeqID = A.UniqueAccountSeqid\r\n\t\tINNER JOIN (SELECT DISTINCT UniqueMeterSeqID, DataCategory FROM Solar.SiteHardware) AS SSH\r\n\t\t\tON SSH.UniqueMeterSeqID = M.UniqueMeterSeqid AND SSH.DataCategory = 2\r\n\t\tINNER JOIN Billing.Facility AS F ON F.FacilitySeqid = A.FacilityAccount\r\n\tWHERE M.IsCurrentRecord ='Y'\r\n\t\tAND M.IsActive = 1\r\n\t\tAND A.IsCurrentRecord = 'Y'\r\n\t\tAND A.IsActive = 1\r\n\tORDER BY M.UniqueAccountSeqID\r\n\t\t,M.UniqueMeterSeqid\r\n\t\t,M.MeterEffectiveOnDate\r\n\t\t,M.MeterEffectiveOffDate\r\n\t\t,M.CurrentMeterNumber;\r\nEND;"
        }
      ]
    }
  ]
