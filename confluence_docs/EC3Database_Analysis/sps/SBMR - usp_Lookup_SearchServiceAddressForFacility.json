    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "The procedure assumes that "
                },
                {
                  "type": "text",
                  "text": "Block",
                  "marks": [
                    {
                      "type": "code"
                    }
