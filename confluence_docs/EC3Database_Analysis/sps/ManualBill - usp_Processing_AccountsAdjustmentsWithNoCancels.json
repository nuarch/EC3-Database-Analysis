      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [ManualBill].[usp_Processing_AccountsAdjustmentsWithNoCancels]\n\t\t\t\r\n\t\t   \r\n AS \r\n begin\r\n\t\t \r\n\t\t --********************************************************************************\r\n\t\t --\tAuthor: MOHAMMED BELARREM\r\n\t\t -- Description: This returns a list of adjutment transactions and their AccountBilling Couter parts to be replaced\r\n\t\t --\t\t\t\t these transactions are adjustments with no cancels\r\n\t\t --\t\t\t\t \r\n\t\t --\r\n\t\t --\tLog: \r\n\t\t --\t\tCreation 01/18/2011\r\n\t\t --******************************************************************************** \r\n\r\n\r\n\t\t\r\n\t\tSELECT Billing.InvoiceAccountGroup.InvoiceAccountBillingGroupDesc, dbo.MaskAccountNumber(Adjutment.OriginalAccountNumber, Adjutment.UtilityCompanySeqid) AS OriginalAccountNumber, Adjutment.BillingPeriod, Adjutment.BillingPeriodRevision, Adjutment.BillingDays, Adjutment.FromDate, Adjutment.ToDate, \r\n\t\t\t\t\t Adjutment.RevisedBilledAmount, Adjutment.RevisedEnergy, Adjutment.TotalRebilledAmount, Adjutment.TotalEnergy, \r\n\t\t\t\t\t Billing.AccountBilling.FromDate AS AccountBillingFromDate, Billing.AccountBilling.ToDate AS AccountBillingToDate, Billing.AccountBilling.OriginalBilledAmount, \r\n\t\t\t\t\t Billing.AccountBilling.RevisedBilledAmount AS AccountBillingRevisedBilledAmount, Billing.AccountBilling.AccountEnergyUsage, \r\n\t\t\t\t\t Billing.AccountBilling.OriginalEnergyUsage, Billing.AccountBilling.BillingPeriodDays\r\n\t\tFROM ManualBill.UploadManualBillingAccountAdjustment AS Adjutment INNER JOIN\r\n\t\t\t\t\t Billing.AccountBilling ON Adjutment.AccountBillingSeqid = Billing.AccountBilling.AccountBillingSeqid INNER JOIN\r\n\t\t\t\t\t Billing.InvoiceAccountGroup ON Billing.AccountBilling.CurrentInvoiceAccountBillingGroup = Billing.InvoiceAccountGroup.InvoiceAccountGroupSeqid\r\n\t\tWHERE    (Adjutment.AccountBillingSeqid IS NOT NULL) AND (Adjutment.CanceledBilledAmount = 0) AND (Adjutment.CanceledEnergy IS NULL)\r\n\t\tORDER BY Adjutment.InvoiceAccountGroupSeqid, Billing.AccountBilling.OriginalAccountNumber, Billing.AccountBilling.BillingPeriod\r\n\t\t \r\n\t\t \r\n\t\t \r\n\r\nend"
        }
      ]
    }
