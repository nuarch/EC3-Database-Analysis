    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "1. Overview"
        }
      ]
    },
    {
      "type": "paragraph",
      "content": [
        {
          "type": "text",
          "text": "The stored procedure "
        },
        {
          "type": "text",
          "text": "usp_Published_EnergyComparisonDashboardReport_Updated",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " is designed to generate a report for energy usage comparison across different agencies for a specified billing period. It retrieves data from various tables, calculates fiscal years, and logs report usage. The procedure outputs a dataset containing energy usage metrics, agency details, and a calculated percentage of total usage for each agency group."
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "2. Complexity Level: Medium"
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "3. Input Parameters"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@PublishedBillingPeriod AS VARCHAR(6)",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": This parameter specifies the billing period for which the report is generated. It is expected to be in a format that can be parsed as a string of six characters, likely representing a year and month (e.g., '202301' for January 2023)."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "4. Business Logic and Workflow"
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Initial Setup",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure begins by setting "
                        },
                        {
                          "type": "text",
                          "text": "NOCOUNT ON",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to prevent the message indicating the number of rows affected by a SQL statement from being returned."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It sets the transaction isolation level to "
                        },
                        {
                          "type": "text",
                          "text": "READ UNCOMMITTED",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", allowing dirty reads to improve performance by avoiding locks."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Parameter Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "If "
                        },
                        {
                          "type": "text",
                          "text": "@PublishedBillingPeriod",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " is not provided or is empty, it defaults to the maximum billing period available in the "
                        },
                        {
                          "type": "text",
                          "text": "Published.AccountLevelRawDataForCurrentPeriod",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " table."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Logging",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure logs its execution by calling "
                        },
                        {
                          "type": "text",
                          "text": "Audit.usp_AddReportUsageLog",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", capturing details such as the report name, stored procedure name, and the billing period."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Fiscal Year Calculation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It calculates the current and prior fiscal years based on the provided billing period using a user-defined function "
                        },
                        {
                          "type": "text",
                          "text": "dbo.CalculateFiscalYear",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Retrieval and Transformation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure retrieves year-to-date (YTD) and monthly energy usage data from "
                        },
                        {
                          "type": "text",
                          "text": "Published.TemporalEnergyUsageSummaryGroupByAgencyAndEnergyType",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It joins this data with agency details from "
                        },
                        {
                          "type": "text",
                          "text": "Billing.AgencyDivision",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and agency group information from "
                        },
                        {
                          "type": "text",
                          "text": "Report.AgencyGroupItem",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and "
                        },
                        {
                          "type": "text",
                          "text": "Report.AgencyGroup",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The data is filtered to include only relevant agency groups or those without a specific target report sequence ID."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Output",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The final output includes various metrics such as total usage, BTU, agency details, fiscal year, and a calculated percentage of total usage for each agency group."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The results are ordered by display order index and agency code."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Isolation Level",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Using "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can improve performance by reducing locking overhead but may lead to reading uncommitted or dirty data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Indexing",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Ensure that the tables involved in joins and where clauses are properly indexed to optimize query performance."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Aggregation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "SUM",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "GROUP BY",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can be resource-intensive, especially on large datasets. Consider optimizing these operations or using indexed views if applicable."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Function Calls",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The repeated call to "
                },
                {
                  "type": "text",
                  "text": "dbo.CalculateFiscalYear",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " could be optimized by caching results if the function is computationally expensive."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can lead to inconsistencies if the underlying data is being modified concurrently."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Parameter Validation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure assumes the billing period is in a valid format without explicit validation, which could lead to errors if incorrect data is passed."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Hardcoded Values",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The "
                },
                {
                  "type": "text",
                  "text": "CASE",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " statement for "
                },
                {
                  "type": "text",
                  "text": "PercentageOfTotalUsage",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " uses hardcoded values, which may require updates if agency group IDs or their corresponding percentages change."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": As data grows, the performance of aggregation and joins may degrade. Regular monitoring and optimization may be necessary."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dependency on External Functions",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure relies on "
                },
                {
                  "type": "text",
                  "text": "dbo.CalculateFiscalYear",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", which must be correctly implemented and performant to avoid bottlenecks."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_Published_EnergyComparisonDashboardReport_Updated]\n(\r\n\t@PublishedBillingPeriod AS VARCHAR(6)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tIF (@PublishedBillingPeriod IS NULL OR @PublishedBillingPeriod = '')\r\n\tBEGIN\r\n\t\tSELECT @PublishedBillingPeriod = MAX(BillingPeriod) FROM Published.AccountLevelRawDataForCurrentPeriod;\r\n    END;\r\n\r\n\tDECLARE @spname VARCHAR(100) = OBJECT_NAME(@@PROCID);\r\n\tEXEC [Audit].usp_AddReportUsageLog\r\n\t\t@ReportName\t\t\t\t\t= @spname,\r\n\t\t@StoredProcName\t\t\t\t= @spname,\r\n\t\t@RequestedBy\t\t\t\t= NULL,    \r\n\t\t@prmPublishedBillingPeriod\t= @PublishedBillingPeriod,\r\n\t\t@prmBillingPeriod\t\t\t= NULL,\r\n\t\t@prmAgency_ies\t\t\t\t= NULL,\r\n\t\t@prmFacilityNumber_s\t\t= NULL,\r\n\t\t@prmStartingBillingPeriod\t= NULL,\r\n\t\t@prmEndingBillingPeriod\t\t= NULL;\r\n\r\n\tDECLARE @CurrentFiscalYear AS VARCHAR(4), @PriorFiscalYear AS VARCHAR(4);\r\n\r\n\tSET @CurrentFiscalYear = dbo.CalculateFiscalYear(@PublishedBillingPeriod);\r\n\tSET @PriorFiscalYear = CAST(CAST(@CurrentFiscalYear AS INT) -1 AS VARCHAR(4));\r\n\r\n\tSELECT t.YTDTotalUsage,\r\n        t.YTDTotalBTU,\r\n        t.AgencyCodeOEC,\r\n        t.AgencyName,\r\n        t.FiscalYear,\r\n        t.MonthTotalUsage,\r\n        t.MonthTotalBTU,\r\n        t.CurrentPeriod,\r\n        t.PublishedPeriodCurrentFY,\r\n        t.PublishedPeriodPriorFY,\r\n        t.AgencyGroupSeqID,\r\n        t.AgencyGroupName,\r\n        t.DisplayOrderIndex,\r\n\t\tCASE WHEN t.AgencyGroupSeqID =14 then 24.3\r\n\t\t\tWHEN t.AgencyGroupSeqID = 22 THEN 13.7\r\n\t\t\tWHEN t.AgencyGroupSeqID = 15 THEN 12.8\r\n\t\t\tWHEN t.AgencyGroupSeqID = 10 THEN 9.2\r\n\t\t\tWHEN t.AgencyGroupSeqID = 13 THEN 6.7\r\n\t\t\tWHEN t.AgencyGroupSeqID = 19 THEN 3.9\r\n\t\t\tWHEN t.AgencyGroupSeqID = 12 THEN 3.7\r\n\t\t\tWHEN t.AgencyGroupSeqID = 20 THEN 3.2\r\n\t\t\tWHEN t.AgencyGroupSeqID = 26 THEN 2.9\r\n\t\t\tWHEN t.AgencyGroupSeqID = 9 THEN 2.7\r\n\t\t\tWHEN t.AgencyGroupSeqID = 27 THEN 2.0\r\n\t\t\tWHEN t.AgencyGroupSeqID = 18 THEN 1.8\r\n\t\t\tWHEN t.AgencyGroupSeqID = 21 THEN 1.5\r\n\t\t\tWHEN t.AgencyGroupSeqID = 16 THEN 1.0\r\n\t\t\tWHEN t.AgencyGroupSeqID = 8 THEN 0.3\r\n\t\t\tWHEN t.AgencyGroupSeqID = 11 THEN 5.6\r\n\t\t\tWHEN t.AgencyGroupSeqID = 24 THEN 1.4\r\n\t\t\tWHEN t.AgencyGroupSeqID = 25 THEN 3.1\r\n\t\t\tELSE 0 END AS PercentageOfTotalUsage\r\n\tFROM (SELECT YTDSummary.TotalUsage YTDTotalUsage,\r\n\t\t\tYTDSummary.TotalBTU AS YTDTotalBTU,\r\n\t\t\tYTDSummary.AgencyCodeOEC,\r\n\t\t\tad.AgencyShortDesc AS AgencyName,\r\n\t\t\tYTDSummary.PeriodSpan AS FiscalYear,\r\n\t\t\tMTHSummary.TotalUsage AS MonthTotalUsage,\r\n\t\t\tMTHSummary.TotalBTU AS MonthTotalBTU,\r\n\t\t\tMTHSummary.PeriodSpan AS CurrentPeriod,\r\n\t\t\t@CurrentFiscalYear AS PublishedPeriodCurrentFY,\r\n\t\t\t@PriorFiscalYear AS PublishedPeriodPriorFY,\r\n\t\t\tISNULL(ag.AgencyGroupSeqID, 25) AgencyGroupSeqID,\r\n\t\t\tISNULL(ag.AgencyGroupName, 'Other') AgencyGroupName,\r\n\t\t\tISNULL(ag.DisplayOrderIndex,19)DisplayOrderIndex\r\n\t\tFROM (SELECT SUM(TotalUsage) AS TotalUsage,\r\n\t\t\t\tSUM(TotalBTU) AS TotalBTU,\r\n\t\t\t\tAgencyCodeOEC,\r\n\t\t\t\tMAX(AgencyName) AS AgencyName,\r\n\t\t\t\tPeriodSpan\r\n\t\t\tFROM Published.TemporalEnergyUsageSummaryGroupByAgencyAndEnergyType\r\n\t\t\tWHERE TypeOfCalculation = 'YTD' AND EffectiveStartPeriod <= @PublishedBillingPeriod AND EffectiveEndPeriod > @PublishedBillingPeriod\r\n\t\t\tGROUP BY AgencyCodeOEC, PeriodSpan) AS YTDSummary\r\n\t\t\tINNER JOIN Billing.AgencyDivision AS ad ON YTDSummary.AgencyCodeOEC = ad.AgencyCodeOEC\r\n\t\t\tLEFT JOIN Report.AgencyGroupItem AS agi ON ad.AgencyDivisionSeqid = agi.AgencyDivisionSeqID\r\n\t\t\tLEFT JOIN Report.AgencyGroup AS ag ON agi.AgencyGroupSeqID = ag.AgencyGroupSeqID\r\n\t\t\tLEFT JOIN (SELECT SUM(TotalUsage) AS TotalUsage,\r\n\t\t\t\t\tSUM(TotalBTU) AS TotalBTU,\r\n\t\t\t\t\tAgencyCodeOEC,\r\n\t\t\t\t\tMAX(AgencyName) AS AgencyName,\r\n\t\t\t\t\tPeriodSpan\r\n\t\t\t\tFROM Published.TemporalEnergyUsageSummaryGroupByAgencyAndEnergyType\r\n\t\t\t\tWHERE TypeOfCalculation <> 'YTD' AND EffectiveStartPeriod <= @PublishedBillingPeriod AND EffectiveEndPeriod > @PublishedBillingPeriod\r\n\t\t\t\tGROUP BY AgencyCodeOEC, PeriodSpan) AS MTHSummary\r\n\t\t\tON YTDSummary.AgencyCodeOEC = MTHSummary.AgencyCodeOEC AND YTDSummary.PeriodSpan = dbo.CalculateFiscalYear(MTHSummary.PeriodSpan)      \r\n\t\tWHERE ag.TargetReportSeqID = '2' OR ag.TargetReportSeqID IS NULL) AS t \r\n\tORDER BY  t.DisplayOrderIndex, t.AgencyCodeOEC;\r\nEND;"
        }
      ]
    }
  ]
}