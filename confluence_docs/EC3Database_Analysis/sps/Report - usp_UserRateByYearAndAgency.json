    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_UserRateByYearAndAgency]\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT EM.UserFirstName AS [First Name]\r\n\t\t,EM.UserLastName AS [Last Name]\r\n\t\t,EM.Email\r\n\t\t,A.AgencyName AS [Agency Name]\r\n\t\t,YEAR(AEM.AuditDateTime) AS [Calendar Year]\r\n\t\t,SUM(CASE WHEN AEM.EC3MembershipAuditID IS NULL THEN 0 ELSE 1 END) AS [User Frequency]\r\n\tFROM Membership.EC3Membership AS EM\r\n\t\tINNER JOIN Billing.AgencyDivision AS A ON EM.AgencyDivisionSeqID = A.AgencyDivisionSeqid\r\n\t\tINNER JOIN Audit.AuditEC3Membership AS AEM ON AEM.Email = EM.Email\r\n\tGROUP BY EM.UserFirstName, EM.UserLastName, EM.Email, A.AgencyName, YEAR(AEM.AuditDateTime)\r\n\tORDER BY [Agency Name], EM.Email, [Calendar Year];\r\nEND;"
        }
      ]
    }
