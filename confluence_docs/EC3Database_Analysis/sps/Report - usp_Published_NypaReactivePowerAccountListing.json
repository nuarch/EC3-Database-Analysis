    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_Published_NypaReactivePowerAccountListing]\n(\r\n    @EmailAddress AS VARCHAR(75)\r\n   ,@PublishedBillingPeriod AS VARCHAR(6)\r\n   ,@AgencyCodeOEC AS VARCHAR(MAX)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n    SELECT A.AgencyCodeOEC\r\n        ,N.AgencyShortDesc\r\n        ,N.OecFacilityNumber\r\n        ,N.BillingPeriod\r\n        ,N.CurrentAccountNumber\r\n        ,N.FacilityName\r\n        ,N.Address1\r\n        ,N.UtilityServiceAddress\r\n    FROM Billing.uftn_TableGetAgencyCodeOEC(@AgencyCodeOEC, @EmailAddress) AS A\r\n        INNER JOIN Nypa.uvw_AgencyNYPAKvarAccounts AS N on A.AgencyCodeOEC = N.AgencyCodeOEC\r\n    WHERE N.RecordType = 'A' AND N.BillingPeriod = @PublishedBillingPeriod;\r\nEND;"
        }
      ]
    }
  ]
