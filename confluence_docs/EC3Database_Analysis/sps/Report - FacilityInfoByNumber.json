      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Integrity",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can lead to reading inconsistent data, which might not be suitable for all reporting needs."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Index Usage",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The trimming of "
                },
                {
                  "type": "text",
                  "text": "CityPlanningBIN",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "BIN",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " could lead to suboptimal index usage, potentially resulting in slower query performance."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Null Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The concatenation logic assumes that address components are either null or valid strings. If any component contains unexpected data, it could lead to malformed addresses."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Parameter Length",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The "
                },
                {
                  "type": "text",
                  "text": "VARCHAR(7)",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " type for "
                },
                {
                  "type": "text",
                  "text": "@OecFacilityNumber",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " assumes that all facility numbers are exactly seven characters long. If this assumption is incorrect, it could lead to errors or missed records."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[FacilityInfoByNumber]\n(\r\n\t@OecFacilityNumber AS VARCHAR(7)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSELECT F.OecFacilityNumber,\r\n\t\tF.FacilityName,\r\n\t\tCOALESCE(F.Address1, '') + COALESCE(F.Address2, '') + COALESCE(', ' + F.City, '') + COALESCE(', ' + F.[State], '') + COALESCE(', ' + F.Zipcode, '') AS [BuildingAddress],\r\n\t\tP.TotalBldgGrossSqFt,\r\n\t\tP.BIN,\r\n\t\tP.BBL,\r\n\t\tP.YearBuilt,\r\n\t\tP.BenchmarkedOn,\r\n\t\tP.EPAPMBenchmarkedAs,\r\n\t\tP.Rating\t\t\r\n\tFROM Billing.Facility AS F INNER JOIN Billing.Property AS P ON LTRIM(RTRIM(F.CityPlanningBIN)) = LTRIM(RTRIM(P.BIN))\r\n\tWHERE F.OecFacilityNumber = @OecFacilityNumber\r\n\t\tAND P.Rating IS NOT NULL;\r\nEND;"
        }
      ]
    }
  ]
}