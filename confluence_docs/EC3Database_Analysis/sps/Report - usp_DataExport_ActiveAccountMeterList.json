    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_DataExport_ActiveAccountMeterList]\n(\r\n\t@EmailAddress AS VARCHAR(75)\r\n\t,@IsAgencyUser BIT = 0 -- not in use but needed\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @spname AS VARCHAR(100) = OBJECT_NAME(@@PROCID);\r\n\r\n\tEXEC [Audit].usp_AddReportUsageLog\r\n\t\t@ReportName = @spname,\r\n\t\t@StoredProcName = @spname,\r\n\t\t@RequestedBy = @EmailAddress,\r\n\t\t@prmPublishedBillingPeriod = NULL,\r\n\t\t@prmBillingPeriod = NULL,\r\n\t\t@prmAgency_ies = NULL,\r\n\t\t@prmFacilityNumber_s = NULL,\r\n\t\t@prmStartingBillingPeriod = NULL,\r\n\t\t@prmEndingBillingPeriod = NULL;\r\n\t\r\n\tSELECT F.OecFacilityNumber AS OECBN\r\n\t\t,F.CityPlanningBIN AS BIN\r\n\t\t,F.Borough + F.[Block] + F.LotNumber AS BBL\r\n\t\t,REPLACE(REPLACE(F.Address1, CHAR(10), ' '), CHAR(13), ' ') AS AcountAddress\r\n\t\t,REPLACE(REPLACE(A.UtilityServiceAddress, CHAR(10), ' '), CHAR(13), ' ') AS MeterAddress\r\n\t\t,E.EnergyType\r\n\t\t,A.CurrentAccountNumber\r\n\t\t,CASE WHEN M.CurrentMeterNumber IS NULL THEN '1111111' ELSE M.CurrentMeterNumber END AS CurrentMeterNumber\r\n\t\t,A.AccountEffectiveTurnOn AS MeterEffectiveOnDate\r\n\t\t,U.[ShortDesc] AS UtilityCompany\r\n\t\t,F.FacilityName\r\n\t\t,AD.AgencyCodeOEC\r\n\t\t,AD.AgencyName\r\n\tFROM Billing.Account AS A\r\n\t\tINNER JOIN Billing.EnergyDeliveryType AS E ON E.EnergyDeliveryType = A.EnergyAccountDescription\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON AD.AgencyDivisionSeqid = A.AgencyAccount\r\n\t\tINNER JOIN Billing.Facility AS F ON F.FacilitySeqid = A.FacilityAccount\r\n\t\tINNER JOIN Billing.UtilityCompany AS U ON U.UtilityCompanySeqid = A.UtilityAccountProvider\r\n\t\tINNER JOIN dbo.uftn_TableGetActiveAgenciesByEmailAddress(@EmailAddress) AS AG ON AD.AgencyCodeOEC = AG.AgencyCodeOEC\r\n\t\tLEFT JOIN Billing.Meter AS M ON M.UniqueAccountSeqID = A.UniqueAccountSeqid AND M.IsCurrentRecord = 'Y' AND M.IsActive = 1\r\n\tWHERE A.IsCurrentRecord = 'Y' AND A.IsActive = 1;\r\nEND;"
        }
      ]
    }
  ]
}