      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Billing].[usp_TurnOffMeterByMeterSeqID] @MeterSeqID INT, @MeterEffectiveTurnOff VARCHAR(8), @TurnOffDate VARCHAR(6),@AuthenticatedUserID int , @Notes notes, @StatusCode INT OUTPUT, @Message VARCHAR(1000) out\n AS \r\n \r\n BEGIN\r\n--**************************************************************************************\r\n--* Description:\tmeter turn off\r\n--*\r\n--* AUTHOR:       Zafer Durmaz\t\r\n--* Created On:   2016-02-29\r\n--**************************************************************************************\r\n--* Date       Tech Description of Change\r\n--* ---------- ---\t-------------------------------------------------------------\r\n--* 2016-02-29 zd  First Version \r\n--* 2016-04-01 MB  Added AuthenticatedUserID and some validation\r\n--***************************************************************************************\r\n\r\n\r\nDECLARE @StatusCodePeriod VARCHAR(6) = ( SELECT atf.BillingPeriod FROM Billing.ApplicationTimeFrame AS atf WHERE atf.CurrentProcessingPeriod='Y' )\r\n\r\nIF @TurnOffDate IS NULL\r\nSET @TurnOffDate = ( SELECT atf.BillingPeriod FROM Billing.ApplicationTimeFrame AS atf WHERE atf.CurrentProcessingPeriod = 'Y' )\r\n\r\nIF @Notes IS NULL\r\nSET @Notes = 'Meter is turned off by DEM.'\r\n\r\nDECLARE @MeterNumber VARCHAR(11) = (SELECT CurrentMeterNumber FROM Billing.Meter WHERE MeterSeqid = @MeterSeqID)\r\n\r\n-- verify the meter exists under an active status\r\nIF ( NOT EXISTS (    \r\n\t\t\t\t\tSELECT * FROM Billing.Meter \r\n\t\t\t\t\tWHERE MeterSeqid = @MeterSeqID AND MeterStatus <> ('27') \r\n\t) )\r\nBEGIN\r\n\tSET @Message = 'The meter number ' + @MeterNumber + ' does not exist in the system under an active status. Please verify'\r\n\tSET @StatusCode = 0\r\n\treturn\r\nEND\r\n\t\t\r\n-- verify the meter status\r\nIF (  '27' =  (    SELECT MeterStatus FROM Billing.Meter WHERE MeterSeqid = @MeterSeqID ) )\r\nBEGIN\r\n\tSET @Message = 'The meter number ' + @MeterNumber + ' is already turned off. Please verify'\t\r\n\tSET @StatusCode = 0\r\n\treturn\r\nEND\r\n\r\n\r\n\r\n\r\nBEGIN TRY\r\n\tbegin transaction\r\n\r\n\t\t\t\t--  turn off meter\r\n\t\t\t\tUPDATE  Billing.Meter\r\n\t\t\t\tSET     MeterPreviousStatus\t\t= MeterStatus\r\n\t\t\t\t,       MeterStatus\t\t\t\t= '27'\r\n\t\t\t\t,       MeterStatusCodePeriod\t= @StatusCodePeriod\r\n\t\t\t\t,       MeterEffectiveOffDate\t= @MeterEffectiveTurnOff\r\n\t\t\t\t,       TurnOffDate\t\t\t\t= @TurnOffDate\r\n\t\t\t\t,       LastUpdate\t\t\t\t= GETDATE()\r\n\t\t\t\t,\t\tAuthenticatedUserID\t\t= @AuthenticatedUserID\r\n\t\t\t\t,       Notes\t\t\t\t\t= @Notes\r\n\t\t\t\t,       FireAuditTrigger\t\t= 'Y'\r\n\t\t\t\tWHERE   MeterSeqid\t\t\t\t= @MeterSeqID\r\n\r\n\t\t\t\t-- Update meter track\r\n\t\t\t\tUPDATE       Billing.AccountExchangeMeterTrack\r\n\t\t\t\tSET\r\n\t\t\t\tMeterEffectiveBillingEndDate\t= @MeterEffectiveTurnOff,\r\n\t\t\t\tAccountEffectiveBillingEndDate\t= @MeterEffectiveTurnOff,\r\n\t\t\t\tFireAuditTrigger\t\t\t\t= 'Y',\r\n\t\t\t\tLastUpdate\t\t\t\t\t\t= GETDATE()\r\n\t\t\t\t,AuthenticatedUserID\t\t\t= @AuthenticatedUserID\r\n\t\t\t\t,Notes\t\t\t\t\t\t\t= @Notes\r\n\t\t\t\tWHERE OriginalMeterSeqid\t\t= @MeterSeqID\r\n\r\n\t\t\t\tSET @StatusCode = 1\r\n\t\t\t\tSET @Message = ''\r\n\r\n\tCOMMIT TRANSACTION;\r\nEND TRY\r\n\r\nBEGIN CATCH\r\n\t\tDECLARE @ErrorMessage NVARCHAR(4000);\r\n\t\tDECLARE @ErrorSeverity INT;\r\n\t\tDECLARE @ErrorState INT;\r\n \t\tDECLARE @ErrorNumber INT;\r\n\t\tDECLARE @ErrorLine INT;\r\n \t\tDECLARE @ErrorProcedure NVARCHAR(126);\r\n\t\t--\r\n\t\tSELECT \r\n\t\t\t@ErrorMessage ='Please contact IT with a screen shot of this error message! ' + ERROR_MESSAGE(),\r\n\t\t\t@ErrorSeverity = ERROR_SEVERITY(),\r\n\t\t\t@ErrorState = ERROR_STATE(),\r\n\t\t\t@ErrorNumber = ERROR_NUMBER(),\r\n\t\t\t@ErrorProcedure = ERROR_PROCEDURE(),\r\n\t\t\t@ErrorLine = ERROR_LINE();\t\t\t\r\n\t\t--\r\n\t\tRAISERROR (@ErrorMessage, -- Message text.\r\n\t\t\t\t\t@ErrorSeverity, -- Severity.\r\n\t\t\t\t\t@ErrorState, -- State.\r\n\t\t\t\t\t@ErrorProcedure,\r\n\t\t\t\t\t@ErrorLine\r\n\t\t\t\t\t) WITH LOG;\r\n\t\t\t\t\t\t\r\n\t\tSET @StatusCode  = 0\r\n\t\tSET @Message = @ErrorMessage\t\t\t\t\t\t   \r\n\tROLLBACK TRAN;\r\n\t\t\t\t\t\t\t\t   \r\nEND CATCH;\r\n\r\nEND"
        }
      ]
    }
  ]
}