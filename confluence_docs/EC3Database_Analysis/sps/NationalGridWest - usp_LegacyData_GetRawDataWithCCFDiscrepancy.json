    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [NationalGridWest].[usp_LegacyData_GetRawDataWithCCFDiscrepancy]\nAS\r\nBEGIN\r\n\r\n\tSELECT \r\n\t\t[BillingPeriod]\r\n\t\t,dbo.KeyspanConvertLegacyAccountToEC3Number(dbo.KeyspanConvertCancellationAccountNumber([KeyspanOldAccountNumber],BilledAmount)) as EC3AccountNumber\r\n\t\t,MeterNumber\r\n\t\t,dbo.ConvertOverpunch(FromMeterReading) AS FromMeterReading\r\n\t\t,dbo.ConvertOverpunch(ToMeterReading) AS ToMeterReading\r\n\t\t,cast(dbo.ConvertOverpunch(CCF) as int) as CCF\r\n\t\t,dbo.ConvertOverpunch9ToMoney(BilledAmount) as BilledAmount\r\n\t\t,cast(dbo.ConvertOverpunch(Therms) as int) as Therms\r\n\r\n\t\t,[UtilityServiceAccountName] as FacilityName\r\n\t\t,[UtilityServiceAddress] as ServiceAddress\r\n\t\t,dbo.MMDDYKeyspanToYYYYMMDD(FromDateMMDDY) as FromDate\r\n\t\t,dbo.MMDDYKeyspanToYYYYMMDD(ToDateMMDDY) as ToDate\r\n\t\t,cast(dbo.ConvertOverpunch(NumberOfBillingDays) as int) as NumberOfBillingDays\r\n\t\t\r\n\t\t,cast(dbo.ConvertOverpunch5ToDecimal(ThermsFactor) as numeric(6,4)) as ThermsFactor\r\n\t\t,cast(NumberOfDials as int) as [Dials]\r\n\t\t,(case when EstimatedOrActualBillingCode = 'ES' then 'EST' else 'ACT' end)\r\n\tFROm \t\t\t\t\t\t\r\n\t\tNationalGridWest.UploadLegacyNationalGridWestDataPreload\r\n\tWHERE \r\n\t\tdbo.KeyspanConvertLegacyAccountToEC3Number(dbo.KeyspanConvertCancellationAccountNumber([KeyspanOldAccountNumber],BilledAmount))\r\n\t\tIN(\r\n\t\t\tSELECT  acc.OriginalAccountNumber\r\n\t\t\tFROM \r\n\t\t\t( SELECT    SUM(CancelCCF) AS totalCancelCCF, SUM(ccf) AS totalCCF,\r\n\t\t\t\t\t\tOriginalAccountNumber\r\n\t\t\t  FROM   NationalGridWest.UploadLegacyKeyspanWestMeterBillingAdjustmentGas\r\n\t\t\t  GROUP BY  OriginalAccountNumber\r\n\t\t\t) AS met\r\n\t\t\tINNER JOIN \r\n\t\t\t( SELECT SUM(CancelTotalCCF) AS totalCancelCCF, SUM(totalCCF) AS totalCCF,\r\n\t\t\t\t\t\tOriginalAccountNumber\r\n\t\t\t\t FROM NationalGridWest.UploadLegacyKeyspanWestAccountBillingAdjustmentGas\r\n\t\t\t\t GROUP BY OriginalAccountNumber\r\n\t\t\t) AS acc \r\n\t\t\tON \r\n\t\t\tmet.OriginalAccountNumber = acc.OriginalAccountNumber\r\n\t\t\tAND \r\n\t\t\t(\tmet.totalCCF <> acc.totalCCF \r\n\t\t\t\tOR\r\n\t\t\t\tmet.totalCancelCCF <> acc.totalCancelCCF \r\n\t\t\t)\r\n\t\t)\r\n\r\nEND"
        }
      ]
    }
  ]
