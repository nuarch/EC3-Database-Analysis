                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " for document type information."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "Scheduler.DistributionReportArchive",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " for archived report linkage."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "Contact.ContactAddress",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " for user email addresses."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "Scheduler.DistributionReport",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and "
                        },
                        {
                          "type": "text",
                          "text": "Scheduler.DistributionReportBakup",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " for report descriptions."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Ordering",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The results are ordered by email address and document name."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED Isolation Level",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": While this can improve performance by reducing locking, it may lead to reading inconsistent data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Joins and Indexing",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure involves multiple joins, which can be resource-intensive. Proper indexing on join columns can help improve performance."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Volume",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "DISTINCT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can be costly in terms of performance, especially with large datasets, as it requires sorting and deduplication."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can lead to dirty reads, where the procedure might retrieve data that is not yet committed and could be rolled back."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Null Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": If the "
                },
                {
                  "type": "text",
                  "text": "Billing.ApplicationTimeFrame",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " table does not have a row with "
                },
                {
                  "type": "text",
                  "text": "CurrentProcessingPeriod = 'Y'",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", the procedure may fail to set a valid billing period."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Consistency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The reliance on "
                },
                {
                  "type": "text",
                  "text": "LEFT JOIN",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " for report descriptions means that if both "
                },
                {
                  "type": "text",
                  "text": "Scheduler.DistributionReport",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "Scheduler.DistributionReportBakup",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " have entries, the procedure will prioritize the former, which may not always be the desired behavior."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": As the number of records grows, the performance of the procedure may degrade due to the complexity of the joins and the use of "
                },
                {
                  "type": "text",
                  "text": "DISTINCT",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Scheduler].[usp_GetArchivedReportListForSelectedAgencyAccessFolder]\n(\r\n\t@BillingPeriod AS VARCHAR(6) = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\t\r\n\tIF(@BillingPeriod IS NULL)\r\n\tBEGIN\r\n\t\tSELECT @BillingPeriod = BillingPeriod FROM Billing.ApplicationTimeFrame\r\n\t\tWHERE CurrentProcessingPeriod = 'Y';\r\n\tEND;\r\n\t\r\n\tSELECT DISTINCT doc.DocumentEndOfMonthSeqid\r\n        ,doc.UserId\r\n\t\t,ad.EmailAddress\r\n        ,doc.FiscalYear\r\n        ,doc.BillingPeriod\r\n        ,doc.AgencyAccessFolder\r\n        ,doc.DocumentName\r\n        ,doc.DocumentByteCount/1000 AS DocumentKBCount\r\n        ,doc.DocumentType\r\n        ,dt.FileExtension\r\n        ,doc.AuthenticatedUserID\r\n        ,doc.DocumentName AS ReportName\r\n        ,ISNULL(report.ReportDescription, reportb.ReportDescription) AS ReportDescription\r\n        ,doc.DateAdded\r\n        ,dt.DocumentTypeName\r\n\tFROM ContentManagement.DocumentEndOfMonth AS doc\r\n\t\tINNER JOIN ContentManagement.DocumentType AS dt ON dt.DocumentTypeID = doc.DocumentType\r\n\t\tINNER JOIN Scheduler.DistributionReportArchive AS archive ON doc.DocumentEndOfMonthSeqid = archive.documentEndOfMonthID\r\n\t\tINNER JOIN Contact.ContactAddress AS ad ON archive.contactAddressID = ad.ContactAddressSeqid\r\n\t\tLEFT JOIN Scheduler.DistributionReport AS report ON archive.DistributionReportID = report.DistributionReportID\r\n\t\tLEFT JOIN Scheduler.DistributionReportBakup AS reportb ON archive.DistributionReportID = reportb.DistributionReportID\r\n\tWHERE doc.BillingPeriod = @BillingPeriod\r\n\tORDER BY ad.EmailAddress, doc.DocumentName;\r\nEND;"
        }
      ]
    }
  ]
}