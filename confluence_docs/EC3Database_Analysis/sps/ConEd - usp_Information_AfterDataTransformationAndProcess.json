        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\tSummary data after transformation\r\n-- =============================================\r\nCREATE PROCEDURE [ConEd].[usp_Information_AfterDataTransformationAndProcess]\r\nAS\r\nBEGIN\r\n\r\n\t/*\r\n\tUpload Data Summary\r\n\t*/\r\n\tSELECT\r\n\t\t'Upload Data Summary' AS DataType,     \r\n\t\tBillingPeriod, CASE WHEN BillingPeriod = BillingPeriodRevision THEN 'Original Account Billing' ELSE 'Adjustment Account Billing' END AS description,\r\n\t\tSUM(TotalRebilledAmount) AS TotalRebilledAmount, MIN(ThermsFactor) AS ThermsFactor, SUM(TotalCanceledAmount) AS TotalCanceledAmount, \r\n\t\tSUM(TotalRebilledAmount + TotalCanceledAmount) AS NetBilledAmount, SUM(TotalTherms) AS TotalRebilledTherms, \r\n\t\tSUM(CAST(ROUND(ThermsFactor * TotalCCF, 0) AS int)) AS DerivedRebilledTherms, SUM(ISNULL(CancelTotalTherms, 0)) AS CancelTotalTherms, \r\n\t\tSUM(TotalTherms + ISNULL(CancelTotalTherms, 0)) AS NetTherms, \r\n\t\tSUM(TotalCCF) AS TotalRebilledCCF, SUM(CAST(ROUND(case when Isnull(ThermsFactor,0) = 0 then 0 else TotalTherms/ThermsFactor end , 0) AS int)) AS DerivedRebilledCCF,\r\n\t\tSUM(ISNULL(CancelTotalCCF, 0)) \r\n\t\tAS CancelTotalCCF, SUM(TotalCCF + ISNULL(CancelTotalCCF, 0)) AS NetCCF\r\n\tFROM ConEd.UploadConEdisonAccountBillingAdjustmentGas\r\n\tGROUP BY BillingPeriod, CASE WHEN BillingPeriod = BillingPeriodRevision THEN 'Original Account Billing' ELSE 'Adjustment Account Billing' END\r\n\tUNION ALL\r\n\tSELECT\r\n\t\t\t'Upload Data Summary' AS DataType,         \r\n\t\t\tBillingPeriod, MIN('Total Account Billing') AS description, \r\n\t\t\tSUM(TotalRebilledAmount) AS TotalRebilledAmount, MIN(ThermsFactor) AS ThermsFactor, SUM(TotalCanceledAmount) AS TotalCanceledAmount, \r\n\t\t\tSUM(TotalRebilledAmount + TotalCanceledAmount) AS NetBilledAmount, SUM(TotalTherms) AS TotalRebilledTherms, \r\n\t\t\tSUM(CAST(ROUND(ThermsFactor * TotalCCF, 0) AS int)) AS DerivedRebilledTherms, SUM(ISNULL(CancelTotalTherms, 0)) AS CancelTotalTherms, \r\n\t\t\tSUM(TotalTherms + ISNULL(CancelTotalTherms, 0)) AS NetTherms, \r\n\t\t\tSUM(TotalCCF) AS TotalRebilledCCF, SUM(CAST(ROUND(case when Isnull(ThermsFactor,0) = 0 then 0 else TotalTherms/ThermsFactor end , 0) AS int)) AS DerivedRebilledCCF,\r\n\t\t\tSUM(ISNULL(CancelTotalCCF, 0)) \r\n\t\t\tAS CancelTotalCCF, SUM(TotalCCF + ISNULL(CancelTotalCCF, 0)) AS NetCCF\r\n\tFROM ConEd.UploadConEdisonAccountBillingAdjustmentGas AS UploadConEdisonAccountBillingAdjustmentGas_1\r\n\tGROUP BY BillingPeriod\r\n\r\n\r\nEND"
        }
      ]
