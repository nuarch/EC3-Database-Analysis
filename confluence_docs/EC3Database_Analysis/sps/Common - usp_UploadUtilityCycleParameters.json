        {
          "type": "text",
          "text": "CREATE PROCEDURE [Common].[usp_UploadUtilityCycleParameters]\n(\r\n\t\t\t@UtilityCompanySeqid int,\r\n\t\t\t@InvoiceAccountGroupSeqid INT,\r\n            @BillingPeriod yyyymm,\r\n            @FiscalYear yyyy,\r\n            @CanProcessData yesno,\r\n            @IsCurrentPeriodProcessingComplete yesno,\r\n\t\t\t@CurrentProcessingPeriodCompleted yyyymmdd,\r\n            @BillCreationDate yyyymmdd,\r\n            @PostMarkDate yyyymmdd,\r\n            @DatePaymentDue yyyymmdd,\r\n            @DateAcceptanceIntoOEC yyyymmdd,\r\n\t\t\t@UtilityComapnyFolderName varchar(20),\r\n            @AuthenticatedUserID int,\r\n\t\t\t@StatusCode int output\r\n)\r\nAS\r\n\r\ndeclare @UtililtyCompanyCycleParametersExist int \r\ndeclare\t@CorrectInvoiceAccountGroup int \r\n\r\n-- check if the InvoiceAccountGroup corresponds to the UtilityCompany\r\n\r\nSELECT @CorrectInvoiceAccountGroup = COUNT(*) \r\nFROM Billing.InvoiceAccountGroup \r\nWHERE (InvoiceAccountGroupSeqid = @InvoiceAccountGroupSeqid) AND (InvoiceAccountGroupUtiliyCompany = @UtilityCompanySeqid)\r\n\r\nIF ( @CorrectInvoiceAccountGroup = 1 )\r\n\tBEGIN\r\n\r\n\t\t-- Check if the parameters exist\r\n\r\n\t\tselect\t@UtililtyCompanyCycleParametersExist = count(*)\r\n\t\tfrom\tUploadCycleParameters\r\n\t\twhere\t\r\n\t\t\t\t(UtilityCompanySeqid = @UtilityCompanySeqid) AND (InvoiceAccountGroupSeqid = @InvoiceAccountGroupSeqid ) and\r\n\t\t\t\t(BillingPeriod =@BillingPeriod) --AND (CanProcessData = @CanProcessData)\r\n\t\t--\r\n\t\tif (@UtililtyCompanyCycleParametersExist = 0 )--AND @CorrectInvoiceAccountGroup = 1)\r\n\r\n\t\t   Begin\r\n\t\t   --  If the parameters do not exist, Insert\r\n\r\n\t\t\tINSERT INTO Common.UploadCycleParameters\r\n\t\t\t\t\t(UtilityCompanySeqid, InvoiceAccountGroupSeqid, BillingPeriod, FiscalYear, CanProcessData, IsCurrentPeriodProcessingComplete, CurrentProcessingPeriodCompleted, \r\n\t\t\t\t\tBillCreationDate, PostMarkDate, DatePaymentDue, DateAcceptanceIntoOEC, UtilityComapnyFolderName, AuthenticatedUserID, DateAdded, LastUpdate)\r\n\t\t\t\t\tVALUES     (            \r\n\t\t\t\t\t@UtilityCompanySeqid,\r\n\t\t\t\t\t@InvoiceAccountGroupSeqid, \r\n\t\t\t\t\t@BillingPeriod, \r\n\t\t\t\t\t@FiscalYear, \r\n\t\t\t\t\t@CanProcessData, \r\n\t\t\t\t\t@IsCurrentPeriodProcessingComplete,\r\n\t\t\t\t\t@CurrentProcessingPeriodCompleted, \r\n\t\t\t\t\t@BillCreationDate, \r\n\t\t\t\t\t@PostMarkDate, \r\n\t\t\t\t\t@DatePaymentDue, \r\n\t\t\t\t\t@DateAcceptanceIntoOEC, \r\n\t\t\t\t\t@UtilityComapnyFolderName,\r\n\t\t\t\t\t@AuthenticatedUserID,\r\n\t\t\t\t\tGETDATE(),\r\n\t\t\t\t\tGETDATE()\t)\r\n\t\t\t\t\t\r\n\t\t\t-- SET @StatusCode = 0 -- success \t\t\r\n\t\t   End\r\n\r\n\t\telse\r\n\r\n\t\t   Begin\r\n\t\t   --  If the parameters do exist, Update\r\n\r\n\t\t\tUPDATE Common.UploadCycleParameters \r\n\t\t\tSET \r\n\t\t\t\t\tUtilityCompanySeqid=@UtilityCompanySeqid, \r\n\t\t\t\t\tInvoiceAccountGroupSeqid=@InvoiceAccountGroupSeqid, \r\n\t\t\t\t\tBillingPeriod=@BillingPeriod, \r\n\t\t\t\t\tFiscalYear=@FiscalYear, \r\n\t\t\t\t\tCanProcessData=@CanProcessData, \r\n\t\t\t\t\tIsCurrentPeriodProcessingComplete=@IsCurrentPeriodProcessingComplete,\r\n\t\t\t\t\tCurrentProcessingPeriodCompleted=@CurrentProcessingPeriodCompleted, \r\n\t\t\t\t\tBillCreationDate=@BillCreationDate, \r\n\t\t\t\t\tPostMarkDate=@PostMarkDate, \r\n\t\t\t\t\tDatePaymentDue=@DatePaymentDue, \r\n\t\t\t\t\tDateAcceptanceIntoOEC=@DateAcceptanceIntoOEC, \r\n\t\t\t\t\tUtilityComapnyFolderName=@UtilityComapnyFolderName,\r\n\t\t\t\t\tAuthenticatedUserID=@AuthenticatedUserID,\r\n\t\t\t\t\tLastUpdate=GETDATE()\r\n\t\t\t\t\tWHERE  (UtilityCompanySeqid = @UtilityCompanySeqid) AND (InvoiceAccountGroupSeqid = @InvoiceAccountGroupSeqid ) AND (BillingPeriod =@BillingPeriod) --AND (CanProcessData = @CanProcessData)\r\n\t\t   END\r\n\t\t   \r\n\t\t   SET @StatusCode = 0 -- success \t\r\n\tEND\r\n\r\nELSE\r\n\tBEGIN\r\n\t\tPRINT 'Utility company does not match the invoice billing group'\r\n\t\tSET @StatusCode = 9 -- success \t\r\n\tEND"
        }
      ]
    }
  ]
