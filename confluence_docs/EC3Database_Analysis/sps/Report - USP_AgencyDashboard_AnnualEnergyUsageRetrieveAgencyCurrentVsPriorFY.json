    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "1. Overview"
        }
      ]
    },
    {
      "type": "paragraph",
      "content": [
        {
          "type": "text",
          "text": "The stored procedure "
        },
        {
          "type": "text",
          "text": "USP_AgencyDashboard_AnnualEnergyUsageRetrieveAgencyCurrentVsPriorFY",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " is designed to retrieve and compare the energy usage data for a specified agency over the current and prior fiscal years. It calculates and returns energy usage, demand usage, billed amount, and BTU (British Thermal Units) for each billing period within a specified timeframe. The procedure uses a rolling 12-month period by default if no specific period is provided."
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "2. Complexity Level: Medium"
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "3. Input Parameters"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@agencyCode AS VARCHAR(10)",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": This parameter specifies the agency code for which the energy usage data is to be retrieved. It is a mandatory parameter."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@period AS INT = NULL",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": This optional parameter defines the number of months to consider for the rolling period. If not provided, it defaults to 12 months."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "@debugMode AS BIT = NULL",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": This optional parameter is intended for debugging purposes, though it is not utilized within the procedure."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "4. Business Logic and Workflow"
        }
      ]
    },
    {
      "type": "orderedList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Initial Setup",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": "
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure begins by setting "
                        },
                        {
                          "type": "text",
                          "text": "NOCOUNT ON",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to prevent the message indicating the number of rows affected by a SQL statement from being returned."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It sets the transaction isolation level to "
                        },
                        {
                          "type": "text",
                          "text": "READ UNCOMMITTED",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " to allow dirty reads, which can improve performance by not locking the data."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Period Determination",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "If "
                        },
                        {
                          "type": "text",
                          "text": "@period",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " is not specified, it defaults to 12 months."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Agency Hierarchy Retrieval",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure retrieves the agency hierarchy for the specified "
                        },
                        {
                          "type": "text",
                          "text": "@agencyCode",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " using a user-defined function "
                        },
                        {
                          "type": "text",
                          "text": "UDF_GetAgencyHierarchy",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and stores the result in a temporary table "
                        },
                        {
                          "type": "text",
                          "text": "#tmpAgencies",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Billing Period Calculation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It determines the latest published billing period and calculates the start date for the rolling period."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "A table variable "
                        },
                        {
                          "type": "text",
                          "text": "@billingPeriodTable",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " is used to store billing periods and a flag indicating whether the period is current."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Loop for Billing Periods",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "A loop populates "
                        },
                        {
                          "type": "text",
                          "text": "@billingPeriodTable",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " with billing periods from the start date to the latest published period, including both current and prior fiscal year periods."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Data Retrieval and Aggregation",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The procedure performs a "
                        },
                        {
                          "type": "text",
                          "text": "LEFT JOIN",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " between "
                        },
                        {
                          "type": "text",
                          "text": "@billingPeriodTable",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " and a subquery that aggregates energy usage data from "
                        },
                        {
                          "type": "text",
                          "text": "TemporalAccountLevelSummaryByAgency",
                          "marks": [
                            {
                              "type": "code"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "It calculates energy usage, demand usage, billed amount, and BTU, converting them to appropriate units (e.g., dividing by 1,000,000 for energy usage)."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Output",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ":"
                }
              ]
            },
            {
              "type": "bulletList",
              "content": [
                {
                  "type": "listItem",
                  "content": [
                    {
                      "type": "paragraph",
                      "content": [
                        {
                          "type": "text",
                          "text": "The result set includes agency code, fiscal year, billing period, calendar year, month name, month-year, energy usage, demand usage, billed amount, BTU, and a flag indicating if the period is current."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "5. Performance Considerations"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": While this isolation level can improve performance by reducing locking, it may lead to reading uncommitted or dirty data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Temporary Tables and Table Variables",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of a temporary table and table variable can be efficient for small to medium datasets but may impact performance if the dataset is large."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "NOLOCK Hint",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Used in the subquery to prevent locking, which can improve performance but at the risk of reading uncommitted data."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Loop for Billing Periods",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The loop could be a performance bottleneck if the range of periods is large, though it is generally efficient for a 12-month period."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " and "
                },
                {
                  "type": "text",
                  "text": "NOLOCK",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can lead to reading uncommitted data, which may not be accurate."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Date Calculations",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The logic for incrementing billing periods assumes a specific format and may not handle edge cases or invalid data gracefully."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Debug Mode Parameter",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The "
                },
                {
                  "type": "text",
                  "text": "@debugMode",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " parameter is defined but not used, which could lead to confusion or maintenance issues."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure may face performance issues with larger datasets or if the number of agencies or billing periods increases significantly."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Error Handling",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure lacks explicit error handling, which could lead to unhandled exceptions or failures in certain scenarios."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[USP_AgencyDashboard_AnnualEnergyUsageRetrieveAgencyCurrentVsPriorFY]\n(\r\n\t@agencyCode AS VARCHAR(10)\r\n\t,@period AS INT = NULL\r\n\t,@debugMode\tAS BIT = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\t\r\n\t-- Set rolling 12 months if period is not specified\r\n\tSET @period = ISNULL(@period, 12);\r\n\r\n\t-- Get the agency hierarchy\r\n\tSELECT AgencyCodeOEC INTO #tmpAgencies FROM [Billing].[UDF_GetAgencyHierarchy](@agencyCode);\r\n\t\r\n\tDECLARE @publishedBillingPeriod AS VARCHAR(6), @startBillingPeriod AS VARCHAR(6);\r\n\tSELECT @publishedBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData WITH (NOLOCK);\r\n\r\n\tDECLARE @currentBPDate AS DATETIME, @startBPDate AS DATETIME;\r\n\tSET @currentBPDate = CONVERT(DATETIME, LEFT(@publishedBillingPeriod, 4) + '-' + RIGHT(@publishedBillingPeriod, 2) + '-01');\r\n\tSET @startBPDate = DATEADD(MM, 0 - (@period - 1), @currentBPDate);\r\n\tSET @startBillingPeriod = CONVERT(VARCHAR(12), @startBPDate, 112);\r\n\r\n\t\r\n\tDECLARE @billingPeriodTable TABLE (BillingPeriod VARCHAR(6), CurrentPeriod BIT NOT NULL DEFAULT(0));\r\n\r\n\tWHILE @startBillingPeriod <= @publishedBillingPeriod\r\n\tBEGIN\r\n\t\tINSERT INTO @billingPeriodTable (BillingPeriod, CurrentPeriod) VALUES (@startBillingPeriod, 1), (@startBillingPeriod - 100, 0);\r\n\r\n\t\tIF RIGHT(@startBillingPeriod, 2) = 12\r\n\t\t\tSET @startBillingPeriod = CONVERT(VARCHAR(4), (CONVERT(INT, LEFT(@startBillingPeriod, 4)) + 1)) + '01';\r\n\t\tELSE\r\n\t\t\tSET @startBillingPeriod = CONVERT(VARCHAR(6), CONVERT(INT, @startBillingPeriod) + 1);\r\n\tEND;\r\n\t\r\n\t-- Agency usage current and prior FY\r\n\tSELECT @agencyCode AS AgencyCodeOEC\r\n\t\t,[dbo].[CalculateFiscalYear](TBP.BillingPeriod) AS FiscalYear\r\n\t\t,TBP.BillingPeriod\r\n\t\t,SUBSTRING(TBP.BillingPeriod, 1, 4) AS CalendarYear\r\n\t\t,LEFT(DATENAME(MM, CONVERT(DATETIME, SUBSTRING(TBP.BillingPeriod, 1, 4) + '-' + SUBSTRING(TBP.BillingPeriod, 5, 2) + '-01')), 3) AS [MonthName]\r\n\t\t,LEFT(DATENAME(MM, CONVERT(DATETIME, SUBSTRING(TBP.BillingPeriod, 1, 4) + '-' + SUBSTRING(TBP.BillingPeriod, 5, 2) + '-01')), 3) + ' ' + SUBSTRING(TBP.BillingPeriod, 1, 4) AS MonthYear\r\n\t\t,ISNULL(A1.EnergyUsage, 0.00) AS EnergyUsage\r\n\t\t,ISNULL(A1.DemandUsage, 0.00) AS DemandUsage\r\n\t\t,ISNULL(A1.BilledAmount, 0.00) AS BilledAmount\r\n\t\t,ISNULL(A1.BTU, 0.00) AS BTU\r\n\t\t,TBP.CurrentPeriod\r\n\tFROM @billingPeriodTable TBP\r\n\tLEFT JOIN (\r\n\t\tSELECT ALSA.BillingPeriod\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), ALSA.EnergyUsage)) / 1000000.00) AS EnergyUsage\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), ALSA.DemandUsage)) / 1000000.00) AS DemandUsage\r\n\t\t\t,SUM(CONVERT(DECIMAL(36, 2), ALSA.BilledAmount)) AS BilledAmount\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), ALSA.BTU)) / 1000000.00) AS BTU\r\n\t\tFROM [Published].[TemporalAccountLevelSummaryByAgency] AS ALSA WITH (NOLOCK)\r\n\t\tJOIN #tmpAgencies TA ON TA.AgencyCodeOEC = ALSA.AgencyCodeOEC\r\n\t\tWHERE (ALSA.EffectiveStartPeriod <= @PublishedBillingPeriod AND ALSA.EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\tGROUP BY ALSA.BillingPeriod\r\n\t) A1 ON A1.BillingPeriod = TBP.BillingPeriod\r\n\tORDER BY FiscalYear ASC, TBP.CurrentPeriod ASC;\r\nEND;"
        }
      ]
    }
