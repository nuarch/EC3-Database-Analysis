              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Concurrency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Consider the impact on concurrent transactions. If other processes depend on the "
                },
                {
                  "type": "text",
                  "text": "URL",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " values, coordinate the execution of this procedure to minimize disruption."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [dbo].[ChangeEC3AppPointer] AS\nUPDATE  Membership.MenuItem\r\nSET     URL = REPLACE(URL, 'dcasec3prod/ec3', 'dcasec3qa/ec3_T2')\r\nWHERE   URL LIKE '%dcasec3prod%';"
        }
      ]
    }
  ]
