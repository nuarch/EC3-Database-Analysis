        {
          "type": "text",
          "text": "WHERE",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " clause with a "
        },
        {
          "type": "text",
          "text": "LIKE",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " condition are basic SQL operations."
        }
