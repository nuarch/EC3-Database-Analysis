        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Scheduler].[usp_GetDistributionContactThatNeedsMembership]\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @primaryContact TABLE (primaryContact seqid NOT NULL);\r\n\r\n\tINSERT INTO @primaryContact (primaryContact)\r\n\tSELECT DISTINCT prim.ContactAddressSeqid\r\n\tFROM Scheduler.DistributionJobDistributionProfile AS job\r\n\t\tINNER JOIN Contact.MassDistributionListReportContext AS prim ON job.DistributionProfileSeqID = prim.MassDistributionProfileSeqid\r\n\t\tINNER JOIN Contact.ContactAddress AS primAddress ON prim.ContactAddressSeqid = primAddress.ContactAddressSeqid\r\n\t\tLEFT JOIN Contact.MassDistributionListEmailRecipient AS sec ON sec.MassDistributionProfileSeqid = prim.MassDistributionProfileSeqid\r\n\t\t\tAND sec.ContactAddressSeqid = prim.ContactAddressSeqid\r\n\t\tLEFT JOIN Contact.ContactAddress secAddress ON secAddress.ContactAddressSeqid = sec.SecondaryEmailRecipient;\r\n\r\n\tINSERT INTO @primaryContact (primaryContact)\r\n\tSELECT DISTINCT CA.ContactAddressSeqid\r\n\tFROM Scheduler.DistributionJobDistributionProfile AS DJ\r\n\t\tINNER JOIN Contact.MassDistributionProfile AS MDP ON DJ.DistributionProfileSeqID = MDP.MassDistributionProfileSeqid\r\n\t\tINNER JOIN Contact.ContactProfile AS CP ON MDP.ContactProfileSeqID = CP.ContactProfileSeqid\r\n\t\tINNER JOIN Contact.ContactProfileAddress AS CPA ON CP.ContactProfileSeqid = CPA.ContactProfile\r\n\t\tINNER JOIN Contact.ContactAddress AS CA ON CPA.ContactAddress = CA.ContactAddressSeqid\r\n\tWHERE MDP.ContactProfileSeqID IS NOT NULL;\r\n\r\n\tSELECT DISTINCT ca.ContactAddressSeqid AS ContactProfileSeqId, ca.ContactFirstName, ca.ContactLastName,\r\n\t\tca.EmailAddress, ad.AgencyName, ad.AgencyCodeOEC, ad.FisaAgencyCode AS FiscalAgencyCode, ad.AgencyDivisionSeqId\r\n\tFROM Contact.ContactAddress AS ca\r\n\t\tINNER JOIN @primaryContact AS pc ON ca.ContactAddressSeqid = pc.primaryContact\r\n\t\tLEFT JOIN Billing.AgencyDivision AS ad ON ca.AgencyContact = ad.AgencyDivisionSeqid\r\n\t\tLEFT JOIN Membership.EC3Membership AS m ON ca.EmailAddress = m.Email\r\n\tWHERE m.AuthenticatedUserID IS NULL AND ad.AgencyDivisionSeqid IS NOT NULL AND ca.IsRemoved = 'N'\r\n\tORDER BY ad.AgencyCodeOEC, ca.ContactLastName, ca.ContactFirstName;\r\nEND;"
        }
      ]
    }
  ]
}