        {
          "type": "text",
          "text": "INSERT",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " operation with a "
        },
        {
          "type": "text",
          "text": "SELECT",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " statement that includes a "
        },
        {
          "type": "text",
          "text": "NOT IN",
          "marks": [
            {
              "type": "code"
            }
          ]
        },
        {
          "type": "text",
          "text": " subquery to filter out existing entries. The logic is simple and does not involve complex operations or multiple steps."
        }
      ]
