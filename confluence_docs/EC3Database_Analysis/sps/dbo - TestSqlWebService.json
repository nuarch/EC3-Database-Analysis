    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE     PROCEDURE [dbo].[TestSqlWebService] (@UtilityCompanySeqid int ,@AuthorizedUserId int)\nAS\r\nSELECT Billing.UtilityCompany.ShortDesc, Billing.Facility.OecFacilityNumber, uftn_TableGetAgencyByMemberAgencyAccessAction_1.AgencyCodeOEC, \r\n                      uftn_TableGetAgencyByMemberAgencyAccessAction_1.AgencyShortDesc, Billing.Account.CurrentAccountNumber, \r\n                      Billing.Account.OriginalAccountNumber, Billing.Account.AccountStatus, Billing.Account.AccountStatusCodePeriod\r\nFROM Billing.Account INNER JOIN\r\n                      dbo.uftn_TableGetAgencyByMemberAgencyAccessAction(@AuthorizedUserId) AS uftn_TableGetAgencyByMemberAgencyAccessAction_1 ON \r\n                      Billing.Account.AgencyAccount = uftn_TableGetAgencyByMemberAgencyAccessAction_1.AgencyDivisionSeqid INNER JOIN\r\n                      Billing.Facility ON Billing.Account.FacilityAccount = Billing.Facility.FacilitySeqid INNER JOIN\r\n                      Billing.UtilityCompany ON Billing.Account.UtilityAccountProvider = Billing.UtilityCompany.UtilityCompanySeqid\r\nWHERE (Billing.Account.UtilityAccountProvider = @UtilityCompanySeqid)"
        }
      ]
    }
