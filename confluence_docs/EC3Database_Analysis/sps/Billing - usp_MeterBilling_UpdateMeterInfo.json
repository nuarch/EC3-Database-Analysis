          "type": "text",
          "text": "CREATE  PROCEDURE [Billing].[usp_MeterBilling_UpdateMeterInfo]\n\t@idList\tdbo.UDT_IDList READONLY\r\nAS  \r\nBEGIN\r\n\r\nDECLARE  @TEMP_Meter_ID TABLE\r\n(\r\n[OriginalAccountNumber] ACCTNUM,\r\n[OriginalMeterNumber] METERNUMBER,\r\n[MeterSeqid] int\r\n)\r\n\r\nDECLARE  @TEMP_Account_ID TABLE\r\n(\r\n[OriginalAccountNumber] ACCTNUM NOT NULL,\r\n[AccountSeqid] int\r\n)\r\n\r\nINSERT INTO @TEMP_Meter_ID\r\n(\r\n    OriginalAccountNumber,\r\n    OriginalMeterNumber,\r\n    MeterSeqid\r\n)\r\nSELECT b.OriginalAccountNumber, b.OriginalMeterNumber, MAX(b.MeterSeqid) AS MeterSeqid\r\n    FROM [Billing].[MeterBilling] a JOIN billing.Meter b ON a.OriginalAccountNumber = b.OriginalAccountNumber AND a.OriginalMeterNumber=b.OriginalMeterNumber\r\n\tWHERE a.MeterBillingSeqid IN (SELECT ID FROM @idList)\r\n\tGROUP BY b.OriginalAccountNumber, b.OriginalMeterNumber;\r\n\r\nINSERT INTO @TEMP_Account_ID\r\n(\r\n    OriginalAccountNumber,\r\n    AccountSeqid\r\n)\r\nSELECT b.OriginalAccountNumber, MAX(b.AccountSeqid) AS AccountSeqid\r\n    FROM [Billing].[MeterBilling] a JOIN billing.Account b ON a.OriginalAccountNumber = b.OriginalAccountNumber\r\n\tWHERE a.MeterBillingSeqid IN (SELECT ID FROM @idList)\r\n\tGROUP BY b.OriginalAccountNumber;\r\n\r\n\r\n\r\n\tUPDATE [Billing].[MeterBilling]\r\n\tSET\tMeterBillingStatus = b.MeterStatus\r\n\t\t,PreviousMeterBillingStatus = b.MeterPreviousStatus\r\n\t\t,MeterType = b.MeterType\r\n\t\t,Tension = b.Tension\r\n\t\t,UtilityServiceAddress = b.UtilityServiceAddress\r\n\t\t,MeterDials = b.MeterDials\r\n\t\t,MeterConstant = b.MeterConstant\r\n\t\t,MeterLocation = b.UtilityAddressMeterLocation\r\n\t\t,AdjustmentRecordSeqid = b.AdjustmentRecordSeqid\r\n\t\t,AMRIndicator = b.AMRIndicator\r\n\t\t,UniqueMeterSeqId = b.UniqueMeterSeqId\r\n\tFROM [Billing].[MeterBilling] a JOIN @TEMP_Meter_ID c ON a.OriginalAccountNumber = c.OriginalAccountNumber AND a.OriginalMeterNumber=c.OriginalMeterNumber\r\n\tJOIN billing.Meter b ON c.MeterSeqid=b.MeterSeqid\r\n\tWHERE a.MeterBillingSeqid IN (SELECT ID FROM @idList);\r\n\r\n\tUPDATE [Billing].[MeterBilling]\r\n\tSET\tAccountUtilityCompanySeqid = b.UtilityAccountProvider\r\n\t\t,EnergySource = b.EnergySource\r\n\t\t,SalesType = b.SalesType\r\n\t\t,UniqueAccountSeqId = b.UniqueAccountSeqId\r\n\t\t,FireAuditTrigger = CASE WHEN a.LastPeriodModified=a.BillingPeriod THEN 'N' ELSE 'Y' END\r\n\t\t,ConsecutivePeriodsEstimatedReading=[Common].[CalculateConsecutiveNumberOfEstimatedReadings] \r\n\t\t(a.OriginalAccountNumber, a.OriginalMeterNumber, b.UtilityAccountProvider, a.BillingPeriod, b.BillingCycle, ISNULL(a.EstimatedReading, ''))\r\n\tFROM [Billing].[MeterBilling] a JOIN @TEMP_Account_ID c ON a.OriginalAccountNumber=c.OriginalAccountNumber\r\n\tJOIN billing.Account b ON c.AccountSeqid = b.AccountSeqid\r\n\tWHERE a.MeterBillingSeqid IN (SELECT ID FROM @idList);\r\n\r\n\tUPDATE [Billing].[MeterBilling]\r\n\tSET AccountExchangeMeterTrackSeqid=d.AccountExchangeMeterTrackSeqid\r\n\tFROM [Billing].[MeterBilling] a JOIN @TEMP_Account_ID b ON a.OriginalAccountNumber=b.OriginalAccountNumber\r\n\tJOIN @TEMP_Meter_ID c ON a.OriginalAccountNumber = c.OriginalAccountNumber AND a.OriginalMeterNumber=c.OriginalMeterNumber\r\n\tJOIN billing.AccountExchangeMeterTrack d ON b.AccountSeqid = d.OriginalAccountSeqid AND c.MeterSeqid = d.OriginalMeterSeqid\r\n\tWHERE a.MeterBillingSeqid IN (SELECT ID FROM @idList);\r\n\r\nEND;"
        }
