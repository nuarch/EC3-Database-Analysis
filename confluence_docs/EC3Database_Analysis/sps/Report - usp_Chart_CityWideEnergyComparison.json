      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_Chart_CityWideEnergyComparison]\nAS\r\nBEGIN\r\n\r\n\tDECLARE @CurrentFiscalYear VARCHAR(4), \r\n\t@PriorFiscalYear VARCHAR(4),\r\n\t@PublishedBillingPeriod varchar(6)\r\n\tSELECT  @PublishedBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData\r\n\tSET @PriorFiscalYear = dbo.CreateFiscalYear(@PublishedBillingPeriod,'P')\r\n\tSET @CurrentFiscalYear = dbo.CreateFiscalYear(@PublishedBillingPeriod,'C')\r\n\r\n\r\nSELECT \r\n\tFiscalYear ,\r\n\t[Month] ,\r\n\tTotalBTU,\r\n\tTotalBilledAmount\r\nFROM \r\n(\r\n\t  SELECT    'FY' + FiscalYear AS FiscalYear,\r\n\t\t\t\tSUBSTRING([Month], 1, 3) AS [Month] ,\r\n\t\t\t\tCAST(TotalBTU AS INT) AS TotalBTU,\r\n\t\t\t\tTotalBilledAmount,\r\n\t\t\t\tCASE WHEN DATEPART(mm,CAST(UPPER(SUBSTRING([Month], 1, 3)) + ' 1900' AS DATETIME)) >=7 THEN DATEPART(mm,CAST(UPPER(SUBSTRING([Month], 1, 3))+ ' 1900' AS DATETIME)) - 6 ELSE DATEPART(mm,CAST(UPPER(SUBSTRING([Month], 1, 3))+ ' 1900' AS DATETIME)) + 6 END AS SortOrder\r\n\t  FROM   Published.TemporalAccountLevelSummaryForDollarsBtusAndCo2 AS talsfdbac\r\n\t  WHERE CitywideAgencyOrFacility = 'CW'\r\n\t\t\t\tAND (EffectiveStartPeriod <= @PublishedBillingPeriod AND EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\t\t\tAND FiscalYear BETWEEN @PriorFiscalYear AND @CurrentFiscalYear\r\n\t\r\n) AS Result\r\n  ORDER BY Result.SortOrder, Result.FiscalYear\r\n  \r\nEND"
        }
      ]
    }
  ]
