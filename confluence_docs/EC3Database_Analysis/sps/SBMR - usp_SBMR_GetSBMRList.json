    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\tDerek\r\n-- Create date: 2008 April 15\r\n-- Description:\tget SBMR\r\n-- =============================================\r\nCREATE PROCEDURE [SBMR].[usp_SBMR_GetSBMRList]\r\n\r\nAS\r\nBEGIN\r\n\r\n\r\n\tselect s.SBMRHeaderSeqid,\r\n\t\ta.AgencyCodeOEC,\r\n\t\tf.OecFacilityNumber,\r\n\t\t\r\n\t\tf.City,  \r\n\t\tf.Address1 as [facilityAddress],\r\n\t\ts.SbmrReferenceNumber, \r\n\t\tu.Description as [utilityCompany],\r\n\t\tu.utilityCompanySeqid,\r\n\t\ts.AgencyDivisionSeqid,\r\n\t\ts.FacilitySeqid\r\n\tfrom SBMR.SBMRHeader as s\r\n\tLEFT JOIN Billing.Facility as f\r\n\ton s.FacilitySeqid = f.FacilitySeqid \r\n\tLEFT JOIN Billing.UtilityCompany as u\r\n\ton u.utilityCompanySeqid = s.DeliveryUtilityCompanySeqid\r\n\tLEFT JOIN Billing.AgencyDivision as a\r\n\ton a.AgencyDivisionSeqid = s.AgencyDivisionSeqid\r\n\t\r\n\r\n\t \r\nEND"
        }
      ]
