    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\t<Description,,>\r\n-- =============================================\r\nCREATE PROCEDURE [Scheduler].[usp_GetContactWhoFailedToGenerateReport] \r\n    @DistributionJobID seqid\r\nAS \r\n    BEGIN\r\n\r\n        DECLARE @primaryContact TABLE ( primaryContact seqid )\r\n\r\n        INSERT  INTO @primaryContact\r\n                ( primaryContact\r\n                )\r\n                SELECT  DISTINCT\r\n                        prim.ContactAddressSeqid\r\n                FROM Scheduler.DistributionJobDistributionProfile AS job\r\n                        INNER JOIN Contact.MassDistributionListReportContext\r\n                       AS prim ON job.DistributionProfileSeqID = prim.MassDistributionProfileSeqid\r\n                        INNER JOIN Contact.ContactAddress primAddress ON prim.ContactAddressSeqid = primAddress.ContactAddressSeqid\r\n                        LEFT JOIN Contact.MassDistributionListEmailRecipient\r\n                       AS sec ON sec.MassDistributionProfileSeqid = prim.MassDistributionProfileSeqid\r\n                                  AND sec.ContactAddressSeqid = prim.ContactAddressSeqid\r\n                        LEFT JOIN Contact.ContactAddress secAddress ON secAddress.ContactAddressSeqid = sec.SecondaryEmailRecipient\r\n                WHERE   job.JobId = @DistributionJobID\r\n                        AND primAddress.IsRemoved = 'N'\r\n\r\n        INSERT  INTO @primaryContact\r\n                ( primaryContact \r\n                )\r\n                SELECT  DISTINCT\r\n                        Contact.ContactAddress.ContactAddressSeqid\r\n                FROM Scheduler.DistributionJobDistributionProfile AS DistributionJob\r\n                        INNER JOIN Contact.MassDistributionProfile AS MassDistributionProfile ON DistributionJob.DistributionProfileSeqID = MassDistributionProfile.MassDistributionProfileSeqid\r\n                        INNER JOIN Contact.ContactProfile ON MassDistributionProfile.ContactProfileSeqID = Contact.ContactProfile.ContactProfileSeqid\r\n                        INNER JOIN Contact.ContactProfileAddress ON Contact.ContactProfile.ContactProfileSeqid = Contact.ContactProfileAddress.ContactProfile\r\n                        INNER JOIN Contact.ContactAddress ON Contact.ContactProfileAddress.ContactAddress = Contact.ContactAddress.ContactAddressSeqid\r\n                WHERE   ( DistributionJob.JobId = @DistributionJobID )\r\n                        AND ( MassDistributionProfile.ContactProfileSeqID IS NOT NULL )\r\n                        AND Contact.ContactAddress.IsRemoved = 'N'\r\n\r\n        SELECT  ca.ContactFirstName ,\r\n                ca.ContactFirstName ,\r\n                ca.ContactLastName ,\r\n                ca.EmailAddress ,\r\n                ad.AgencyName ,\r\n                CASE WHEN m.AuthenticatedUserID IS NULL\r\n                     THEN 'Missing Membership Record'\r\n                     ELSE ''\r\n                END\r\n        FROM Contact.ContactAddress AS ca\r\n                INNER JOIN @primaryContact AS pc ON ca.ContactAddressSeqid = pc.primaryContact\r\n                INNER JOIN Billing.AgencyDivision AS ad ON ca.AgencyContact = ad.AgencyDivisionSeqid\r\n                LEFT JOIN Membership.EC3Membership AS m ON ca.EmailAddress = m.Email\r\n        WHERE   ca.ContactAddressSeqid NOT IN (\r\n                SELECT DISTINCT\r\n                        a.ContactAddressID\r\n                FROM Scheduler.DistributionReport AS r\r\n                        INNER JOIN Scheduler.DistributionJob AS j ON r.DistributionJobID = j.DistributionJobID\r\n                        INNER JOIN Scheduler.DistributionReportArchive AS a ON r.DistributionReportID = a.DistributionReportID\r\n                WHERE   j.DistributionJobID = @DistributionJobID )\r\n    END"
        }
      ]
    }
  ]
