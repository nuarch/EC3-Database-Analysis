    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[usp_Published_ActiveAccountsWithNoActiveMeters]\n(\r\n\t@EmailAddress AS VARCHAR(75)\r\n\t,@PublishedBillingPeriod AS VARCHAR(6)\r\n\t,@AgencyCodeOEC AS VARCHAR(MAX)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @spname AS VARCHAR(100) = OBJECT_NAME(@@PROCID), @AgencyCode AS VARCHAR(7);\r\n\r\n\tEXEC [Audit].usp_AddReportUsageLog\r\n\t\t@ReportName\t\t\t\t\t= @spname,\r\n\t\t@StoredProcName\t\t\t\t= @spname,\r\n\t\t@RequestedBy\t\t\t\t= @EmailAddress,\r\n\t\t@prmPublishedBillingPeriod\t= @PublishedBillingPeriod,\r\n\t\t@prmBillingPeriod\t\t\t= NULL,\r\n\t\t@prmAgency_ies\t\t\t\t= @AgencyCodeOEC,\r\n\t\t@prmFacilityNumber_s\t\t= NULL,\r\n\t\t@prmStartingBillingPeriod\t= NULL,\r\n\t\t@prmEndingBillingPeriod\t\t= NULL\r\n\r\n\r\n\tIF(@AgencyCodeOEC = '*')\r\n\tBEGIN\r\n\t\tSET @AgencyCodeOEC = NULL;\r\n\tEND;\r\n\r\n\tSELECT @AgencyCode = CASE WHEN @AgencyCodeOEC IS NOT NULL THEN @AgencyCodeOEC + '%' ELSE @AgencyCodeOEC END; \r\n\r\n\tIF (@AgencyCodeOEC IS NOT NULL)\r\n\tBEGIN\r\n\t\tSELECT T.Seqid AS RawDataForCurrentPeriodSeqid\r\n\t\t\t,@PublishedBillingPeriod AS PublishedBillingPeriod\r\n\t\t\t,T.BillingPeriod\r\n\t\t\t,T.EnergyType\r\n\t\t\t,T.OecFacilityNumber\r\n\t\t\t,T.AgencyCodeOEC\r\n\t\t\t,T.AgencyName\r\n\t\t\t,T.Address1\r\n\t\t\t,T.CurrentAccountNumber\r\n\t\t\t,T.OriginalAccountNumber\r\n\t\t\t,T.FacilityName\r\n\t\t\t,T.Borough\r\n\t\t\t,T.[Block]\r\n\t\t\t,T.LotNumber\r\n\t\t\t,T.CityPlanningBIN\r\n\t\t\t,T.AccountStatus\r\n\t\t\t,T.UtilityServiceAddress\r\n\t\t\t,T.AccountEffectiveTurnOn\r\n\t\t\t,T.UtilityCompany\r\n\t\tFROM Billing.uftn_TableGetAgencyChildrenByAgencyCodeOEC(@AgencyCodeOEC, @EmailAddress) AS A\r\n\t\t\tINNER JOIN Published.TemporalAccountLevelRawDataForCurrentPeriod AS T ON A.AgencyCodeOEC = T.AgencyCodeOEC\r\n\t\tWHERE (T.EffectiveStartPeriod <= @PublishedBillingPeriod AND T.EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\t\tAND T.AccountStatus = '27';\r\n\tEND;\r\n\tELSE\r\n\tBEGIN\r\n\t\tSELECT T.Seqid AS RawDataForCurrentPeriodSeqid\r\n\t\t\t,@PublishedBillingPeriod AS PublishedBillingPeriod\r\n\t\t\t,T.BillingPeriod\r\n\t\t\t,T.EnergyType\r\n\t\t\t,T.OecFacilityNumber\r\n\t\t\t,T.AgencyCodeOEC\r\n\t\t\t,T.AgencyName\r\n\t\t\t,T.Address1\r\n\t\t\t,T.CurrentAccountNumber\r\n\t\t\t,T.OriginalAccountNumber\r\n\t\t\t,T.FacilityName\r\n\t\t\t,T.Borough\r\n\t\t\t,T.[Block]\r\n\t\t\t,T.LotNumber\r\n\t\t\t,T.CityPlanningBIN\r\n\t\t\t,T.AccountStatus\r\n\t\t\t,T.UtilityServiceAddress\r\n\t\t\t,T.AccountEffectiveTurnOn\r\n\t\t\t,T.UtilityCompany\r\n\t\tFROM Billing.uftn_TableGetAgencyChildrenByAgencyCodeOEC(@AgencyCodeOEC, @EmailAddress) AS A\r\n\t\t\tINNER JOIN Published.TemporalAccountLevelRawDataForCurrentPeriod AS T ON A.AgencyCodeOEC = T.AgencyCodeOEC\r\n\t\tWHERE (T.EffectiveStartPeriod <= @PublishedBillingPeriod AND T.EffectiveEndPeriod > @PublishedBillingPeriod)\r\n\t\t\tAND T.AccountStatus = '27';\r\n\tEND;\r\nEND;"
        }
      ]
    }
  ]
}