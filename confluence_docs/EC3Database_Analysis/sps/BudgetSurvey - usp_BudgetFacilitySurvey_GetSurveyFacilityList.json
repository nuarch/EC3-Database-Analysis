    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [BudgetSurvey].[usp_BudgetFacilitySurvey_GetSurveyFacilityList]\n(\r\n\t@Email AS VARCHAR(75)\r\n)\r\nAS \r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\t\r\n    SELECT DISTINCT AD.AgencyDivisionSeqid AS AgencyDivisionSeqId\r\n\t\t,F.FacilitySeqid AS FacilitySeqId\t\t\r\n\t\t,AFS.AgencyFacilitySurveyId\r\n\t\t,CONCAT(F.OecFacilityNumber, ' - ', F.FacilityName, ' (', ISNULL(F.Address1, ''), ')') AS Facility\r\n\t\t,CONCAT(AD.AgencyCodeOEC, ' - ', AD.AgencyName ,' (', AD.AgencyShortDesc, ')') AS Agency\r\n        ,SAT.[Name] AS ActionType\r\n\t\t,SAT.[Description] AS ActionTypedDescription        \r\n    FROM Billing.Facility AS F\r\n        INNER JOIN Billing.Account AS A ON F.FacilitySeqid = A.FacilityAccount\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON AD.AgencyDivisionSeqid = A.AgencyAccount\r\n        INNER JOIN Billing.uftn_TableGetAgencyChildrenByAgencyCodeOEC('*', @Email) AS FAD ON A.AgencyAccount = FAD.AgencyDivisionSeqID\r\n        LEFT JOIN BudgetSurvey.AgencyFacilitySurvey AS AFS ON F.FacilitySeqid = AFS.FacilitySeqId AND AD.AgencyDivisionSeqid = AFS.AgencyDivisionSeqId\r\n        LEFT JOIN BudgetSurvey.ActionType AS SAT ON AFS.ActionTypeId = SAT.ActionTypeId AND SAT.ActionTypeId = 3\r\n\t\tLEFT JOIN BudgetSurvey.AgencyBudgetFacilitySurveyAttribute AS AFSA ON AFSA.FiscalYear = AFS.FiscalYear\r\n\tWHERE A.IsCurrentRecord = 'Y' AND A.IsActive = 1;\r\nEND;"
        }
      ]
    }
  ]
}