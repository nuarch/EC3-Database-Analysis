    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Nypa].[usp_PreProcessingMatchCoverLetterToEfileCoversheet] @TotalCicMonthlyPaymentAmount money , @DcasWindCharges money , @CunyWindCharges money ,@Shorepowercredit MONEY, @AuthenticatedUserId int , @Status int  OUTPUT\n   \r\n AS --**************************************************************************************\r\n--* Name:         \r\n--*\r\n--* Description:  Correct all of the account and meters that are in error and do not create an\r\n--*\t\t\t\t  Exchange (46) record where the CreateExchangeRecord is equal to \"N\"o.\r\n--*               \r\n--*\r\n--* Database:     OEC\r\n--*\r\n--* Return:\t        0  Success\r\n--*                 1 Failures\r\n--*\r\n--* AUTHOR:      Peter Heller\r\n--* Created On:   10/2/2009\r\n--**************************************************************************************\r\n--* Date         Tech Description of Change\r\n--* ---------- ----  -------------------------------------------------------------\r\n--* 03/25/2009 PAH  Corrected Schema name  for store proc or table\r\n--* 06/30/2016 ZD\tShorepower updates, shore power credits insertion MiscellaneousCharges set to N\r\n--***************************************************************************************\r\n--\r\n-- This proceedure will insert NYPA missing accounts and meters as a 46 exchange.\r\n--\r\ndeclare @NypaCoverSheetDescription  varchar(35)\r\ndeclare @ApplicationTimeFrameBillingPeriod  varchar(6)\r\ndeclare @BillingPeriod  varchar(6)\r\ndeclare @CicLoanCount varchar(4)\r\ndeclare @SumMonthlyPaymentAmount money\r\ndeclare @RevisedCustomerTotal money\r\n--\r\nset @Status = 0\r\n--\r\nDELETE FROM Nypa.UploadNYPACoverSheet WHERE (CoverSheetRecordType IN ('19', '20', '21'))\r\n--\r\nSELECT @ApplicationTimeFrameBillingPeriod = BillingPeriod from Billing.ApplicationTimeFrame where (CurrentProcessingPeriod = 'Y')\r\n--\r\nSELECT    \r\n\t\t@BillingPeriod = BillingPeriod, \r\n\t\t@NypaCoverSheetDescription = cast(DATENAME(month, BillingPeriod+'01') + ' '+DATENAME(year, BillingPeriod+'01')+' - SENY CIC Charges' as varchar(35)),\r\n\t\t@SumMonthlyPaymentAmount=sum(MonthlyPaymentAmount)\r\nFROM \r\n\t\tNypa.UploadNYPAEncoreMonthlyBilling\r\ngroup by BillingPeriod, DATENAME(month, BillingPeriod+'01')+','+DATENAME(year, BillingPeriod+'01')\r\n\r\nIF (@@ROWCOUNT =0) SET @NypaCoverSheetDescription = cast(DATENAME(month, @ApplicationTimeFrameBillingPeriod+'01') + ' '+DATENAME(year, @ApplicationTimeFrameBillingPeriod+'01')+' - SENY CIC Charges' as varchar(35))\r\n--\r\nif (@ApplicationTimeFrameBillingPeriod<>@BillingPeriod) set @Status = 1\r\nif (@TotalCicMonthlyPaymentAmount<>@SumMonthlyPaymentAmount) set @Status = @Status + 2\r\n\r\n--\r\nINSERT INTO Nypa.UploadNYPACoverSheet\r\n(MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID)\r\nVALUES     ('N',7,1,@ApplicationTimeFrameBillingPeriod ,'19',@NypaCoverSheetDescription,@TotalCicMonthlyPaymentAmount,@AuthenticatedUserId)\r\n\r\nINSERT INTO Nypa.UploadNYPACoverSheet\r\n(MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID)\r\nVALUES     ('Y',7,1,@ApplicationTimeFrameBillingPeriod ,'20','CUNY Horizon Wind Charges',@CunyWindCharges,@AuthenticatedUserId)\r\n\r\nINSERT INTO Nypa.UploadNYPACoverSheet\r\n(MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID)\r\nVALUES     ('Y',7,1,@ApplicationTimeFrameBillingPeriod ,'21','DCAS Horizon Wind Charges',@DcasWindCharges,@AuthenticatedUserId)\r\n\r\nINSERT INTO Nypa.UploadNYPACoverSheet\r\n(MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID)\r\nVALUES     ('N',7,1,@ApplicationTimeFrameBillingPeriod ,'90','Shore Power Credit',@Shorepowercredit,@AuthenticatedUserId)\r\n\r\n\r\n\r\nSELECT MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID  FROM Nypa.UploadNYPACoverSheet\r\n\r\nSELECT @RevisedCustomerTotal = sum(TotalDollars)\r\nFROM Nypa.UploadNYPACoverSheet\r\nwhere 17 < cast(CoverSheetRecordType as int) and cast(CoverSheetRecordType as int) < 99\r\n\r\n\r\nUPDATE    Nypa.UploadNYPACoverSheet\r\nSET              TotalDollars =@RevisedCustomerTotal,MiscellaneousCharges='N', AuthenticatedUserID =@AuthenticatedUserId\r\nWHERE (CoverSheetRecordType = '99')\r\n\r\nSELECT MiscellaneousCharges,CoverSheetUtilityCompany, CoverSheetAccountBillingGroup, BillingPeriod, CoverSheetRecordType, CoverSheetRecordDesc, TotalDollars,AuthenticatedUserID  FROM Nypa.UploadNYPACoverSheet\r\norder by cast(CoverSheetRecordType as int)"
        }
      ]
    }
  ]
}