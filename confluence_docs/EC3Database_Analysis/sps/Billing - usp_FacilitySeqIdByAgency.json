    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "6. Potential Issues or Risks"
        }
      ]
    },
    {
      "type": "bulletList",
      "content": [
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Dirty Reads",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The use of "
                },
                {
                  "type": "text",
                  "text": "READ UNCOMMITTED",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " can result in reading uncommitted or inconsistent data, which may not be suitable for all business scenarios."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "String Length Assumptions",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The procedure assumes specific lengths for address and facility name truncation, which may not align with actual data lengths, potentially leading to unexpected results."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Scalability",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": As the number of records grows, the performance of string manipulation and joins may degrade if not properly indexed or optimized."
                }
              ]
            }
          ]
        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Function Dependency",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": The reliance on the user-defined function means any changes to its logic or performance can directly impact this procedure."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Billing].[usp_FacilitySeqIdByAgency]\n(\r\n\t@UserEmailAddress AS emailaddr,\r\n\t@AgencyCodeOEC AS VARCHAR(500)\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRAN ISOLATION LEVEL READ UNCOMMITTED;\r\n    \r\n\t SELECT DISTINCT\r\n\t\tF.FacilitySeqid AS Id,\r\n\t\t'(' + F.OecFacilityNumber + ') ' + LTRIM(RTRIM(SUBSTRING(F.Address1, 0, 30)  + CASE WHEN F.FacilityName like '%unassigned%' THEN '' ELSE '-' + SUBSTRING(F.FacilityName, 0, 25) END)) AS [Name]\r\n\t FROM Billing.uftn_TableGetAgencyChildrenByAgencyCodeOEC(@AgencyCodeOEC,  @UserEmailAddress) AS UA\r\n\t\tINNER JOIN Billing.Account AS A ON A.AgencyAccount = UA.AgencyDivisionSeqID\r\n\t\tINNER JOIN Billing.Facility AS F ON A.FacilityAccount = F.FacilitySeqid\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON UA.AgencyDivisionSeqID = AD.AgencyDivisionSeqid\r\n\tWHERE A.IsActive = 1 AND AD.Inactive = 'N'\r\n\tORDER BY [Name];\r\nEND;"
        }
      ]
    }
  ]
