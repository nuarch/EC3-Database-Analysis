    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[DEMDashboard_EstimatedAccountsPercentageFor12Months]\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tDECLARE @latestBillingPeriod AS VARCHAR(6);\r\n\r\n\tSELECT @latestBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData;\r\n\t\r\n\tSELECT\r\n\t\t'CITY' AS AgencyCodeOEC, L.BillingPeriod, L.Category, L.EnergyType AS Label,\r\n\t\tCONVERT(DECIMAL(18, 4), ISNULL(L.[Count],0)) AS [Count],\r\n\t\tCONVERT(DECIMAL(18, 4), ISNULL(CW.TotalCount,0)) AS TotalCount,\r\n\t\tCONVERT(DECIMAL(18, 2), 100 * ISNULL(L.[Count], 0)/ISNULL(CW.TotalCount, 0)) AS [Percentage]\r\n\tFROM\r\n\t\t(SELECT\r\n\t\tCASE WHEN UtilityCompany = 'Con Edison' AND EnergyType = 'GAS' THEN 'Con Edison GAS' WHEN UtilityCompany = 'Con Edison' AND EnergyType = 'STM' THEN 'Con Edison STM' ELSE UtilityCompany END AS Category, \r\n\t\tCOUNT(DISTINCT CurrentAccountNumber) AS [Count],\r\n\t\tBillingPeriod,\r\n\t\tEnergyType\r\n\t\tFROM Published.TemporalAccountLevelRawDataForCurrentPeriod\r\n\t\tWHERE (EffectiveStartPeriod <= @latestBillingPeriod AND EffectiveEndPeriod > @latestBillingPeriod)\r\n\t\t\tAND BillingPeriod BETWEEN CAST((CAST(@latestBillingPeriod AS INT) - 99) AS VARCHAR(6)) AND @latestBillingPeriod\r\n\t\t\tAND ActualOrEstimated = 'EST'\r\n\t\tGROUP BY BillingPeriod, EnergyType, UtilityCompany, EnergyType) AS L\r\n\tRIGHT JOIN\r\n\t\t(SELECT DISTINCT\r\n\t\t\tCASE WHEN UC.UtilityCompanySeqid = 7 THEN 'NYPA' WHEN UC.UtilityCompanySeqid = 2 THEN 'NGrid West'\r\n\t\t\tWHEN UC.UtilityCompanySeqid = 9 AND EDT.EnergyType='GAS' THEN 'Con Edison GAS'\r\n\t\t\tWHEN UC.UtilityCompanySeqid = 9 AND EDT.EnergyType='STM' THEN 'Con Edison STM'  ELSE 'other' END AS Category,\r\n\t\tCOUNT(DISTINCT A.CurrentAccountNumber) AS TotalCount\r\n\t\tFROM Billing.Account AS A\r\n\t\t\tINNER JOIN Billing.AgencyDivision AS AD ON A.AgencyAccount = AD.AgencyDivisionSeqid\r\n\t\t\tINNER JOIN Billing.Facility AS F ON A.FacilityAccount = F.FacilitySeqid\r\n\t\t\tINNER JOIN Billing.UtilityCompany AS UC ON A.UtilityAccountProvider = UC.UtilityCompanySeqid\r\n\t\t\tINNER JOIN Billing.EnergyDeliveryType AS EDT ON A.EnergyAccountDescription = EDT.EnergyDeliveryType\r\n\t\t\tLEFT JOIN (SELECT OriginalAccountNumber, MAX(BillingPeriod) AS LastPeriodBilled\r\n\t\t\t\tFROM Billing.AccountBilling GROUP BY OriginalAccountNumber) AccntNumber ON AccntNumber.OriginalAccountNumber = A.OriginalAccountNumber\r\n\t\tWHERE AD.Inactive = 'N' AND A.AccountStatus IN ('46', '47', 'AC', 'LA', 'UA')\r\n\t\tGROUP BY CASE WHEN UC.UtilityCompanySeqid = 7 THEN 'NYPA' WHEN UC.UtilityCompanySeqid = 2 THEN 'NGrid West'\r\n\t\t\tWHEN UC.UtilityCompanySeqid = 9 AND EDT.EnergyType='GAS' THEN 'Con Edison GAS'\r\n\t\t\tWHEN UC.UtilityCompanySeqid = 9 AND EDT.EnergyType='STM' THEN 'Con Edison STM'  ELSE 'other' END) AS CW ON L.category = CW.category\r\n\tWHERE L.BillingPeriod IS NOT NULL\r\n\tORDER BY L.BillingPeriod;\r\nEND;"
        }
      ]
    }
  ]
}