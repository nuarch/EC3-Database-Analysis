      "content": [
        {
          "type": "text",
          "text": "-- =============================================\n-- Author:\t\t<Author,,Name>\r\n-- Create date: <Create Date,,>\r\n-- Description:\tReturn list of views of scheduler. These views contains parameter value for report generation\r\n-- =============================================\r\nCREATE PROCEDURE [Scheduler].[usp_Report_GetReportParameterValueDataSourceList]\r\nAS\r\nBEGIN\r\n\tselect TABLE_NAME as ViewName from information_schema.VIEWS \r\n    where Table_schema = 'Scheduler'\r\nEND"
        }
      ]
