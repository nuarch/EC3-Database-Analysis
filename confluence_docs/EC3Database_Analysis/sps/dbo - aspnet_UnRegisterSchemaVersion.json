        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [dbo].[aspnet_UnRegisterSchemaVersion]\n    @Feature                   nvarchar(128),\n    @CompatibleSchemaVersion   nvarchar(128)\nAS\nBEGIN\n    DELETE FROM dbo.aspnet_SchemaVersions\n        WHERE   Feature = LOWER(@Feature) AND @CompatibleSchemaVersion = CompatibleSchemaVersion\nEND"
        }
      ]
