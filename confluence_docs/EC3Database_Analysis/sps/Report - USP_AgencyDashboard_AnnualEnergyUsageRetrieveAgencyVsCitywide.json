    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "7. Stored Procedure Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE PROCEDURE [Report].[USP_AgencyDashboard_AnnualEnergyUsageRetrieveAgencyVsCitywide]\n(\r\n\t@agencyCode AS VARCHAR(10)\r\n\t,@maxFiscalYears AS INT = NULL\r\n\t,@debugMode AS BIT = NULL\r\n)\r\nAS\r\nBEGIN\r\n\tSET NOCOUNT ON;\r\n\tSET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;\r\n\r\n\tSET @maxFiscalYears = ISNULL(NULLIF(@maxFiscalYears, 0), 5);\r\n\t\r\n\tIF (@maxFiscalYears <= 0)\r\n\tBEGIN    \r\n\t\tSET @maxFiscalYears = 5;\r\n\tEND;\r\n\r\n\t-- Get the agency hierarchy\r\n\tSELECT AgencyCodeOEC INTO #tmpAgencies FROM [Billing].[UDF_GetAgencyHierarchy](@agencyCode);\t\r\n\r\n\tDECLARE @publishedBillingPeriod AS VARCHAR(6), @currentFY AS INT;\r\n\tSELECT @publishedBillingPeriod = MAX(PublishedBillingPeriod) FROM Published.MonthlyPublishedSummaryData WITH (NOLOCK);\r\n\tSELECT @currentFY = dbo.CalculateFiscalYear(@publishedBillingPeriod);\r\n\t\r\n\r\n\tDECLARE @fyTable TABLE (FiscalYear VARCHAR(4), AgencyCodeOEC VARCHAR(6));\r\n\tDECLARE @fyCtr AS INT;\r\n\tSET @fyCtr = (@currentFY - @maxFiscalYears) + 1;\r\n\r\n\tWHILE @fyCtr <= @currentFY\r\n\tBEGIN\r\n\t\tINSERT INTO @fyTable (FiscalYear, AgencyCodeOEC) VALUES (@fyCtr, @agencyCode), (@fyCtr, 'CITY');\r\n\t\tSET @fyCtr = @fyCtr + 1;\r\n\tEND;\t\r\n\r\n\t-- Agency vs city usage last 5 FYs\r\n\tSELECT TFY.AgencyCodeOEC\r\n\t\t,TFY.FiscalYear\r\n\t\t,ISNULL(A.EnergyUsage, 0.00) AS EnergyUsage\r\n\t\t,ISNULL(A.DemandUsage, 0.00) AS DemandUsage\r\n\t\t,ISNULL(A.BTU, 0.00) AS BTU\r\n\t\t,ISNULL(A.BilledAmount, 0.00) AS BilledAmount\r\n\tFROM @fyTable TFY\r\n\tLEFT JOIN (\r\n\t\tSELECT @agencyCode AS AgencyCodeOEC\r\n\t\t\t,ALSA.FiscalYear\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), EnergyUsage)) / 1000000.00) AS EnergyUsage\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), DemandUsage)) / 1000000.00) AS DemandUsage\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), BTU)) / 1000000.00) AS BTU\r\n\t\t\t,CONVERT(DECIMAL(36, 2), SUM(BilledAmount)) AS BilledAmount\r\n\t\tFROM Published.TemporalAccountLevelSummaryByAgency AS ALSA WITH (NOLOCK)\r\n\t\tJOIN #tmpAgencies TA ON TA.AgencyCodeOEC = ALSA.AgencyCodeOEC\r\n\t\tWHERE ALSA.EffectiveStartPeriod <= @PublishedBillingPeriod AND ALSA.EffectiveEndPeriod > @PublishedBillingPeriod\r\n\t\t\tAND ALSA.FiscalYear BETWEEN ((@currentFY - @maxFiscalYears) + 1) AND @currentFY\r\n\t\tGROUP BY ALSA.FiscalYear\r\n\t\tUNION\r\n\t\tSELECT 'CITY' AS AgencyCodeOEC\r\n\t\t\t,FiscalYear\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), EnergyUsage)) / 1000000.00) AS EnergyUsage\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), DemandUsage)) / 1000000.00) AS DemandUsage\r\n\t\t\t,(SUM(CONVERT(DECIMAL(36, 2), BTU)) / 1000000.00) AS BTU\r\n\t\t\t,CONVERT(DECIMAL(36, 2), SUM(BilledAmount)) AS BilledAmount\r\n\t\tFROM Published.TemporalAccountLevelSummaryByCityWide WITH (NOLOCK)\r\n\t\tWHERE EffectiveStartPeriod <= @PublishedBillingPeriod AND EffectiveEndPeriod > @PublishedBillingPeriod\r\n\t\t\tAND FiscalYear BETWEEN ((@currentFY - @maxFiscalYears) + 1) AND @currentFY\r\n\t\tGROUP BY FiscalYear\r\n\t) A ON A.FiscalYear = TFY.FiscalYear AND A.AgencyCodeOEC = TFY.AgencyCodeOEC\r\n\tORDER BY A.FiscalYear ASC, A.AgencyCodeOEC ASC;\r\nEND;"
        }
      ]
    }
  ]
}