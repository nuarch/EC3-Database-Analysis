    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[uftn_TableGetAgencyByEmailAddressAgencyAccessAction]\n(\r\n\t@LoweredEmailAdress AS VARCHAR(75)\r\n)\r\nRETURNS @ViewableAgencyDivision TABLE \r\n(\r\n\tAgencyDivisionSeqid [seqid] NOT NULL,\r\n\tParentAgencyDivisionSeqid [seqid] NULL,\r\n\tAgencyCodeOEC dbo.OECAgencyCode PRIMARY KEY, \r\n\tInactive [dbo].[yesno] NULL,\r\n\tAgencyName [dbo].[ldesc] NULL,\r\n\tAgencyShortDesc [dbo].[sdesc] NULL,\r\n\tAgencyDivisionHierarchy HIERARCHYID NULL\r\n)\r\nAS\r\nBEGIN\r\n\tINSERT @ViewableAgencyDivision\r\n\t\t(AgencyCodeOEC,\r\n\t\tParentAgencyDivisionSeqid,\r\n\t\tAgencyDivisionSeqid,\r\n\t\tInactive,\r\n\t\tAgencyDivisionHierarchy,\r\n\t\tAgencyShortDesc,\r\n\t\tAgencyName)\r\n\tSELECT DISTINCT ADC.AgencyCodeOEC, ADC.ParentAgencyDivisionSeqid, ADC.AgencyDivisionSeqid,\r\n\t\tADC.Inactive, ADC.AgencyDivisionHierarchy, ADC.AgencyShortDesc, ADC.AgencyName\r\n\tFROM Membership.EC3Membership AS M\r\n\t\tINNER JOIN Membership.MembershipAgencyAccess AS MA ON M.AuthenticatedUserID = MA.AuthenticatedUserID\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON AD.AgencyDivisionSeqid = MA.AgencyDivisionSeqID OR MA.IsAllAgencyAccess = 'Y'\r\n\t\tINNER JOIN Billing.AgencyDivision AS ADC ON ADC.AgencyDivisionHierarchy.IsDescendantOf(AD.AgencyDivisionHierarchy) = 1\r\n\tWHERE LOWER(M.Email) = LOWER(@LoweredEmailAdress)\r\n\t\tAND AD.Inactive = 'N' AND ADC.Inactive = 'N';\r\n\tRETURN;\r\nEND;"
        }
