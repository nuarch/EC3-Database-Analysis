    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "8. Function Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[GetHouseNumberFromAddress] ( @address varchar(40))\nRETURNS varchar(8) AS  \r\n\r\nBEGIN \r\n--\r\ndeclare @HouseNumber varchar(40)\r\n\r\n\r\ndeclare @StartPosOfBlank int\r\n\r\ndeclare @blank varchar(1)\r\n\r\nset @blank = ' '\r\n\r\nset @StartPosOfBlank = CHARINDEX(@blank,@address,0)\r\n\r\nset @HouseNumber = rtrim(ltrim(substring(@address, 0, @StartPosOfBlank)))\r\n\r\n\r\nreturn @HouseNumber \r\n\r\n\r\nEND"
        }
      ]
