    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "8. Function Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[MMDDYKeyspanToYYYYMMDD] (@MMDDY varchar(5))\nRETURNS varchar(8) AS  \r\nBEGIN \r\n-- Return Type YYYYMMDD\r\ndeclare @YYYYMMDD varchar(8)\r\ndeclare @yearIndicator int\r\ndeclare @year int\r\n--\r\n-- Default Value for a \"00000\" is '18760101'\r\nset @YYYYMMDD = '18760101'\r\nif (@MMDDY ='00000')  return  (@YYYYMMDD)\r\n\r\nset @year = YEAR(GETDATE())\r\nset @yearIndicator = cast(substring(@MMDDY,5,1) as int)\r\n\r\nwhile((@year % 10) <> @yearIndicator)\r\nbegin\r\n\tset @year = @year - 1\r\nend\r\n\r\nset @YYYYMMDD =  cast(@year as varchar) + substring(@MMDDY,1,4)\r\n\r\n-- Reformat from MMDDY to YYYYMMDD\r\n--set @YYYYMMDD =  '200' + substring(@MMDDY,5,1)+substring(@MMDDY,1,4)\r\n\r\nreturn  @YYYYMMDD\r\nEND"
        }
      ]
