      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [Budget].[DeriveBudgetedFacilityPointDollars] (@BudgetDeliveryFacilityPoints int, @StreetLightingFacilityPoints decimal(12,6))\n\r\nRETURNS money AS  \r\nBEGIN \r\n--\r\ndeclare @BudgetedFacilityPointDollars money\r\ndeclare @BudgetDeliveryFacilityPointsDecimal decimal(12,1)\r\n--\r\nselect @BudgetDeliveryFacilityPointsDecimal=cast(@BudgetDeliveryFacilityPoints as decimal(12,1))\r\n--\r\nset @BudgetedFacilityPointDollars = Cast ((IsNull(@BudgetDeliveryFacilityPointsDecimal,0)*IsNull(@StreetLightingFacilityPoints,0)) as money)\r\nreturn \t@BudgetedFacilityPointDollars\t\t\t\t\t\t \r\nEND"
        }
