    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[GetUniqueAccountSeqIDbyAccountNumber] (@AccountNumber VARCHAR(15))\nRETURNS INT AS  \r\nBEGIN \r\n--\r\nDECLARE @uniqueSeqID INT \r\nSELECT @uniqueSeqID =  A.UniqueAccountSeqid FROM Billing.Account AS A WHERE A.OriginalAccountNumber =@AccountNumber\r\n \r\n \r\nRETURN @uniqueSeqID\r\nEND"
        }
