        },
        {
          "type": "listItem",
          "content": [
            {
              "type": "paragraph",
              "content": [
                {
                  "type": "text",
                  "text": "Returning this number as the unassigned meter number."
                }
              ]
            }
          ]
        }
      ]
    },
