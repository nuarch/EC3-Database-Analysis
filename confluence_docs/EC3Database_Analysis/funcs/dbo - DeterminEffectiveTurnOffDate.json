    },
    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "8. Function Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[DeterminEffectiveTurnOffDate]  (@JulDate varchar(5))\n--\r\nRETURNS varchar(8) AS  \r\nBEGIN \r\ndeclare @t  varchar(8) \r\n--\r\nif (@JulDate  =  '00000')\r\n\tset @t = '99991231'\r\nelse\r\n\tset @t = [dbo].[JulianDatetoYYYYMMDD] (@JulDate)\r\nreturn @t\r\nEND"
        }
      ]
    }
