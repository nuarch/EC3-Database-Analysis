        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[DetermineNypaIsStreetLighting] ( @OriginalAccountNumber varchar(15))\nRETURNS varchar(1) AS  \r\n\r\nBEGIN \r\n--\r\nif (substring(@OriginalAccountNumber,1,1) > '8') return 'Y' else return 'N'\r\n--\r\nreturn 'N'\r\nEND"
