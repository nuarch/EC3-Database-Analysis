    {
      "type": "heading",
      "attrs": {
        "level": 1
      },
      "content": [
        {
          "type": "text",
          "text": "8. Function Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE     FUNCTION [dbo].[ConvertOverpunch8ToInt32] (@overPunchField varchar(8) )\nRETURNS    int\r\nBEGIN \r\n-- Declare variables\r\n   DECLARE @convertedField   varchar(9)\r\n   DECLARE @IntegerlField   int\r\n--\r\n  select @convertedField  = [DBO].[ConvertOverpunch8](@overPunchField)\r\n  select @IntegerlField  = cast(@convertedField as int)\r\n\r\nreturn @IntegerlField\r\nEND"
        }
      ]
    }
