      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[CalculateConsecutiveNumberOfEstimatedReadings] (@TypeOfReading varchar(3),@PreviousPeriodNumberOfEstimatedReadings int)\nRETURNS int AS  \r\nBEGIN \r\n--\r\nif @TypeOfReading = 'ACT' return 0\r\n--\r\nreturn IsNull(@PreviousPeriodNumberOfEstimatedReadings,0) + 1\r\nEND"
