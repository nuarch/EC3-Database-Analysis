      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[KeyspanAccount#WithoutTrip] (@Account varchar(15))\nRETURNS varchar(10) AS  \r\nBEGIN \r\n-- Strip the last 10 bytes for the \"folio\" account number without the trip ticket number \r\n-- that is in the first 5 bytes of the 15 character account number\r\nreturn SUBSTRING(@Account, 6, 10)\r\nEND"
        }
