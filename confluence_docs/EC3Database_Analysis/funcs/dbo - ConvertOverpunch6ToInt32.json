      "content": [
        {
          "type": "text",
          "text": "CREATE    FUNCTION [dbo].[ConvertOverpunch6ToInt32] (@overPunchField varchar(6) )\nRETURNS    int\r\nBEGIN \r\n-- Declare variables\r\n   DECLARE @convertedField   varchar(7)\r\n   DECLARE @IntegerlField   int\r\n--\r\n  select @convertedField  = [DBO].[ConvertOverpunch6](@overPunchField)\r\n  select @IntegerlField  = cast(@convertedField as int)\r\n\r\nreturn @IntegerlField\r\nEND"
