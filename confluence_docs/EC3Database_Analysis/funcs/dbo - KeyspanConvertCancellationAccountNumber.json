      "content": [
        {
          "type": "text",
          "text": "CREATE     FUNCTION [dbo].[KeyspanConvertCancellationAccountNumber] (@AccountNumber varchar(12),@BilledAmount varchar(10) )\nRETURNS    varchar(12)\r\nBEGIN \r\n--\r\ndeclare @LastDigit integer\r\ndeclare @BillingAmount money\r\n\r\n--set @BillingAmount = cast([DBO].[ConvertOverpunch10ToDecimal] (@BilledAmount) as money)\r\nset @BillingAmount = cast([DBO].[ConvertOverpunch] (@BilledAmount) as money)\r\n--\r\n\r\nif (@BillingAmount >= 0 )\r\n\t-- Not a canceled Account\r\n\treturn @AccountNumber\r\n--\r\nset @LastDigit = cast (substring(@AccountNumber,12,1) as integer)\r\n\r\nif (@LastDigit = 0 ) \r\n    set @LastDigit = 9\r\nelse\r\n    set @LastDigit = @LastDigit - 1\r\nBegin\r\nreturn substring(@AccountNumber,1,11) + cast ( @LastDigit as varchar(1))\r\nEnd\r\nEND"
