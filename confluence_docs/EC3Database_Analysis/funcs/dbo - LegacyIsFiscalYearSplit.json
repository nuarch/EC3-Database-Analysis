        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[LegacyIsFiscalYearSplit] (@JulFromDate varchar(5),@JulToDate varchar(5))\nRETURNS varchar(1) AS  \r\nBEGIN \r\ndeclare @FromFiscalYear varchar(6)\r\ndeclare @ToFiscalYear varchar(6)\r\ndeclare @FiscalYear1 varchar(6)\r\ndeclare @FiscalYear2 varchar(6)\r\ndeclare @IsSplit varchar(1)\r\ndeclare @FromDate varchar(8)\r\ndeclare @ToDate varchar(8)\r\n--\r\n--  [dbo].[CreateFiscalYear] (@YYYYMM varchar(6),@Current varchar(1))  \r\nset @FromDate = [dbo].[JulianDateToYYYYMMDD] (@JulFromDate)\r\nset @ToDate = [dbo].[JulianDateToYYYYMMDD] (@JulToDate)\r\n--\r\nset @FromFiscalYear = substring(@FromDate,1,6) \r\nset @ToFiscalYear     = substring(@ToDate,1,6) \r\nselect   @FiscalYear1 = [dbo].[CreateFiscalYear] (@FromFiscalYear,'C')  \r\n\r\nselect   @FiscalYear2 = [dbo].[CreateFiscalYear] (@ToFiscalYear,'C')  \r\n\r\n--\r\nset @IsSplit = 'Y'\r\nif (@FiscalYear1 = @FiscalYear2)  set @IsSplit =  'N'\r\n--\r\nreturn @IsSplit\r\nEND"
