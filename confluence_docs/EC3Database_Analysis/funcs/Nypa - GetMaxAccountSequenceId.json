    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [Nypa].[GetMaxAccountSequenceId] ( @AccountNumber varchar(15),@UtilityCompanySeqid int)\nRETURNS int AS  \r\n\r\nBEGIN \r\n--\r\ndeclare @MaxAccountSeqid int\r\n--\r\nSELECT @MaxAccountSeqid = MAX(AccountSeqid)\r\nFROM Billing.Account\r\nGROUP BY OriginalAccountNumber, UtilityAccountProvider\r\nHAVING (OriginalAccountNumber = @AccountNumber) AND (UtilityAccountProvider = @UtilityCompanySeqid)\r\n--\r\nreturn @MaxAccountSeqid\r\nEND"
        }
      ]
