      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [Common].[GetDefaultEnergySubType]\n(@EnergyTypeID AS INT)\r\nRETURNS INT\r\nAS\r\nBEGIN \r\n\tDECLARE @DefaultEnergySubTypeID AS INT;\r\n\r\n\tSELECT @DefaultEnergySubTypeID = EnergyDeliverySubTypeId\r\n\tFROM Billing.EnergyDeliverySubType AS ST \r\n\t\tINNER JOIN Billing.EnergyDeliveryType AS T\r\n\t\tON ST.EnergyDeliveryTypeId = T.EnergyDeliveryType AND ST.IsDefault = 1 \r\n\tWHERE T.EnergyDeliveryType = @EnergyTypeID;\r\n\r\n\tRETURN @DefaultEnergySubTypeID;\r\nEND;"
