      "content": [
        {
          "type": "text",
          "text": "8. Function Definition"
        }
      ]
    },
    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE      FUNCTION [dbo].[CalculateLoadFactorElectric] (@BilledKilowattHours numeric(8,2),@PeakDemand numeric(8,2),@FromDate varchar(8),@ToDate varchar(8))\nRETURNS numeric(6,4) AS  \r\nBEGIN \r\ndeclare @TwentyFourHours numeric(6,3)\r\ndeclare  @TestPeakDemand  numeric(8,2)\r\ndeclare @ValidLoadFactor numeric(6,4)\r\nset  @TwentyFourHours = 24.0\r\n--\r\n-- BilledKilowattHours/(Number of Hours in the billing period x Peak Demand)\r\n--\r\n-- @BilledKilowattHours/( @PeakDemand * 24*DATEDIFF(DAY,@FromDate,@ToDate))\r\n\r\nset @TestPeakDemand = isnull(@PeakDemand,0) \r\nif (@FromDate = null or  @ToDate = null or @PeakDemand = 0.0 or @TestPeakDemand = 0.0 )\r\n\treturn -1.00\r\nelse\r\n\tBegin\r\n\t-- select @ValidLoadFactor = IsNull(@BilledKilowattHours,0)  / (@TestPeakDemand * @TwentyFourHours * DATEDIFF ( day , @FromDate ,@ToDate  ) )\r\n\treturn @BilledKilowattHours/( @PeakDemand * 24.0 *DATEDIFF(DAY,@FromDate,@ToDate))\r\n\r\n\tif(@ValidLoadFactor > 0.00 and @ValidLoadFactor <= 1.00)\r\n\t\treturn  @ValidLoadFactor\r\n\telse\r\n\t\treturn -1.00\r\n\tEnd\r\nreturn -1.00\r\nEND"
        }
      ]
    }
