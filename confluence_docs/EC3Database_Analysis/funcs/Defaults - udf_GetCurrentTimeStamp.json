                {
                  "type": "text",
                  "text": "Precision",
                  "marks": [
                    {
                      "type": "strong"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ": Depending on whether "
                },
                {
                  "type": "text",
                  "text": "GETDATE()",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " or "
                },
                {
                  "type": "text",
                  "text": "SYSDATETIME()",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " is used, the precision of the timestamp may vary. "
                },
                {
                  "type": "text",
                  "text": "SYSDATETIME()",
                  "marks": [
                    {
                      "type": "code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " provides more precision than "
                },
                {
                  "type": "text",
