    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[ConvertOverpunch3] (@overPunchField varchar(3) )\nRETURNS    varchar(3)\r\nBEGIN \r\n-- Declare variables\r\n   DECLARE @convertedField   varchar(3)\r\n   DECLARE @overPunchColumnLength int\r\n   DECLARE @overPunchValuePosition int\r\n   DECLARE @OverPunchCodes varchar(20)\r\n   DECLARE @OverPunchValues varchar(20)\r\n   DECLARE @OverPunch varchar(1)\r\n--\r\n  set @OverPunchCodes  = '{ABCDEFGHIJKLMNOPQR}'\r\n  set @OverPunchValues = '01234567891234567890'\r\n--\r\n  set @overPunchColumnLength = 3\r\n\r\n   set  @OverPunch =  SUBSTRING(@overPunchField,3,1 )\r\n   select  @overPunchValuePosition = CHARINDEX(@OverPunch,@OverPunchCodes)\r\n\r\n   if (@overPunchValuePosition = 0) return @overPunchField\r\n--\r\n  set @convertedField = SUBSTRING(@overPunchField,1,@overPunchColumnLength-1 )+SUBSTRING( @OverPunchValues, @overPunchValuePosition,1 )\r\n\r\n  if (@overPunchValuePosition > 10) \r\n\t   return ( '-' + @convertedField)\r\n\r\nreturn @convertedField\r\nEND"
        }
