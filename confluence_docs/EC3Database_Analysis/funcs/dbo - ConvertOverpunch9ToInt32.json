    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE   FUNCTION [dbo].[ConvertOverpunch9ToInt32] (@overPunchField varchar(9) )\nRETURNS    int\r\nBEGIN \r\n-- Declare variables\r\n   DECLARE @convertedField   varchar(10)\r\n   DECLARE @IntegerlField   int\r\n--\r\n  select @convertedField  = [DBO].[ConvertOverpunch9](@overPunchField)\r\n  select @IntegerlField  = cast(@convertedField as int)\r\n\r\nreturn @IntegerlField\r\nEND"
        }
