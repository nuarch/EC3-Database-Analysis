      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[DetermineKeyspanWestWdNumber] ( @CurrentBillingPeriod varchar(6),@CrisOrSL integer,@AccountNumber varchar(15))\nRETURNS varchar(2) AS  \r\n\r\nBEGIN \r\n--\r\ndeclare @AccountWdNumber varchar(2)\r\ndeclare @WdNumber integer\r\n--\r\n-- Get original WD number from the account\r\n--\r\nset @AccountWdNumber = substring(@AccountNumber,12,2)\r\n-- No change to the WD number if it is Special Ledger\r\nif (@CrisOrSL = 4) return @AccountWdNumber\r\n--\r\nset @WdNumber = cast(@AccountWdNumber as integer)\r\n--\r\nif ([dbo].[EvenOrOddPeriod]( @CurrentBillingPeriod)=0) \r\n   Begin\r\n--    Even Periods have WD numbers from 01 to 20 therefore the monthly meter reads use the wd number plus 20 schedule\r\n      if (@WdNumber < 21)\r\n            Begin\r\n\t       return @AccountWdNumber\r\n\t    End\r\n         else \r\n            Begin\r\n\t       set @WdNumber = @WdNumber - 20\r\n\t       if (@WdNumber < 10)\r\n\t             return '0'+ cast(@WdNumber as varchar(1))\r\n\t          else\r\n\t             return cast(@WdNumber as varchar(2))\r\n\t   End\r\n   End\r\nelse \r\n   Begin\r\n--    Odd Periods have WD numbers from 21 to 40 therefore the monthly meter reads use the wd number minus 20 schedule\r\n      if (@WdNumber < 21)\r\n\t    Begin\r\n\t       set @WdNumber = @WdNumber + 20\r\n\t       return cast(@WdNumber as varchar(2))\r\n\t    End\r\n         else \r\n            Begin\r\n\t       return @AccountWdNumber\r\n\t    End\r\n   End\r\n--\r\nreturn '20'\r\nEND"
        }
