    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [dbo].[uftn_TableGetActiveAgenciesByEmailAddress]\n(\r\n\t@EmailAdress AS VARCHAR(75)\r\n)\r\nRETURNS @ViewableAgencyDivision TABLE \r\n(\r\n\t[AgencyDivisionSeqid] [seqid] NOT NULL,\r\n\t[ParentAgencyDivisionSeqid] [seqid] NULL,\r\n\t[AgencyCodeOEC] dbo.OECAgencyCode PRIMARY KEY, \r\n\t[Inactive] [dbo].[yesno],\r\n\t[AgencyName] [dbo].[ldesc],\r\n\t[AgencyShortDesc] [dbo].[sdesc],\r\n    AgencyDivisionHierarchy HIERARCHYID NULL\r\n)\r\nAS\r\nBEGIN\r\n\tINSERT @ViewableAgencyDivision\r\n\t    (AgencyCodeOec,\r\n\t    ParentAgencyDivisionSeqid,\r\n\t    AgencyDivisionSeqid,\r\n\t    Inactive,\r\n\t    AgencyDivisionHierarchy,\r\n\t    AgencyShortDesc,\r\n\t    AgencyName)\r\n\tSELECT DISTINCT ADC.AgencyCodeOEC, ADC.ParentAgencyDivisionSeqid, ADC.AgencyDivisionSeqid,\r\n\t\tADC.Inactive, ADC.AgencyDivisionHierarchy, ADC.AgencyShortDesc, ADC.AgencyName\r\n\tFROM Membership.EC3Membership AS M\r\n\t\tINNER JOIN Membership.MembershipAgencyAccess AS MA ON M.AuthenticatedUserID = MA.AuthenticatedUserID\r\n\t\tINNER JOIN Billing.AgencyDivision AS AD ON AD.AgencyDivisionSeqid = MA.AgencyDivisionSeqID OR MA.IsAllAgencyAccess = 'Y'\r\n\t\tINNER JOIN Billing.AgencyDivision AS ADC ON ADC.AgencyDivisionHierarchy.IsDescendantOf(AD.AgencyDivisionHierarchy) = 1\r\n\tWHERE M.Email = @EmailAdress\r\n\t\tAND AD.Inactive = 'N' AND ADC.Inactive = 'N';\r\n\tRETURN;\r\nEND;"
        }
      ]
