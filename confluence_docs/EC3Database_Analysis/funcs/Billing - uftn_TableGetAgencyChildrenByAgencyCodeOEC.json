    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE FUNCTION [Billing].[uftn_TableGetAgencyChildrenByAgencyCodeOEC]\n(\r\n    @AgencyCodeOEC AS VARCHAR(MAX),\r\n    @EmailAddress AS emailaddr\r\n)\r\nRETURNS @selectedAgency TABLE\r\n(\r\n\tAgencyCodeOEC VARCHAR(6),\r\n\tAgencyDivisionSeqID seqid NULL,\r\n\tAgencyDivisionHierarchy HIERARCHYID NULL,\r\n\tParentAgencyDivisionSeqid seqid NULL,\r\n\tAgencyName VARCHAR(75)\r\n)\r\nAS\r\nBEGIN\r\n    DECLARE @delimiter AS VARCHAR(1);\r\n    SET @delimiter = ',';\r\n\r\n    IF (@AgencyCodeOEC = '*')\r\n    BEGIN\r\n        INSERT INTO @selectedAgency\r\n\t\t\t(AgencyCodeOEC,\r\n            AgencyDivisionSeqID,\r\n            AgencyDivisionHierarchy,\r\n            ParentAgencyDivisionSeqid,\r\n            AgencyName)\r\n        SELECT DISTINCT AgencyCodeOEC,\r\n            AgencyDivisionSeqID,\r\n            AgencyDivisionHierarchy,\r\n            ParentAgencyDivisionSeqid,\r\n            AgencyName\r\n        FROM dbo.uftn_TableGetAgencyByEmailAddressAgencyAccessAction(@EmailAddress)\r\n        WHERE Inactive = 'N'\r\n        ORDER BY AgencyCodeOEC;\r\n    END;\r\n    ELSE\r\n    BEGIN\r\n\t\t-- parse agency code oec\r\n        WITH cteAgencyCodeOEC AS (SELECT 0 AS [pos], 1 AS [level]\r\n\t\tUNION ALL\r\n        SELECT CONVERT(INT, CHARINDEX(@delimiter, @AgencyCodeOEC, cteAgencyCodeOEC.pos + 1)), [level] + 1\r\n        FROM cteAgencyCodeOEC\r\n        WHERE CHARINDEX(@delimiter, @AgencyCodeOEC, cteAgencyCodeOEC.pos + 1) > 0)\r\n        INSERT INTO @selectedAgency (AgencyCodeOEC)\r\n        SELECT SUBSTRING(@AgencyCodeOEC, a.pos + 1, CASE WHEN b.pos IS NULL THEN (LEN(@AgencyCodeOEC) - a.pos) ELSE (b.pos - a.pos - 1) END)\r\n        FROM cteAgencyCodeOEC AS a\r\n\t\t\tLEFT JOIN cteAgencyCodeOEC b ON a.[level] + 1 = b.[level] option (maxrecursion 0);\r\n\r\n\t\tUPDATE sg\r\n        SET sg.AgencyDivisionSeqID = ag.AgencyDivisionSeqID,\r\n            sg.AgencyDivisionHierarchy = ag.AgencyDivisionHierarchy,\r\n            sg.ParentAgencyDivisionSeqid = ag.ParentAgencyDivisionSeqid,\r\n            sg.AgencyName = ag.AgencyName\r\n        FROM @selectedAgency AS sg\r\n            INNER JOIN Billing.AgencyDivision AS ag ON sg.AgencyCodeOEC = ag.AgencyCodeOEC;\r\n\r\n\t\t--  subtree of selected node(s)\r\n\t\tINSERT INTO @selectedAgency\r\n\t\t\t(AgencyCodeOEC,\r\n\t\t\tAgencyDivisionSeqID,\r\n\t\t\tAgencyDivisionHierarchy,\r\n\t\t\tParentAgencyDivisionSeqid,\r\n\t\t\tAgencyName)\r\n        SELECT DISTINCT C.AgencyCodeOEC,\r\n\t\t\tC.AgencyDivisionSeqID,\r\n\t\t\tC.AgencyDivisionHierarchy,\r\n\t\t\tC.ParentAgencyDivisionSeqid,\r\n\t\t\tC.AgencyName\r\n        FROM @selectedAgency AS sg\r\n            INNER JOIN Billing.AgencyDivision AS C ON C.AgencyDivisionHierarchy.IsDescendantOf(sg.AgencyDivisionHierarchy) = 1\r\n                AND C.AgencyDivisionSeqID <> sg.AgencyDivisionSeqID AND C.InActive = 'N'\r\n            INNER JOIN dbo.uftn_TableGetAgencyByEmailAddressAgencyAccessAction(@EmailAddress) AS UA ON UA.AgencyDivisionSeqID = C.AgencyDivisionSeqID\r\n        WHERE C.AgencyDivisionSeqID NOT IN(SELECT AgencyDivisionSeqID FROM @selectedAgency);\r\n    END;\r\n    RETURN;\r\nEND;"
        }
