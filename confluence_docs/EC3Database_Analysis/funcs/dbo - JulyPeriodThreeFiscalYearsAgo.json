      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[JulyPeriodThreeFiscalYearsAgo] (@CurrentFiscalYear varchar(4))\nRETURNS varchar(6) AS  \r\nBEGIN \r\ndeclare @JulyPeriodThreeFiscalYearsAgo datetime\r\nselect  @JulyPeriodThreeFiscalYearsAgo = DATEADD(year, -3, @CurrentFiscalYear+'0101') \r\nreturn cast(DATEPART ( yyyy , @JulyPeriodThreeFiscalYearsAgo ) as varchar(4))+'07'\r\nEND"
