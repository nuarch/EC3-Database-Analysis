      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [dbo].[DetermineKeyspanWestBorough] (@AccountNumber varchar(15))\nRETURNS varchar(1) AS  \r\nBEGIN \r\n--\r\ndeclare @b varchar(3)\r\ndeclare @boroughCode varchar(1)\r\n--\r\nset @b = substring( @AccountNumber,2,3)\r\nset  @boroughCode = '0\r\n--'\r\nif ('000'<= @b and @b <= '065') set @boroughCode = '3'\r\nelse if ('066'<= @b and @b <= '169') set @boroughCode = '4'\r\nelse if ('200'<= @b and @b <= '209') set @boroughCode = '3'\r\nelse if ('220'<= @b and @b <= '239') set @boroughCode = '3'\r\nelse if ( @b = '915') set @boroughCode = '5'\r\n \r\nreturn @boroughCode\r\n\r\nEND"
        }
