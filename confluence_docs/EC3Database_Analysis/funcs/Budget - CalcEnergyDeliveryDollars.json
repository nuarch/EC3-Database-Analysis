    {
      "type": "codeBlock",
      "attrs": {
        "language": "sql"
      },
      "content": [
        {
          "type": "text",
          "text": "CREATE  FUNCTION [Budget].[CalcEnergyDeliveryDollars] (@EnergyUsage decimal(12,2), @BudgetDeliveryConsumption decimal(12,6))\n\r\nRETURNS money AS  \r\nBEGIN \r\n--\r\ndeclare @EnergyDeliveryDollars money\r\n--\r\nset @EnergyDeliveryDollars = Cast (IsNull(@EnergyUsage,0)*IsNull(@BudgetDeliveryConsumption,0) as money)\r\n\r\nreturn \t@EnergyDeliveryDollars\t\t\t\t\t \r\nEND"
        }
      ]
